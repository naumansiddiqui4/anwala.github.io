        }
        t._e = [];
        t.ready = function(f) {
            t._e.push(f);
        };
        return t;
    }(document, 'script', 'twitter-wjs');
    twttr.ready(function (twttr) {
        // Now bind our custom intent events
        twttr.events.bind('tweet', function (e) {
            ga('send', 'event', 'socialBtn', 'click', "twitterBtn");
        });
    });
</script>        </body>
</html>
