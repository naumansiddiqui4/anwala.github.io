			'uri': 'http://news.naver.com/main/read.nhn?oid=108&aid=0002761384',
			'dimension_1': '%EC%97%B0%EC%98%88'
			,'dimension_3': '44042'
		});

		nil.send('cv', 'post_view'); 
		window.onunload = function() {
			nil.send('leave'); 
		}
	} catch (e) {
	}
}

	analysysExe(false);
</script><script type="text/javascript">
function handleWebAccess(welTabWrap, index) {
	try{
		welTabWrap.query("ul.Nlnb_menu_list > li.is_active").attr("aria-selected", "false");
		welTabWrap.query("ul.Nlnb_menu_list > li.is_active").removeClass("is_active");
		welTabWrap.queryAll("ul.Nlnb_menu_list > li")[index].addClass("is_active");
		welTabWrap.queryAll("ul.Nlnb_menu_list > li")[index].attr("aria-selected", "true");
	}catch(e) {
	}
}
</script><script type="text/javascript">
jindo.$Fn(function() {
	var elLNB = jindo.$("LNB_WRAPPER");
	if (elLNB) {
		jindo.$Element("LNB_SCROLLER").css("right", "");
		var bNext = true;
		var oScroll = new jindo.m.Scroll(elLNB, {
			bUseHScroll : true,
			bUseVScroll : false,
		 	bUseScrollbar : false
		});
		
		if (bNext) oScroll.scrollTo(170,0);
		jindo.m.bindRotate(function(){
			oScroll.refresh();
			if (bNext) oScroll.scrollTo(170,0);
		});
	}
}).attach(window, 'load');
