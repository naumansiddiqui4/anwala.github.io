function doAfterLoadEnd(callback){
	var eventType = "onpageshow" in window ? "pageshow" : "load";
	jindo.$Element(window).attach(eventType, function(we){
		if(Boolean(we.$value().persisted)){ // bfcache
			return;
		}
		callback();
	});
}

try{
	if('N' == 'N'){
		var ttsPlayer = null;
		function sendToJavaScript(value) {
			ttsPlayer && ttsPlayer.end();
		}
		doAfterLoadEnd(function() {
			jindo.$Fn(function() {
				ttsPlayer = new enter.m.read.NewsTTSPlayer({
					document_id : 'tts_player_div',
					office_id : '108',
					article_id : '0002761384',
					nation_text : 'ko'
				});
			}).delay(0.8);
		});
	}
}catch(e){
}

doAfterLoadEnd(function() {
	initRead();
});

function initRead() {
	
	var oDate = new jindo.$Date(Date.now());
	var currentTimeStamp = oDate.format("YmdHis");
	
	enter.m.read.init({
		oLikeItData : {
			serviceId : "ENTERTAIN",  
			contentId : "ne_108_0002761384",  
			drawButton : true , 
			domain : "https://news.like.naver.com",  
			lang : "ko", 
			moreLessButton : true, 
			preHandler : function() {},
			viewLabel : true
		},
		oSpiData : {
			domain : 'https://ssl.pstatic.net/spi'
			
			, officeName : ' | 스타뉴스'
			
		},
		oReadingData : {
			sContentsId : 'contentArea',
			oid : '108',
			aid : '0002761384',
			sid : '106',
			articleType : '1',
			gdid : '880000C3_000000000000000002761384',
			timestamp : currentTimeStamp
		}
	});
	
	
	try{
		enter.m.read.translate.init('app.translate.naver.net/js/NaverTranslator.js');
	} catch(e){
	}
	
	
	try {
		jindo.$Fn(function() {
			enter.m.read.fontsize.init();
		}).delay(0.2);
	} catch (e) {
	}

	
	try {
		jindo.$Fn(function() {
			enter.m.read.enterRanking.init();
		}).delay(0.2);
	}catch (e) {
	}
	
	
	try {
		enter.m.commonComment.init({
			sDomain : 'https://ssl.pstatic.net/static.cbox',
			sApiDomain : 'https://apis.naver.com/commentBox/cbox5',
			sTicket : 'news',
			sObjectId : 'news108,0002761384',
			nPageSize : 5, 
			nReplyPageSize : 20, 
			sTemplateId : 'view_ent',
			sCountType : 'comment',
			sAuthType : 'both',
			aFormation : ['count', 'graph', 'write', 'notice', 'list', 'view'],
			sLikeItId : "ne_108_0002761384",  // 컨텐츠 ID,
			bMobile : true ,
			sHelp : 'down',
			vViewAllHandler : function(){
				window.location.href = '/now/comment/list?oid=108&aid=0002761384';
			},
			htEventHandler : {
				loadList : function(oEvent) {
					var oJson = oEvent.oJson;
					var welCommentCount = jindo.$Element('cmt_count');
					var sTempHtml = "<span class=\"bar\"></span><span class=\"sp_ico\"><span class=\"blind\">댓글</span></span>";
					if (oJson.success) {
						var commentCnt = oJson.result.count.comment;
						sTempHtml += jsutility.changeNumberFormat(commentCnt);
					} else {
						sTempHtml += "0";
					}

					welCommentCount.html(sTempHtml);
				},
				loadCreate : function(oEvent) {
					var oJson = oEvent.oJson;
						if (oJson.success) {
							commentCountUpdate(oJson.result.count.comment);
						}
				},
				afterRemove : function(oEvent) {
					var oJson = oEvent.oJson;
						if (oJson.success) {
							commentCountUpdate(oJson.result.count.comment);
						}
				 },
				afterList :  function(oEvent) {
					var oJson = oEvent.oJson;
					if (oJson.success) {
						var commentCnt = oJson.result.count.comment;
						if(commentCnt == 0){
							jindo.$Element(jindo.$Element("cbox_module").query(".u_cbox_view_comment")).hide();
						}
					}
				} 
			},
			htErrorHandler : {
				'5010': '댓글/답글은 60초 내에 한 개만 등록하실 수 있습니다.',
				'5006': '자신의 글에 공감 할 수 없습니다.',
				'5008': '자신의 글에 비공감 할 수 없습니다.',
				'4004': '해당되는 댓글이 없습니다.',
				'5003': '정상적으로 반영되지 않았습니다. 화면 새로고침 후 다시 시도해 주세요.',
				'5026': '공감/비공감은 최근 24시간내 50회까지 가능합니다.',
				'5027': '공감/비공감은 3초 내에 한 번만 클릭하실 수 있습니다.',
				'5029': '기사당 허용된 댓글 수 3개를 초과하였습니다. 기사에 작성한 댓글을 삭제 후 다시 작성하실 수 있습니다.'
			},
			htMessage : {
				alert : {
					min_length : '내용을 최소 {0}자 이상 입력해주세요.',
					max_length : '{0}자까지 입력할 수 있습니다.',
					login_require : '입력창 상단의 서비스별 아이콘을 선택하여 로그인해 주세요.'
				},
				template : {
					write_placeholder : '댓글을 입력해주세요',
