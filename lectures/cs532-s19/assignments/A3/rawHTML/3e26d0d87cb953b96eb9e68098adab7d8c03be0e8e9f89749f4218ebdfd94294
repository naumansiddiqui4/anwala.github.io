		jindo.$Fn(function () {
			if (jindo.$$.getSingle("#tvcast_autoPhoto_scroll")) {
				new jindo.m.Scroll("tvcast_autoPhoto_scroll", {
					bUseHScroll: true,
					bUseVScroll: false,
					bUseScrollbar: false,
					bUseMomentum: true,
					bAutoResize: true
				});
			}
		}).delay(0.2);
	} catch (e) {
	}

	try {
		enter.m.read.section.guide.init();
	} catch (e) {
	}

	
	try{
		var relation_btn_inner_wel = jindo.$Element("contents_end_cmp_wrp").query(".relation_btn_inner");
		if(relation_btn_inner_wel.html().trim() == "") {
			relation_btn_inner_wel.css('display', 'none');
		}
	} catch (e) {
	}

	initReaction();

	
	try {
		new Image().src = "https://n.news.naver.com/one/article/108/0002761384?service=entertain";
	} catch (e) {}

	try {
		enter.m.read.init_msg_sender();
	} catch(e) {
	}
}

function emotionClickControl(){
	
	try {
		enter.m.read.hide_font_translate_popup();
	}catch(e){
	}
}

function initReaction() {
	jindo.LazyLoading.load("https://news.like.naver.com/js/reaction/dist/reaction.min.js?123https://static-entertain.pstatic.net/mobile/resources/20190214_134709", function () {
		try{
			jindo.$Element("contents_end_cmp_wrp").query(".u_likeit_button").attach("click", emotionClickControl);
		}catch(e){
		}
		reaction.init({
			type: "face",
			cssId: "entertain",
			isMobile: true,
			domain : "https://news.like.naver.com",
			dependentLibrary : "jquery",
			isHiddenLayerAfterSelection: true,
			contentCountPerOnceRequest : 10,
			isDebugMode : false,
			isDuplication : false,
			callback : {
				updated : function(param){},
				updateParent : function(param) {},
				clicked : function(param){}
			}
		});

		enter.m.read.toMain.init(); 
	});
}
