(function(){
	var weGoTop = jindo.$Element("goTop");
	if(!weGoTop) return;
	var oDoc = jindo.$Document();
	jindo.$Fn(function() {
		weGoTop[(oDoc.scrollPosition().top > 1) ? "addClass" : "removeClass"]("show");
	}).attach(window, 'scroll').delay(0.1);
})();
