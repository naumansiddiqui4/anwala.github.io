jQuery("document").ready(function($){
    
    var nav = $('.td-header-container');
        
    $(window).scroll(function () {
        if ($(this).scrollTop() > 10) {
            nav.addClass("fix");
        } else {
            nav.removeClass("fix");
        }
    });

    var conteudo = $('#arredar')
    $(window).scroll(function () {
        if ($(this).scrollTop() > 10) {
            conteudo.addClass("conteudo-maior");
        } else {
            conteudo.removeClass("conteudo-maior");
        }
    });    
  

});
</script>
