<div id="shpHeader">


<script type="text/javascript">
    //create empty apntag object if it doesn't exist
    var apntag = apntag || {};
    //create a queue on the apntag object
    apntag.anq = apntag.anq || [];

    //load ast.js - async
    (function() {
       var d = document, e = d.createElement('script'), p = d.getElementsByTagName('head')[0];
       e.type = 'text/javascript';  e.async = true;
       e.src = 'https://acdn.adnxs.com/ast/static/0.7.4/ast.js';
       p.insertBefore(e, p.firstChild);
    })();

    //push commands to loading queue, to allow for async loading
    apntag.anq.push(function() {
        //set global page options
        apntag.setPageOpts({
            member: 3849, // Member ID 固定値
            disablePsa: true,
            targetingParams: {}
        });

        // Sets the Impression Bus endpoint to which ad requests are made
        apntag.setEndpoint('yj-a.p.adnxs.com');

        //define ad tag
        apntag.defineTag({
            tagId: 11903523,　 // Placement IDを設定
            sizes: [1,1], // リッチメディアだけの場合[1,1]にする。フィラーもあればフィラーのサイズを指定する。
            targetId: 'apn_ad_slot_1' // DivタグのＩＤを設定
        });
        //start loading tags
        apntag.loadTags();
    });
</script>

<div class="mdPRMB" id="apn_ad_slot_1" style="display:none">
    <script type="text/javascript">
        apntag.anq.push(function() {
            //signal to script that this DOM element has been loaded and is ready to be populated with an ad
            apntag.onEvent('adLoaded','apn_ad_slot_1',function(adObj, adError){
                if (! adObj || !adObj.native) // no native ad available
                    return;
                var native = adObj.native;
                var clickUrl = native.clickTrackers && native.clickTrackers[0];
                clickUrl += "clickenc=" + encodeURIComponent(native.clickUrl);
                var impTracker = native.impressionTrackers && native.impressionTrackers[0];
                img = native.mainMedia[0].url;
                document.getElementById("apn_ad_slot_1").style.display="block";
                document.getElementById("apn_ad_slot_1").innerHTML = 
                    '<p class="elItem">' +
                    '<a href="' + clickUrl + '" rel="nofollow"><img src="' + img + '" width="290" height="39" /></a>' +
                    '<img style="position: absolute; visibility: hidden; width:1px; height: 1px;" src="' + impTracker + '" />' +
                    '</p>';
            });
            apntag.showTag('apn_ad_slot_1');
        });
    </script>
</div>



