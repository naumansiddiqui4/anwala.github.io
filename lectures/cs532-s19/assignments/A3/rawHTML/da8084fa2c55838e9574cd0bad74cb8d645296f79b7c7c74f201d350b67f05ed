var jsonRapid = '{"keys":{"service":"news","pagetype":"detail","apptype":"web","opttype":"pc","vtgrpid":"hdl","nonepv":1,"status":"logout"},"spaceId":2080194056}';

YAHOO.JP.Ycd.recommend.loader.configure({
    useYcdn: true,
    url: 'https://headlines.yahoo.co.jp/hl?a=20190215-00010001-manetatsun-life',
