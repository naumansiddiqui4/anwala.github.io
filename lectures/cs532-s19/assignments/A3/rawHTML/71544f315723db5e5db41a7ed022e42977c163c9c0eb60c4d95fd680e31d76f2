                        $(window).load(function() {
                var $playerArrowLeft = $('.picHD.player .arrowLeft');
                var $playerArrowRight = $('.picHD.player .arrowRight');

                $('.sliderThumb', $this).each(function(){
                    var $thatSliderThumb = $(this),
                        wrapperWidth = $('.wrapper', $thatSliderThumb).innerWidth(),
                        arrowLeft = $('> .arrowLeft', $thatSliderThumb),
                        arrowRight = $('> .arrowRight', $thatSliderThumb),
                        $prev = null,
                        $next = null,
                        $thisListThumb = $('.listThumb', $thatSliderThumb);

                        //on nettoie les marges
                        $thisListThumb.children().css('marginLeft',0).last().css('marginRight',0);

                    var display = function(){
                        wrapperWidth = $('.wrapper', $thatSliderThumb).innerWidth();
                        if( $thisListThumb.width() > wrapperWidth){
                            var listPosStart = -($thisListThumb.position().left),
                                listPosEnd = listPosStart + wrapperWidth;


                            $prev = null;
                            $next = null;

                            $thisListThumb.children().each(function(){
                                var $this = $(this),
                                    itemPosStart = $this.position().left,
                                    itemPosEnd = itemPosStart + $this.outerWidth();

                                if($this.next().length > 0){
                                    itemPosEnd = $this.next().position().left;
                                }
                                if( itemPosStart < listPosStart && itemPosEnd >= listPosStart ){
                                    $prev = $this;
                                }

                                itemPosEnd = $this.position().left + $this.outerWidth();

                                if($this.prev().length > 0){
                                    itemPosStart = $this.prev().position().left + $this.prev().outerWidth();
                                }
                                if( itemPosStart <= listPosEnd && itemPosEnd > listPosEnd ){
                                    $next = $this;
                                }

                            });

                            arrowLeft.toggle($prev != null);
                            arrowRight.toggle($next != null);
                        }
                        else{
                            arrowLeft.hide();
                            arrowRight.hide();
                        }
                    };

                    display();

                    $(document).on('clickedTabTitle.chatPage', function(e, target) {
                        if(target == '#'+$this.attr('id')) {
                            display();
                        }
                    });

                    arrowLeft.on('click', function(e){
                         if($prev != null){
                            var pos = -($prev.position().left);
                            $thisListThumb.animate({'left': pos},400,'linear',function(){display();});
                        }
                        return false;
                    });
                    arrowRight.on('click', function(e){
                        if($next != null){
                            var pos = wrapperWidth - ($next.position().left + $next.outerWidth()) ;
                            $thisListThumb.animate({'left': pos},400,'linear',function(){display();});
                        }
                        return false;
                    });

                    var availableClipIds = [];

                    $('.video', $(this)).each(function() {
                        var hasCredits = _local.user.isAudio() || _local.user.hasCredits();

                        var clipId = $(this).attr('data-video-id');
                        var free   = videosState[clipId] && videosState[clipId].needCredits === '0';

                        if (free || hasCredits) {
                            availableClipIds.push(clipId);
                        }
                    });

                    var getPlayerVideoIndex = function() {
                        var index = window.lastVideoId === null ? null : availableClipIds.indexOf(window.lastVideoId);

                        if (index === -1) {
                            index = null;
                        }

                        return index;
                    };

                    var renderPlayerArrows = function() {
                        var index = getPlayerVideoIndex();

                        if (availableClipIds.length === 0) {
                            $playerArrowLeft.hide();
                            $playerArrowRight.hide();

                            return;
                        }

                        if (index === null) {
                            $playerArrowLeft.show();
                            $playerArrowRight.show();

                            return;
                        }

                        $playerArrowLeft.toggle(index !== 0);
                        $playerArrowRight.toggle(index !== (availableClipIds.length - 1));
                    };

                    $(document).on('clipsPlayer.clipLoaded', function(e) {
                        renderPlayerArrows();
                    });

                    $playerArrowLeft.on('click', function(e) {
                        var index = getPlayerVideoIndex();
                        var newIndex = index === null ? 0 : index - 1;

                        if (newIndex >= availableClipIds.length || newIndex < 0) {
                            return;
                        }

                        _local.clipsPlayer.startVideo(availableClipIds[newIndex]);
                    });

                    $playerArrowRight.on('click', function(e) {
                        var index = getPlayerVideoIndex();
                        var newIndex = index === null ? 0 : index + 1;

                        if (newIndex >= availableClipIds.length || newIndex < 0) {
                            return;
                        }

                        _local.clipsPlayer.startVideo(availableClipIds[newIndex]);
                    });

                    // Autostart the first free video if any
                    var freeClipIds = [];

                    for (var key in videosState) {
                        if (videosState[key].needCredits === '0') {
                            freeClipIds.push(key);
                        }
                    }

                    // Check if the videos block is visible (on some brands it can be hidden)
                    if ($this.is(':visible') && freeClipIds.length) {
                        var clipId = freeClipIds.shift();

                        _local.clipsPlayer.startVideo(clipId);
                    }
                });
            });
