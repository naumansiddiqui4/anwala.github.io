<script>
    _local.script.push(function() {
        // Grant dependencies
        if (
            !window.swfobject
            || !window.dnx
            || !window.dnx.core
            || !window.dnx.core.Cookie
        ) {
            return;
        }

        var objectId = 'flastTestElement';

        $('body').append('<div id="' + objectId + '"></div>');

        // callback used by streamplayer via ExternalInterface.call()
        _local.flashTestCallback = function(event, data) {
            // Remove the swf from DOM after the callback returns
            setTimeout(function() {
                swfobject.removeSWF(objectId);
            }, 0);

            dnx.core.Cookie.remove('had_stream_error_flash');
        };

        swfobject.embedSWF(
            '/bundles/dnxchat/swf/StreamPlayer.swf',
            objectId,
            '100%',
            '100%',
            '10.1.0',
            'playerProductInstall.swf',
            {
                eventCallback: '_local.flashTestCallback'
            },
            {
                quality: 'high',
                bgcolor: '#000000',
                allowScriptAccess: 'always',
                allowFullScreen: 'true',
                wmode: 'opaque'
            },
            {
                id: objectId,
                name: objectId,
                align: 'middle',
                style: 'position: fixed; top: 0; left: 0; width: 100%; height: 100%; z-index: -1;'
            }
        );

        // Display gdpr banner unless user has already actionned it
        if (dnx.core.Cookie.has('has_clicked_gdpr_banner')) {
            $('.gdprBanner').remove()
        } else {
            $('.gdprBanner')
                .addClass('is-visible')
                .on('click', '.js-accept-button', function(e) {
                    e.preventDefault();
                    dnx.core.Cookie.set('has_clicked_gdpr_banner', 1, {
                        expires: 90 * 24 * 3600 * 1000 // 90 days
                    });
                    // Hide it
                    $('.gdprBanner').fadeOut();
                })
        }
    });
</script>

            <script type="text/javascript">
                _local.script.push(function() {
                                                                                                
                        var footerInfos = {
                            'narrow' : '#footer li.txt',
                            'copyright'        : '#footer .copyright'
                        };

                        var jqxhr = $.ajax({
                            url: '\x2Fcnc\x2Ffooter\x2Dcopyright\x3Finfo\x255B0\x255D\x3Dnarrow\x26info\x255B1\x255D\x3Dcopyright',
                            type: 'GET'
                        });

                        jqxhr.done(function(data) {
                            for (var info in data) {
                                if (footerInfos[info]) {
                                    $(footerInfos[info]).html(data[info]);
                                }
                            }
                        });
                                    });
            </script>
        

    </body>
