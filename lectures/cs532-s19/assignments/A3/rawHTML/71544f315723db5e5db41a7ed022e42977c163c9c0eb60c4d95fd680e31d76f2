                            videosState["50891"] = {
                    canPlay:     "0",
                    needCredits: "1"
                };
                            videosState["58158"] = {
                    canPlay:     "0",
                    needCredits: "1"
                };
                            videosState["49867"] = {
                    canPlay:     "0",
                    needCredits: "1"
                };
                            videosState["2417"] = {
                    canPlay:     "0",
                    needCredits: "1"
                };
                    
        $('.cssPhotoAndVideo').each(function() {
            var $this = $(this);
            
            $(".listThumb .clip a.video", $this).on("click", function(event) {
                event.preventDefault();
                
                var clip       = $(this);
                var clipId     = clip.attr("data-video-id");
                var hasCredits = _local.user.isAudio() || _local.user.hasCredits();

                if (videosState[clipId] && videosState[clipId].canPlay === '1') {
                    if (videosState[clipId].needCredits === '1') {
                        if (_local.avsProcessing && _local.avsProcessing.isCheckRequired()) {
                            _local.avsProcessing.processWithConfirm();
                            return false;
                        }
                    }
                    if (hasCredits || videosState[clipId].needCredits === '0') {
                        if ($(document).scrollTop() === 0) {
                            // Start the clip player
                            _local.clipsPlayer.startVideo(clipId);
                        }
                        else {
                            _local.animateScrollTop(
                                {
                                    scrollTop: 0
                                },
                                200,
                                function() {
                                    // Start the clip player
                                    _local.clipsPlayer.startVideo(clipId);
                                }
                            );
                        }
                    } else {
                        // If we have no credits and the clip is paid, show the payment popin
                        $(document).trigger('callModal.popinPayment', "Out of Credits Pop up");
                    }
                }
                else {
                    var urlLoginPage = '/en/login/SharonSexy/';
                    window.location.href = _local.ga.addParameters(urlLoginPage);
                }
            });

