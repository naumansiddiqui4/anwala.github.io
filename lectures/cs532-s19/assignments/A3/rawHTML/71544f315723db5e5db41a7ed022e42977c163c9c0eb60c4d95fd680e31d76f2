_local.script.push(function(){
    $('.cssWall').each(function(){
        var $this = $(this);

        if( !$this.data('_localJsRun') ){
            $this.data('_localJsRun', true);

            var scrollVerticalRun = false,
                initWallTriggers = function() {
                    $this.each(function () {
                        var $this = $(this);

                        $('form.reply', $this)
                            .on('submit', function (e) {
                                var $thisInputAddPost = $('input', $(this));
                                var parentPostId = $thisInputAddPost.attr('data-postId');
                                $(document).trigger('postInWall.dnxChat', [$thisInputAddPost.val(), parentPostId]);
                                $thisInputAddPost.val('');
                                return false;
                            });

                        refreshScroll();
                    });
                },
                refreshScroll = function(){
                    var elContent = $('.pannelContent', $this),
                        elList = $('.pannelContent > ul', $this);

                    if( !scrollVerticalRun ){
                        scrollVerticalRun = true;

                        _local.scroll.vertical( elContent, elList, {step: '100px'} );
                    }

                    if( elList.height() < elContent.height() ){
                        elContent.addClass('withoutSlider');
                    }
                    else{
                        elContent.removeClass('withoutSlider');
                    }
                };
            
            $(document).on("wall_content_updated", function() {
                $.ajax({
                    url: "/ajax/wall/c16014",
                    type: "GET",
                    success: function(data) {
                        scrollVerticalRun = false;
                        
                        $('.pannelContent', $this).html(data);
                        initWallTriggers();
                    }
                });
            });

            $(document)
                .on('callScrollVertical.wall', function(){
                    refreshScroll();
                })
                .on('postInWall.dnxChat', function (e, message, parentPostId) {
                    // Spinner
                    $this.spin(_local.spin.options.base);

                    $.post('/ajax/wall/post', {
                            message: message,
                            parentPostId: parentPostId,
                            profileId: 'c16014'
                        }, function(data, textStatus, jqXHR) {
                            scrollVerticalRun = false;
                            $this.spin(false);
                            var ct = jqXHR.getResponseHeader('Content-type') || '';
                            if (ct.indexOf('json') > -1) {
                                //response is json, so there was an error, display alert
                                if (data == 'INVALID_NICKNAME') {
                                    $error = "Invalid\x20nickname";
                                } else if (data == 'TOO_SOON') {
                                    $error = "Please\x20wait\x20just\x20for\x20a\x20moment\x20before\x20replying\x20again";
                                } else if (data == 'SAME_MESSAGE') {
                                    $error = "You\x20cannot\x20post\x20the\x20same\x20message\x20twice";
                                } else {
                                    $error = "An\x20error\x20occured";
                                }
                                _local.modal.alert($error);
                            } else {
                                //response is html, refresh wall
                                $('.pannelContent', $this).html(data);
                                initWallTriggers();
                            }
                        }
                    );
                });

            initWallTriggers();
        }
    });
});
