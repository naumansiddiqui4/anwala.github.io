_local.script.push(function(){
    var cssRegisterName = 'cssListLang';

    if( typeof _local.css[cssRegisterName] == 'undefined' ){
        _local.css[cssRegisterName] = true;

        $('.cssListLang').each(function(){
            var that = $(this),
                langSelected = $('li.selected', that).index(),
                langDefault = langSelected == -1 ? 0 : langSelected,
                langDisplay = langDefault,
                elLiDisplay = function(i){
                    var elUl = $('ul', that);

                    langDisplay = i;
                    $('li', that).hide().eq(i).show();
                    elUl.removeClass('open').addClass('close');
                    open = false;
                },
                elLiHide = function(){
                    var elUl = $('ul', that);

                    $('li', that).show();
                    elUl.removeClass('close').addClass('open');
                    open = true;
                },
                open = true;

            that
                .on('click', 'li', function(e){
                    if( $(this).index() > 0 ){

                        $("a", "li#selectedLanguage").text($("a", this).text());
                        
                        var $a = $('a', $(this));
                        
                        window.location = $a.attr('href');
                    }

                    if( open ){
                        
                                                elLiDisplay(0);
                        $(this).triggerHandler('onChoose');
                    }
                    else{
                        elLiHide();

                        $(this).triggerHandler('onOpen');
                    }

                })
                .on('click', 'li a', function(e){
                    e.preventDefault();
                });

            elLiDisplay(langDefault);

            $(document).on('click', function(e){
                if( !$(e.target).parents().is( that ) && open ){
                    $('li:eq(0)', that).trigger('click');
                }
            });
        });
    }
});
