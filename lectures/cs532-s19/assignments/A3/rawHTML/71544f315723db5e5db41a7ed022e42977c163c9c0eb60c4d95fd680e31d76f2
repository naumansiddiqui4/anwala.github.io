        updateChatType: function(type, profileId, cammerSwitched) {
            if (type != _local.ga.tags.updateChatType.type || profileId != _local.ga.tags.updateChatType.profileId) {
                var pageName = null;

                switch (type) {
                    case "FREE":  // Free
                        pageName = "Public/Free Chat/"+profileId;
                        break;
                    case "O2M": // GroupShow
                        pageName = "Private/Paid Chat/Group Show/"+profileId;
                        break;
                    case "O2O":  // PrivateShow
                        pageName = "Private/Paid Chat/1 to 1/"+profileId;
                }

                if (pageName !== null) {
                    if (cammerSwitched) {
                        pageName = 'Switch/' + pageName;
                    }

                    _local.ga("pageView", pageName);
                }
                
                _local.ga.tags.updateChatType.type = type;
                _local.ga.tags.updateChatType.profileId = profileId;
            }
        },
        trackCreditsConsumption: function(amount) {
            if (!_local.ga.tags.trackCreditsConsumption.firstAmount) {
                _local.ga.tags.trackCreditsConsumption.firstAmount = amount;
            }
            
            var firstAmount = _local.ga.tags.trackCreditsConsumption.firstAmount;
            var consummateAmount = firstAmount - amount;
            
                            if (consummateAmount && !_local.ga.tags.trackCreditsConsumption.sent) {
                                            _local.ga("pageView", "Biz Goal - Account visitors first paying action ever");
                                        _local.ga("pageView", "Biz Goal - Account visitors first paying action in this session");
                    
                    _local.ga.tags.trackCreditsConsumption.sent = true;

                    $.get("/tags/recordinfo?k=firstConsumption");
                }
                    },
