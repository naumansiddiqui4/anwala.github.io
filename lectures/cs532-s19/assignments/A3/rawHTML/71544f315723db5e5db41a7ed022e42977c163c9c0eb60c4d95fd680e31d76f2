            var $target = $('#' + $toggle.data('target'));
            
            $toggle.on('click', function() {
                $toggle.data('ignore-mouseover', false);
            }).on('click', function(event) {
                if ($target.is('.hide') && !$toggle.data('ignore-mouseover')) {
                    event.stopPropagation();
                    
                    $toggle.addClass('active');
                    $target.removeClass('hide');
                }
            });
            
            $(document).on('click', function(event){
                if ($toggle.hasClass('active')) {
                    // prevent mouseenter event in case the mouse is already over the button
                    $toggle.data('ignore-mouseover', true);
                    
                    $toggle.removeClass('active');
                    $target.addClass('hide');
                    
                    var onMouseEnterHandler = function() {
                        $toggle
                            .data('ignore-mouseover', false)
                            .off('mouseenter', onMouseEnterHandler)
                        ;
                    };
                    
                    $toggle.on('mouseenter', onMouseEnterHandler);
                }
            });
        });

