    _local.ga.getPaymentMethodName = function(token, def) {
        token = token || "";
        token = token.toLowerCase();
        var names = {
                            creditcard: "CB",                             ideal: "iDeal",                             paysafecard: "PaysafeCard",                             clickandbuy: "ClickandBuy",                             bcmc: "BCMC",                             directpay24: "Sofort",                             multibanco: "Multibanco",                             latambilling: "Latambilling",                             astropaycard: "AstroPayCard"                    };
        return !!names[token] ? names[token] : def;
    };
    
    
