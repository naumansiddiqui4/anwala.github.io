        // limit the number of times a tag will be sent per session
        _limit: function(type, max) {
            var name = 'ga_limit_'+type;
            var cookie = new dnx.core.Cookie(name);
            var nbSent = parseInt(cookie.getValue(0));
            if (nbSent < max) {
                cookie
                    .setValue(++nbSent)
                    .save()
                ;
                
                return true;
            }
            
            return false;
        },
        
