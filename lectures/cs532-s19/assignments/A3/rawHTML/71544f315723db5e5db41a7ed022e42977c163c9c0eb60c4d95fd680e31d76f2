_local.config = $.extend(_local.config, {
    videoWatchedUrl: '/video/watched/%video_id%'
});

_local.script.push(function() {

    window.lastVideoId = window.lastVideoId || null;

    _local.clipsPlayer = {};

    var videoPixel = null;

    var cleanClipStuff = function() {
        $('#ugc_player').empty();

        if (videoPixel !== null) {
            videoPixel.destroy();

            videoPixel = null;
        }
    };

    var initPixel = function(sessionId) {
        videoPixel = new dnx.logic.VideoPixel('https://svc.dnxnetwork.lu/', sessionId, 'c16014');
    };

    _local.clipsPlayer.startVideo = function (clipId) {
        _local.clipsPlayer.preparePlace('player');

        _local.clipsPlayer.loadClip(clipId);
    };

    //prepare the scene for the profile page
    _local.clipsPlayer.preparePlace = function (place) {
        if (place == 'image') {
            cleanClipStuff();

            $('.picHD.image').show();
            $('.picHD.player').hide();
        }
        else {
            $('.picHD.image').hide();
            $('.picHD.player').show();
            $('.pictoTeaser').hide();
        }
    };

    _local.clipsPlayer.loadClip = function (clipId, usePopin) {
        window.lastVideoId = clipId;

        $(document).trigger('clipsPlayer.clipLoaded');

        cleanClipStuff();

        if (!usePopin) {
            $('#idPopinVideosClipPlayer').remove();
        }

        if ($('.video[data-video-id="' + clipId + '"]').data('teaser') == 1) {
            $('.pictoTeaser').show();
        }

        var url = '/ajax/clip/0';
        url = url.replace('/0', '/' + clipId);

        $.getJSON(url, [], function (data) {
            var clipUrl  = data.clipUrl,
                snapshot = data.snapshotUrl;

            if (usePopin) {
                $('#idPopinVideosClipPlayer figcaption').each(function() {
                    var $this = $(this);

                    $('.clipTitle', $this).text(data.title);
                    $('.clipDescription', $this).text(data.description);
                });
            }

            _local.ga("pageView", "Private/Clips Player/c16014");

            if (!clipUrl) {
                // Paid clip, and visitor not logged in => login page
                var urlLoginPage = "/en/login/SharonSexy/";

                window.location.href = _local.ga.addParameters(urlLoginPage);

                return false;
            }

            var trackCreditConsumption = data.sessionId && !_local.user.isAudio();

            if (trackCreditConsumption) {
                initPixel(data.sessionId);
            }

            createPlayer(clipUrl, snapshot);
        });
    };

    var createPlayer = function(clip_url, snapshot) {
        var $playerContainer = $('#ugc_player');

        var options =  {} ;

        var controls = options.controlbar === 'bottom';

        var $video = $('<video />', {
            src: clip_url,
            controls: controls,
            autoplay: true,
            controlsList: 'nodownload' // Hive donwload button on chrome. Option available in Chrome 58+
        });

        $video.on('play', function(e) {
            // Add clip counter
            $.ajax({
                url: _local.config.videoWatchedUrl.replace(/%video_id%/, window.lastVideoId)
            });

            if (videoPixel !== null) {
                videoPixel.play();
            }
        });

        $video.on('pause', function(e) {
            if (videoPixel !== null) {
                videoPixel.pause();
            }
        });

        $video.on('click', function(e) {
            var video = $video[0];

            if (video.paused) {
                video.play();
            }
            else {
                video.pause();
            }
        });

        $video.appendTo($playerContainer);
    };

    var initPlayer = function() {
        $('#idPopinVideosClipPlayer').each(function() {
            var $this = $(this);

            $this
                .on('click', '.slider .clip a', function() {
                    var thatLink = $(this),
                        videoId = thatLink.attr('data-video-id');

                    _local.clipsPlayer.loadClip(videoId, true);

                    return false;
                })
                .on('close.modal', function () {
                    cleanClipStuff();
                });

            $('.slider', $this).each(function() {
                var $thisSlider = $(this),
                    $elListProfil = $('.list', $thisSlider);

                _local.scroll.vertical($thisSlider, $elListProfil);

                if ($thisSlider.height() < $elListProfil.height()) {
                    $thisSlider.removeClass('withoutSlider');
                }
                else {
                    $thisSlider.addClass('withoutSlider');
                }

            });


        });
    };

    
});
