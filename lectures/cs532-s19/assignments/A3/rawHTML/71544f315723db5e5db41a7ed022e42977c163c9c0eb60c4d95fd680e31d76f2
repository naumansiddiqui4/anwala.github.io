        addCrossDomainParameters: function(context) {
            var secureDomain  = "https\x3A\x2F\x2Fwww.sharonsexy.com";
            var isCrossDomain = !(!"");
            
            var $targets   = $('.js-ga-cross-domain', context);
            var attributes = [ "action", "href" ];
            
            if (isCrossDomain) {
                $targets = $targets.add('.js-ga-cross-domain--internal', context);

                var escapedSecureDomain = secureDomain.replace(/'/g, "\\\\'");
                $targets = $targets.add("a[href^='"+escapedSecureDomain+"'], form[action^='"+escapedSecureDomain+"']", context);
            }
            
            if ($targets.length) {
                _gaq.push(function() {
                    var tracker = _gat._getTrackerByName(_local.ga.defaultAccount);
                    
                    $targets.each(function() {
                        var $this = $(this);
                        
                        for (var i in attributes) {
                            var attribute = attributes[i];
                            if ($this.is("["+attribute+"]")) {
                                $this.attr(attribute, tracker._getLinkerUrl($this.attr(attribute)));
                                break;
                            }
                        }
                    });
                });
            }
        }
    };

        
            _local.ga.defaultAccount = "dnx";
    
    _local.ga("addCrossDomainParameters");
    
    for (var i = 0; i < _local.ga.queue.length; ++i) {
        var tag = _local.ga.queue[i];
        _local.ga.tags[tag.functionName].apply(this, tag.parameters);
    }
    
            
        
                    });
