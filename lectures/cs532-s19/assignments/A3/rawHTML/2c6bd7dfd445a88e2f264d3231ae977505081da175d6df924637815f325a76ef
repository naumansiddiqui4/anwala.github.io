<script>
// detect if safari mobile
    function isMobileSafari() {
        return navigator.userAgent.match(/(iPod|iPhone|iPad)/) && navigator.userAgent.match(/AppleWebKit/)
    }

    function isMobileAndroid(){
        return navigator.userAgent.match(/android/)
    }

    //Launch the element in your app if it's already installed on the phone
    function LaunchApp(){
      window.open("iflyer://","_self");
    };

$('#getapp').on('click',function(){
    if (isMobileSafari()){
        var appstorefail = "https://itunes.apple.com/app/iflyer/id687205010";
        window.location = appstorefail;

        /*
        var loadedAt = +new Date;
        setTimeout(
          function(){
            if (+new Date - loadedAt < 2000){
              window.location = appstorefail;
            }
          }
        ,100);
        LaunchApp()
        */

    }else if(isMobileAndroid()){
        var appstore = "https://play.google.com/store/apps/details?id=tv.iflyer";
        window.location = appstore;
    }
});
</script>

