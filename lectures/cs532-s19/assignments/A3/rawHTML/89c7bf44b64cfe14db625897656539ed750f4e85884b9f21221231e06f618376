<script>
function loadPubli(){
	if(isAdsEnabled()){	
	    	publicargada = true;
    		if(EPI.publi.DOMCargado){
			EPI.publi.createInterval();
		}
		else{
			document.addEventListener("DOMContentLoaded", function(event) {
				EPI.publi.createInterval();
			});
		}
	}
}
</script>

