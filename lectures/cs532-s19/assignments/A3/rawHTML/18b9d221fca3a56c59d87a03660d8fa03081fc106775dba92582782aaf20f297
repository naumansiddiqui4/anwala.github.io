    $("#tweetText").keyup(function () {
        var box = $(this).val();
        var main = box.length;
        //var value = (main / 116);
        var count = 117 - main;
        if (box.length < 117) {
            $('.counterTweet').val(count);
        } else {
            $('.counterTweet').css('color', 'red');
            $('.counterTweet').val(count);
            //alert('can not exceed more than 140 characters')
        }
        return false;
    });
