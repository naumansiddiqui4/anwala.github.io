    var mousePos;
    $(".main_article_content").mousemove(function (e) {
        mousePos = {left: e.pageX, top: e.pageY + 5};
    });
    $(".main_article_content").bind('mouseup touchend', function () {
        var current = window.getSelection().toString();
        setTimeout(function () {
            var selectedText = current;
            if (current != '') {
                $('.social_icons_text').show();
                $('.social_icons_text').offset(mousePos);
                selectedText = encodeURIComponent(selectedText).replace(/'/g, "%27");
                var short_url = 'http://nm-4.com/xfg8';
                $(".share_fb_text").attr("onclick", "shareArticleText('" + selectedText + "', 'fb')");
                $(".share_twt_text").attr("onclick", "shareArticleText('" + selectedText + "', 'twt')");
                $(".share_wa_text").attr("href", "whatsapp://send?text=" + current + '.' + short_url);
                //alert($("#share_twt_text").attr("onclick"));
            } else {
                $('.social_icons_text').hide();
            }


        }, 100);
    });
    function shareArticleText(text, shareto) {
        $("#search_loader").css("display", "block");
        $("#search_loader").css("display", "none");
        var canvas = document.getElementById('finalCanvas');
        var context = canvas.getContext('2d');
        context.clearRect(0, 0, canvas.width, canvas.height);
        document.getElementById("finalDiv").style.display = 'block';
        document.getElementById("finalCanvas").style.display = 'none';
        $("#tweetShareModal").modal({
            backdrop: 'static',
            show: true
        });
        $("#sharetxt").text(decodeURIComponent(text));
        var link = 'http://nm-4.com/xfg8';
        var name = $("#article_title").text();
        var shareText = name + " " + link + " via @narendramodi";
        var text_char = 117 - shareText.length;
        $('.counterTweet').val(text_char);
        $("#tweetText").val(shareText);
        if (text_char < 0) {
            $('.counterTweet').css('color', 'red');
        }
        $("#shareTwtButton").unbind().click(function (e) {
            e.preventDefault();
            var tweetText = "";
            if ($("#tweetText").val() == "") {
                alert("Please enter the twitter text.");
                return;
            } else {
                tweetText = $("#tweetText").val();
                if (tweetText.length > 117) {
                    alert("can not exceed more than 117 characters");
                    return;
                }
                $("#tweetShareHide").click();
                var html = $("#shareimage").val();
                $("#search_loader").css("display", "none");
                var page_url = JS_BASE_URL + 'embed/sharetweet?img=' + html + '&txt=' + encodeURIComponent(tweetText).replace(/'/g, "%27");
                var w = 800;
                var h = 650;
                var left = (screen.width / 2) - (w / 2);
                var top = (screen.height / 2) - (h / 2);
                window.open(page_url, '', 'menubar=no,toolbar=no,resizable=no,scrollbars=no,height=400,width=600');
                return true;
            }
        });
        $("#shareFBButton").unbind().click(function (e) {
            e.preventDefault();
            $("#tweetShareHide").click();
            var tweetText = "";
            var link = 'http://nm-4.com/xfg8';
            var name = encodeURIComponent($("#article_title").text()).replace(/'/g, "%27");
            var redirect_uri = 'https://www.narendramodi.in/here-are-the-top-news-stories-for-14th-feb-2019-543523';
            var picture = $("#shareimage").val();
            //                                var app_id = "1633330666942533"; //local.
            //var app_id = "1586536814955252"; //stage
            var app_id = "398836320185104"; //live
            PopupCenter("https://www.facebook.com/dialog/feed?app_id=" + app_id + "&link=" + link + '&name=' + name + '&description=' + '&redirect_uri=' + encodeURI(redirect_uri) + '&picture=' + picture);
        });
    }
