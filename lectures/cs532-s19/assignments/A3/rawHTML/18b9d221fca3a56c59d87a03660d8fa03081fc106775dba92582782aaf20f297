    $("#tweetShareModal").on('shown.bs.modal', function () {
        setTimeout(function () {
            html2canvas($("#finalDiv"), {
                onrendered: function (canvas) {
                    loadCanvas(canvas.toDataURL("image/png;base64;"));
                }
            });
        }, 300);
    });
    function loadCanvas(dataURL) {
        var canvas = document.getElementById('finalCanvas');
        var canvasMy = document.getElementById('finalDiv');
        var context = canvas.getContext('2d');
        var imageObj = new Image();
        canvas.width = canvasMy.offsetWidth;
        canvas.height = canvasMy.offsetHeight;
        imageObj.onload = function () {
            context.drawImage(this, 0, 0, canvasMy.offsetWidth, canvasMy.offsetHeight);
        };
        imageObj.src = dataURL;
        $.ajax({
            url: 'https://www.narendramodi.in/article/getshareimage',
            type: 'POST',
            data: 'image_data=' + dataURL,
            success: function (data) {
                $("#shareimage").val(data);
                document.getElementById("finalDiv").style.display = 'none';
                document.getElementById("finalCanvas").style.display = 'block';
            },
        });
    }



