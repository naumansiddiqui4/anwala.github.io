    function signImage()
    {
        $(".main_article_content p img").each(function () {
            if (this.naturalWidth < 200)
            {
                $(this).addClass('signature');
            }
        });
        $(".main_article_content img").each(function () {
            if (this.naturalWidth < 200)
            {
                $(this).addClass('signature');
            }
        });
        $(".main_article_content div img").each(function () {
            if (this.naturalWidth < 200)
            {
                $(this).addClass('signature');
            }
        });
        $(".article_body img").each(function () {
            var img_title = $("#article_title").text();
            var tag = "Kumbh Mela, Prayagraj, Uttar Pradesh, Ministry of Drinking Water and Sanitation, Good Governance, Clean India";
            $(this).attr('alt', img_title.replace(/'/g, "%27") + ' ' + tag.replace(/'/g, "%27"));
        });
