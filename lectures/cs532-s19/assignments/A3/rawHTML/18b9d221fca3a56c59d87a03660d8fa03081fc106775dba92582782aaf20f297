<script src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
<script type="text/javascript">

                                        var url = 'here-are-the-top-news-stories-for-14th-feb-2019';
                                        var lang = $("#language").val();

                                        var pagesize = 10;
                                        var page = 1;
                                        var loading = false;
                                        var count = 1;
                                        var next_read_count = 1;
                                        $(window).on('scroll', function () {
                                            if (($(window).scrollTop() / ($(document).height() - $(window).height())) > 0.95) {

                                                if (!loading) {
                                                    loading = true;
                                                    loadArticle();//call function to load content when scroll reachs DIV bottom 

                                                }
                                            }

                                            $('div.next_read_div').each(function () {
                                                var isElementInView = Utils.isElementInView(this, false);

                                                if (isElementInView) {
                                                    var visible_id = $(this).attr("id");
                                                    var scrollUrl = $("#url" + visible_id).val();
                                                    var scrollTitle = $("#title" + visible_id).val();
                                                    setTitle(scrollUrl, scrollTitle);

//                                    console.log('in view');
//                                console.log(scrollUrl);

                                                }

                                            })



                                            var isElementInView = Utils.isElementInView($("div.innMainNews"), false);

                                            if (isElementInView) {
                                                var visible_id = $("#get_fixed_id").val();
                                                var scrollUrl = $("#url" + visible_id).val();
                                                var scrollTitle = $("#title" + visible_id).val();
                                                setTitle(scrollUrl, scrollTitle);

//                                console.log('in view');
//                                console.log(scrollUrl);

                                            }
                                        });


                                        function loadArticle()
                                        {
                                            if (count > 10) {
                                                count = 1;
                                            }
                                            var type = $("#content_type").val();

                                            var id = $("#content_id").val();
                                            var tag = $("#bradcrumb").val();
                                            var mainId = $("#get_fixed_id").val();
                                            if ((type != "") && (id != "") && (tag != "")) {
                                                $("#loader").css("display", "block");
                                                $.ajax({
                                                    url: JS_BASE_URL + 'article/loaddetails?lang=' + lang + '&type=' + type + '&id=' + id + '&count=' + count + '&tag=' + tag + '&mainId=' + mainId,
                                                    success: function (data) {
                                                        if (data != "") {
                                                            $("#content_type").remove();
                                                            $("#content_id").remove();
                                                            $('#detailNews').append(data);
                                                            $("#loader").css("display", "none");
                                                            count++;
                                                            replacetweetPoints();
                                                            replaceShareImages();
                                                            setTimeout(signImage, 1000);
                                                            var next_id = $('div.next_read_div:last').attr("id");
                                                            var scrollUrl = $("#url" + next_id).val();
                                                            var scrollTitle = $("#title" + next_id).val();
                                                            setTitle(scrollUrl, scrollTitle);
                                                            var mousePos;
                                                            $(".main_article_content").mousemove(function (e) {
                                                                mousePos = {left: e.pageX, top: e.pageY + 5};
                                                            });
                                                            $(".fullColumn").each(function () {
                                                                if ($(this).find(".banner").length == 0) {
                                                                    $(this).find(".newsUpCaptionDetail").addClass("no-thumb");
                                                                    $(this).find(".newsUpdateBanner").addClass("no-thumb2");
                                                                }
                                                            });
                                                            $(".imgLiquidFill").imgLiquid({fill: true});
                                                            $(".imgLiquidNoFill").imgLiquid({fill: false});
                                                            $(".main_article_content").bind('mouseup touchend', function () {
                                                                var current = window.getSelection().toString();
                                                                setTimeout(function () {
                                                                    var selectedText = current;
                                                                    if (current != '') {
                                                                        $('.social_icons_text').show();
                                                                        $('.social_icons_text').offset(mousePos);
                                                                        selectedText = encodeURIComponent(selectedText).replace(/'/g, "%27");
                                                                        var short_url = 'http://nm-4.com/xfg8';
                                                                        $(".share_fb_text").attr("onclick", "shareArticleText('" + selectedText + "', 'fb')");
                                                                        $(".share_twt_text").attr("onclick", "shareArticleText('" + selectedText + "', 'twt')");
                                                                        $(".share_wa_text").attr("href", "whatsapp://send?text=" + current + '.' + short_url);
                                                                        //alert($("#share_twt_text").attr("onclick"));
                                                                    } else {
                                                                        $('.social_icons_text').hide();
                                                                    }


                                                                }, 100);
                                                            });

                                                            $.getScript('https://platform.twitter.com/widgets.js');
                                                            twttr.widgets.load($("#mainDiv"));
                                                            loading = false;
                                                            setTimeout(function () {
                                                                userActivity();
                                                            }, 1000);

                                                        }
                                                    }
                                                });
                                            }
                                        }

                                        function userActivity() {
                                            if ($("#cristal_id").val() && $("#content_type").val() == 'article') {
                                                var contentId = 543523;
                                                var userId = $("#cristal_id").val();
                                                var type = $("#content_type").val();
                                                console.log($("#content_id").val());
                                                $.ajax({
                                                    url: JS_BASE_URL + 'article/useractivitylog?type=' + type + '&contentid=' + contentId + '&userId=' + userId,
                                                    success: function (data) {
                                                        console.log(data);
                                                    }
                                                })
                                            }
                                        }

                                        $(document).ready(function () {
                                            userActivity();
                                        });

                                        function setTitle(newUrl, newTitle) {

                                            window.history.pushState("", newTitle, newUrl);

                                            document.title = newTitle;
                                        }
                                        function hideSticky() {
                                            $('#namoAndroidApp').fadeOut(350, 'easeInCirc');
                                            $('#namoAndroidApp').hide();
                                        }


                                        //function to show share text tooltip
                                        function showSelectToolTip() {
                                            var getselecttextCookieValue = getCookie('article_select_text_cookie');
                                            if (!getselecttextCookieValue) {
                                                var pattern = /<iframe.*?\/iframe>/gm;
                                                var imgpattern = /<img\s+[^>]*src="([^"]*)"[^>]*>/;
                                                $('.article_body p').each(function (item) {
                                                    var inHtml = "";
                                                    inHtml = $(this)[0].innerHTML;
                                                    if (inHtml != "") {
                                                        if (!pattern.exec(inHtml)) {
                                                            if (!imgpattern.exec(inHtml)) {
                                                                var position = $(this)[0].offsetTop;
                                                                $(".gotitContainer").css({top: position});
                                                                $('.article_body').fadeIn(400, 'linear', function () {
                                                                    $('.gotitContainer').fadeIn(400, 'linear');
                                                                    $('#gotit').focus();
                                                                    //window.location.hash = '#gotitContainer';

                                                                });
                                                                return false;
                                                            }
                                                        }
                                                    }
                                                });
                                            }
                                            $('#gotit').on('click', function (event) {
                                                $('.removeOverlay').fadeOut(400, 'linear');
                                                setCookie('article_select_text_cookie', 'done', 80000);
                                            });
                                        }

                                        function replacetweetPoints()
                                        {
                                            $('.twitterPoint').each(
                                                    function () {
                                                        var tweettext = "";
                                                        tweettext = $(this).text();
                                                        var pageurl = window.location.href;
                                                        var html = '<div class="TwitterLeft"><img class="" src="https://staticmain.narendramodi.in/images/quoteIconArticle.jpg"  alt="Quote"/>';
                                                        html = html + tweettext + '</div>';
                                                        html = html + '<div class="TwitterRight">';
                                                        html = html + '<a href="javascript:void(0)" onclick="tweet(this)"><i class="fa fa-twitter"></i></a>'
                                                        $(this).removeClass('twitterPoint');
                                                        $(this).text('');
                                                        $(this).removeAttr('style');
                                                        $(this).addClass('twitterDiv');
                                                        $(this).append(html);
                                                    });
                                        }
                                        function replaceShareImages()
                                        {
                                            $('.imageShare').each(
                                                    function () {
                                                        var src = "";
                                                        src = $(this).children('img').attr('src');
                                                        var pageurl = window.location.href;
                                                        var img_title = $("#article_title").text();
                                                        if (img_title.length > 80) {
                                                            img_title = img_title.substring(0, 80);
                                                            img_title = img_title + '...';
                                                        }
                                                        var img_title = encodeURIComponent(img_title).replace(/'/g, "%27");
                                                        var twt_arg = "'" + img_title + " " + $("#bitly_url").val() + "','" + src + "'";
                                                        var fb_arg = "'" + $("#bitly_url").val() + "','" + '' + "','" + src + "','" + img_title + "'";
                                                        var html = '<div class="articleImgShare">';
                                                        html = html + '<a href="javascript:void(0)"  onclick="shareThenAndNow(' + twt_arg + ')"><i class="fa fa-twitter last"></i></a><span> | </span>';
                                                        html = html + '<a href="javascript:void(0)"  onclick="facebook_share(' + fb_arg + ')"><i class="fa fa-facebook"></i></a></div>';
                                                        $(this).removeAttr('style');
                                                        //$(this).addClass('twitter_div');
                                                        $(this).append(html);
                                                        $(this).append('<div class="clearfix"></div>');
                                                    });
                                        }
                                        function tweet(tweet_point)
                                        {
                                            var tweet_text = $(tweet_point).parent().siblings('.TwitterLeft').text();
                                            var bitly_url = $("#bitly_url").val();
                                            window.open('https://twitter.com/intent/tweet?original_referer=' + window.location.href + '&amp;text=' + encodeURIComponent(tweet_text) + ' &amp;tw_p=tweetbutton&amp;url=' + bitly_url + '&amp;via=narendramodi', 'menubar=no', 'toolbar=no', 'resizable=yes', 'scrollbars=yes', 'height=400', 'width=300')
                                            return false;
                                        }
                                        function addClass() {
                                            $('.twitter-tweet-rendered').each(
                                                    function () {
                                                        $(this).css('margin', '0 auto');
                                                    });
                                        }


</script>

<script type="text/javascript">
    $('.comment_box_container_submit').on("click", function (e) {
        var comment = $("#comment").val();
        var count = $('#commentka').attr('commentValue');
        if (comment == "") {
            alert("Please type your Comment..!");
            return false;
        } else {
            e.preventDefault();
            $.ajax({
                url: JS_BASE_URL + 'site/comment',
                type: 'POST',
                data: $("#content_comment").serialize(),
                success: function (result) {
                    if (result == 'block keywords') {
                        $("#alertmessage").html("You are not authorized to post this comment!");
                        $("#alertmessage").css("display", "block");
                    } else if (result == 'duplicate_comment') {
                        $("#alertmessage").html(" You have already posted this comment!");
                        $("#alertmessage").css("display", "block");
                    } else if (result != "") {
                        $(".slimScrollDiv").css("display", "block");
                        $("#alertmessage").css("display", "none");
                        $("#commentList").prepend(result);
                        var countUp = ++count;
                        $("#commenttotle").html("");
                        $("#commenttotle").html(" Comments  (" + countUp + ")");
                        $('#commentka').attr('commentValue', countUp);
                        $("#comment").val("");
                        commentedEvent();
                    }

                }
            });
        }
    });
    function commentedEvent() {
