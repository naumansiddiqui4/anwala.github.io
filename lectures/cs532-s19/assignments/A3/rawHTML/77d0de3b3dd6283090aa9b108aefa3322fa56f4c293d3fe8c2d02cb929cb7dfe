        function siteExtraTargeting() {
            googletag.cmd.push(function () {
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().enableSingleRequest();
                googletag.pubads().set('page_url', document.location.href);
                googletag.pubads().setTargeting('adblock', getAdblockSegment());
                googletag.pubads().setTargeting('source', getSourceSegment());
                googletag.pubads().setTargeting('segment', getAdSelector());
                googletag.pubads().setTargeting('url', getAdUrl());
                googletag.pubads().setTargeting('platform', getAdPlatform());
                googletag.pubads().setTargeting('segtest', getSegtest());
                googletag.pubads().setTargeting('landing', "" + getLanding(1));
                googletag.enableServices();
            });
        }

        var getHbUnitsSetup = function (sizes, title) {
            var unitsDesktop = [];
            var unitsMobile = [];
            var unitSupportsWidth = {};
            var unitSupportsHeight = {};
            for (var i in sizes) {
                if (sizes.hasOwnProperty(i)) {
                    unitSupportsWidth[sizes[i][0]] = sizes[i][0];
                    unitSupportsHeight[sizes[i][1]] = sizes[i][1];
                }
            }

            var setup = {
                "HNHH_Desktop_Header": [
                    {bidder: 'ix', params: {id: '01', siteId: "195678", size: [728, 90]}},
                    {bidder: 'ix', params: {id: '02', siteId: "195679", size: [970, 250]}},
