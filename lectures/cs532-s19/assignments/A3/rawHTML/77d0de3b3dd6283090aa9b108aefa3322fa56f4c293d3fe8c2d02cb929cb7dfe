        function __setCookie(cname, cvalue, sec) {
            var d = new Date();
            d.setTime(d.getTime() + (sec * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
