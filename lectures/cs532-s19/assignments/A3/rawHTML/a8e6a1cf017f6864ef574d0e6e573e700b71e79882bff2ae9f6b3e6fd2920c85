<script type="text/javascript">
function addOnloadEvent(fnc){
	  if ( typeof window.addEventListener != "undefined" )
	    window.addEventListener( "load", fnc, false );
	  else if ( typeof window.attachEvent != "undefined" ) {
	    window.attachEvent( "onload", fnc );
	  }
	  else {
	    if ( window.onload != null ) {
	      var oldOnload = window.onload;
	      window.onload = function ( e ) {
	        oldOnload( e );
	        window[fnc]();
	      };
	    }
	    else
	      window.onload = fnc;
	  }
	}
function changcss(){
//	document.body.style.backgroundImage = "url(http://news.tennis365.net/news/img/bg_wim.jpg)";
//	document.body.style.backgroundImage = "url(http://news.tennis365.net/news/img/bg_win2014.jpg)";
//	document.body.style.backgroundImage = "url(http://news.tennis365.net/news/img/bg_ao.jpg)";
    document.body.style.backgroundRepeat = "no-repeat";
	document.body.style.backgroundPosition = "center top";
}
//addOnloadEvent(changcss);
</script>
