                url: "/users/sendtype/",
                type: "POST",
                //    dataType: 'json', //jsonã§å–å¾—ã™ã‚‹å ´åˆã¯å¿…é ˆ
                data: data
            }).done(function(res){
//                alert('success!!');
            }).fail(function(res){
//                alert('error!!!');
            });

        //    $('form').submit();//ãƒ•ã‚©ãƒ¼ãƒ é€ä¿¡
        });

