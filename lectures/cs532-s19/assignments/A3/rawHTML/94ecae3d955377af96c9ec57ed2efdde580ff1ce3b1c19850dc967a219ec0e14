<script>

    $(function () {

        $('#re-body').bind('keyup',function(){
            message = $(this).val();
            var ct = message.replace(/\s+/g,'').length; // â†â˜…replace()ã‚’è¿½åŠ 

            emoji_ct = (message.match(/[\uD800-\uDBFF][\uDC00-\uDFFF]/g)||[]).length;

            ct = ct-emoji_ct;
            $("#text-count").html(ct);

            if(ct > 100){

                $(".mery-btn").addClass("disabled");

            } else {
                $(".mery-btn").removeClass("disabled");

            }


        });


        //$('#modal-dummy').click(); //è¦ç´ ã‚’ã‚¯ãƒªãƒƒã‚¯ã•ã›ã‚‹
        //å‹•çš„è¦ç´ ã«å¯¾å¿œ keyup change
