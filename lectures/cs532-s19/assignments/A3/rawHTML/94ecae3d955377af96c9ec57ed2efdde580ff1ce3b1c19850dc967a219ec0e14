                        $("#sidr").addClass("touchscroll");

                        $("#overlay").fadeIn();//„É°„Ç§„É≥„Çí„ÇØ„É™„ÉÉ„ÇØ„Åß„Åç„Å™„ÅÑ„Çà„ÅÜ„Å´„Åô„Çã
