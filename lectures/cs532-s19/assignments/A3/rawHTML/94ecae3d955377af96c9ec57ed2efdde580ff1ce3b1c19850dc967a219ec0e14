<script>

    $(function () {

        $('#re-body').bind('keyup',function(){
            message = $(this).val();
            var ct = message.replace(/\s+/g,'').length; // ‚Üê‚òÖreplace()„ÇíËøΩÂä†

            emoji_ct = (message.match(/[\uD800-\uDBFF][\uDC00-\uDFFF]/g)||[]).length;

            ct = ct-emoji_ct;
            $("#text-count").html(ct);

            if(ct > 100){

                $(".mery-btn").addClass("disabled");

            } else {
                $(".mery-btn").removeClass("disabled");

            }


        });


        //$('#modal-dummy').click(); //Ë¶ÅÁ¥†„Çí„ÇØ„É™„ÉÉ„ÇØ„Åï„Åõ„Çã
        //ÂãïÁöÑË¶ÅÁ¥†„Å´ÂØæÂøú keyup change
        $(document).on('change', '#send-type', function() {

            var data = {send_type:$(this).prop('checked')};

//            alert($(this).prop('checked'));
//
            $.ajax({
                url: "/users/sendtype/",
                type: "POST",
                //    dataType: 'json', //json„ÅßÂèñÂæó„Åô„ÇãÂ†¥Âêà„ÅØÂøÖÈ†à
                data: data
            }).done(function(res){
//                alert('success!!');
            }).fail(function(res){
//                alert('error!!!');
            });

        //    $('form').submit();//„Éï„Ç©„Éº„É†ÈÄÅ‰ø°
        });

        $(document).on('click', '#kidoku-btn', function() {

            var data = {
                id:"4059491"
            };

            $.ajax({
                url: "/mes/kidoku/",
                type: "POST",
                //    dataType: 'json', //json„ÅßÂèñÂæó„Åô„ÇãÂ†¥Âêà„ÅØÂøÖÈ†à
                data: data
            }).done(function(res){

                $("#kidoku-btn").prop("disabled",true);
                $("#kidoku-btn").html('<i class="fa fa-envelope-open" aria-hidden="true"></i> Ëøî‰ø°Ê∏à„Åø');


            }).fail(function(res){
                // alert('error!!!');
            });


        });

    });

</script>


