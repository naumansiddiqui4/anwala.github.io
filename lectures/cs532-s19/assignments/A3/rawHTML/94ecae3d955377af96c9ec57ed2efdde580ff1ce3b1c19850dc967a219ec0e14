        $(function () {

            var url = $.url();
            soutai_link = url.attr('path');
            $('a[href="'+soutai_link+'"]').addClass('active');






            //ÁµåÈÅéÊôÇÈñì„ÇíË™ø„Åπ„Çã
            function parseDiffTime(date){

                keika = [];
                moment.locale('ja');
                var now = moment();


                //ios„Å†„Å®„Éê„Ç∞„Çã„Çã„ÅÆ„Åß„ÄÅ „Çπ„É©„ÉÉ„Ç∑„É•Âå∫Âàá„Çä„Å´Â§âÊèõ


                tmp_date = date.replace( /-/g , "/" );
                date = moment(new Date(tmp_date)).format("YYYY/MM/DD HH:mm:ss");


                keika['minute'] = parseInt(now.diff(date, 'minute'));
                keika['hour'] = parseInt(now.diff(date, 'hour'));
                keika['day'] = parseInt(now.diff(date, 'day'));
                keika['month'] = parseInt(now.diff(date, 'month'));
//
                diff_time = "‰ªä";

                if(keika['minute'] != 0){
                    diff_time = keika['minute'] + " ÂàÜÂâç";
                }

                if(keika['hour'] != 0){
                    diff_time = keika['hour'] + " ÊôÇÈñìÂâç";
                }

                if(keika['day'] != 0){
                    diff_time = keika['day'] + " Êó•Ââç";
                }

                if(keika['month'] != 0){
                    diff_time = keika['month'] + "„É∂ÊúàÂâç";
                }

                if(keika['month'] > 6){
                    diff_time = "ÂçäÂπ¥‰ª•‰∏äÂâç";
                }


                return diff_time;
            }


//            „Ç´„É≥„ÉûÂå∫Âàá„Çä„Å´„Åô„Çã
            function addFigure(str) {
                var num = new String(str).replace(/,/g, "");
                while(num != (num = num.replace(/^(-?\d+)(\d{3})/, "$1,$2")));
                return num;
            }


            //ÁµåÈÅéÊôÇÈñì„ÇíË™ø„Åπ„Çã
            function parseRealDiffTime(date){

                keika = [];
                moment.locale('ja');
                var now = moment();


                //ios„Å†„Å®„Éê„Ç∞„Çã„Çã„ÅÆ„Åß„ÄÅ „Çπ„É©„ÉÉ„Ç∑„É•Âå∫Âàá„Çä„Å´Â§âÊèõ


                tmp_date = date.replace( /-/g , "/" );
                date = moment(new Date(tmp_date)).format("YYYY/MM/DD HH:mm:ss");

                keika['day'] =  addFigure(parseInt(now.diff(date, 'day')));

//
//                diff_time = "‰ªä";
//
//                if(keika['minute'] != 0){
//                    diff_time = keika['minute'] + " ÂàÜÂâç";
//                }
//
//                if(keika['hour'] != 0){
//                    diff_time = keika['hour'] + " ÊôÇÈñìÂâç";
//                }
//
//                if(keika['day'] != 0){
//                    diff_time = keika['day'] + " Êó•Ââç";
//                }
//
//                if(keika['month'] != 0){
//                    diff_time = keika['month'] + "„É∂ÊúàÂâç";
//                }
//
//                if(keika['month'] > 6){
//                    diff_time = "ÂçäÂπ¥‰ª•‰∏äÂâç";
//                }


                diff_time = keika['day'] + "Êó•";

                return diff_time;
            }

            $.each($('.moment'),
                function(i, v) {
                    $(this).html(parseDiffTime($(this).text()));
                }
            );


            $.each($('.real-moment'),
                function(i, v) {
                    $(this).html(parseRealDiffTime($(this).text()));
                }
            );


            if($(".message").html()){
                notify_type = 'success';
                if($(".message").html().match(/Error/g)){
                    notify_type = "error";
                }


                mes = $(".message").html().replace(/\n/g, "<br>");

                $.notify(mes,notify_type);
            }

        });



    </script>


