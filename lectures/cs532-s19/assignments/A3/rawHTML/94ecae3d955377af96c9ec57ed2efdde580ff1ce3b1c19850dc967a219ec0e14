            //çµŒéæ™‚é–“ã‚’èª¿ã¹ã‚‹
            function parseDiffTime(date){

                keika = [];
                moment.locale('ja');
                var now = moment();


                //iosã ã¨ãƒã‚°ã‚‹ã‚‹ã®ã§ã€ ã‚¹ãƒ©ãƒƒã‚·ãƒ¥åŒºåˆ‡ã‚Šã«å¤‰æ›


                tmp_date = date.replace( /-/g , "/" );
                date = moment(new Date(tmp_date)).format("YYYY/MM/DD HH:mm:ss");


                keika['minute'] = parseInt(now.diff(date, 'minute'));
                keika['hour'] = parseInt(now.diff(date, 'hour'));
                keika['day'] = parseInt(now.diff(date, 'day'));
                keika['month'] = parseInt(now.diff(date, 'month'));
//
                diff_time = "ä»Š";

                if(keika['minute'] != 0){
                    diff_time = keika['minute'] + " åˆ†å‰";
                }

                if(keika['hour'] != 0){
                    diff_time = keika['hour'] + " æ™‚é–“å‰";
                }

                if(keika['day'] != 0){
                    diff_time = keika['day'] + " æ—¥å‰";
                }

                if(keika['month'] != 0){
                    diff_time = keika['month'] + "ãƒ¶æœˆå‰";
                }

                if(keika['month'] > 6){
                    diff_time = "åŠå¹´ä»¥ä¸Šå‰";
                }


                return diff_time;
            }


//            ã‚«ãƒ³ãƒåŒºåˆ‡ã‚Šã«ã™ã‚‹
            function addFigure(str) {
                var num = new String(str).replace(/,/g, "");
                while(num != (num = num.replace(/^(-?\d+)(\d{3})/, "$1,$2")));
                return num;
            }


            //çµŒéæ™‚é–“ã‚’èª¿ã¹ã‚‹
            function parseRealDiffTime(date){

                keika = [];
                moment.locale('ja');
                var now = moment();


                //iosã ã¨ãƒã‚°ã‚‹ã‚‹ã®ã§ã€ ã‚¹ãƒ©ãƒƒã‚·ãƒ¥åŒºåˆ‡ã‚Šã«å¤‰æ›


                tmp_date = date.replace( /-/g , "/" );
                date = moment(new Date(tmp_date)).format("YYYY/MM/DD HH:mm:ss");

                keika['day'] =  addFigure(parseInt(now.diff(date, 'day')));

//
//                diff_time = "ä»Š";
//
//                if(keika['minute'] != 0){
//                    diff_time = keika['minute'] + " åˆ†å‰";
//                }
//
//                if(keika['hour'] != 0){
//                    diff_time = keika['hour'] + " æ™‚é–“å‰";
//                }
//
//                if(keika['day'] != 0){
//                    diff_time = keika['day'] + " æ—¥å‰";
//                }
//
//                if(keika['month'] != 0){
//                    diff_time = keika['month'] + "ãƒ¶æœˆå‰";
//                }
//
//                if(keika['month'] > 6){
//                    diff_time = "åŠå¹´ä»¥ä¸Šå‰";
//                }


                diff_time = keika['day'] + "æ—¥";

                return diff_time;
            }

            $.each($('.moment'),
                function(i, v) {
                    $(this).html(parseDiffTime($(this).text()));
                }
            );


            $.each($('.real-moment'),
                function(i, v) {
                    $(this).html(parseRealDiffTime($(this).text()));
                }
            );


            if($(".message").html()){
                notify_type = 'success';
                if($(".message").html().match(/Error/g)){
                    notify_type = "error";
                }


                mes = $(".message").html().replace(/\n/g, "<br>");

                $.notify(mes,notify_type);
            }

        });



