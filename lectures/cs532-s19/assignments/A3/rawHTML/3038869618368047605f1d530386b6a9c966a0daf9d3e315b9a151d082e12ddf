<script type="text/javascript">
	$(document).ready(function() {

		$('#gnb li a img').mouseover(function(){
			var gnbname = $(this).attr('src');
			$(this).attr('src', gnbname.replace('_off.gif','_on.gif'));
		});
		$('#gnb li a img').mouseout(function(){
			var gnbname = $(this).attr('src');
			if ($(this).attr('id') != '0000')
			{
				$(this).attr('src', gnbname.replace('_on.gif','_off.gif'));
			}
		});




		$('#lnb li:not(.on, #lnb li ol li) img').mouseover(function(){
			var filename = $(this).attr('src');
			$(this).attr('src',filename.replace('_off.gif','_on.gif'));
		});

		$('#lnb li:not(.on, .depth) img').mouseout(function(){
			var filename = $(this).attr('src');
			if ($(this).attr('id') != '0000' && $(this).attr('id') != '0000')
			{
				$(this).attr('src',filename.replace('_on.gif','_off.gif'));
			}
		});

		$('#lnb a').click(function(){
			var a = $(this).attr('href');
			if (a.length > 0)
			{
				location.href = a;
			}
		});
	});
</script>



