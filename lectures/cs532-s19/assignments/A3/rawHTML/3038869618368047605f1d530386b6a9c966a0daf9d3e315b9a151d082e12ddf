<script type="text/javascript">

jQuery(document).ready(function() {

        var summaries = $('.btnArea');
        summaries.each(function(i) {
            var summary = $(summaries[i]);
            var next = summaries[i + 1];

            summary.scrollToFixed({
                marginTop: $('#board_campaign_topmenu').outerHeight(true) ,
                limit: function() {
                    var limit = 0;
                    if (next) {
                        limit = $(next).offset().top - $(this).outerHeight(true)+1000;
                    } else {
                        limit = $('.footer').offset().top - $(this).outerHeight(true)+1000;
                    }
                    return limit;
                },
                zIndex: 999
            });
        });



	slider = $('.bxslider').bxSlider( {
		mode: 'fade',
		captions: false,
		keyboardEnabled: true,
		auto : true,
		autoStart : false,
		autoControls: true
	});

});

</script>

