  <script async src="https://www.googletagservices.com/tag/js/gpt.js"></script> 
  <script type="text/javascript" async src="https://cherry-brightspot.s3.amazonaws.com/resource/assets/uploaded/prebid/prebid1.37.0.js"></script> 
  <script type="text/javascript">
      var googletag = googletag || {};
      googletag.cmd = googletag.cmd || [];

  

        var pbjs = pbjs || [];
        pbjs.que = pbjs.que || [];

        var PREBID_TIMEOUT = 3000;
        var FAILSAFE_TIMEOUT = 1500;
        var adUnits = [];

        window.addEventListener("load", function() {

             const adElts = document.getElementsByClassName('GoogleDfpAd');

              for (var i = 0; i < adElts.length; ++i) {
                const elt = adElts.item(i);

                if (elt.dataset.bidders && elt.dataset.slotSizes) {
                  adUnits.push({
                    code: elt.id,
                    mediaTypes : {
                      banner:{
                        sizes: JSON.parse(elt.dataset.slotSizes)
                      }
                    },
                    bids: JSON.parse(elt.dataset.bidders)
                  })
                }
              }

              adUnits.map(function (el) {
                 el.mediaTypes.banner.sizes = el.mediaTypes.banner.sizes.filter(function (s) {
                    return typeof s != "string" && (s[0] != 0 && s[1] != 0)
                  })
                  .map(function (s) {
                    return s.join(",")
                  })
                  .filter(function (v, i, a) {
                    return a.indexOf(v) === i
                  })
                  .map(function (s) {
                    return s.split(",").map(function (e) {
                      return parseInt(e, 10)
                    })
                  });
                return el
              });

              function sendAdserverRequest() {
                if (pbjs.adserverRequestSent) {
                  return;
                }

                googletag.cmd.push(function () {
                  pbjs.que.push(function () {
                    pbjs.setTargetingForGPTAsync();
                    googletag.pubads().refresh();
                      
                  });
                });
                pbjs.adserverRequestSent = true;
              }


          pbjs.que.push(function () {
            pbjs.addAdUnits(adUnits);
            pbjs.setConfig({
              debug: true,
              priceGranularity: "dense",
              enableSendAllBids: true
            });



            pbjs.requestBids({
              bidsBackHandler: sendAdserverRequest,
              timeout: PREBID_TIMEOUT
            })
          });

        setTimeout(function() {
            sendAdserverRequest();
          }, FAILSAFE_TIMEOUT);

       },false);

        googletag.cmd.push(function () {
          googletag.pubads().collapseEmptyDivs();
          googletag.pubads().disableInitialLoad();
          googletag.pubads().enableSingleRequest();
          googletag.pubads().enableVideoAds();
          googletag.companionAds().setRefreshUnfilledSlots(false);
          googletag.enableServices()
        });


   

</script> 
