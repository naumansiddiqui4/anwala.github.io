<script type="text/javascript">
$(document).ready(function() {
  
  var errModal = $("#_errModal");
  var postModal = $("#_Modal");
  if ($(errModal).length) {
    $(errModal).modal();
  } else if ($(postModal).length) {
    var m = '';
    if( m == 'Youtube' ){
      $(postModal).modal({opacity:98, overlayId:'simplemodal-overlay-dark', containerId:'simplemodal-container-dark', maxWidth:500, onClose: function() {
        $.modal.close();
        window.setTimeout(function() {$('#popupLoading').modal();}, 300);
      }});
    } else {
      $(postModal).modal({onClose: function() {
        $.modal.close();
        window.setTimeout(function() {$('#popupLoading').modal();}, 300);
      }});
    }
  }
  
});
</script>




<script type="text/javascript">
  // バナークリック時
  function clickCount(func){
    if( func ){
      var comp = 'phone1902';
      var type = 'pc';
      click_count( comp, func, type );
    }
  }
</script>

