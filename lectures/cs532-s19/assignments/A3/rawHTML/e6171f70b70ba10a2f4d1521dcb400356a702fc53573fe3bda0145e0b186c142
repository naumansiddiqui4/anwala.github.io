    var exclude_cat_filter = ['mn.entertainment.geinou','mn.hobby.model','mn.life.health','mn.life.ed','mn.life.workstyle'];
    if (exclude_cat_filter.indexOf(cat_target) > -1) {
      // ng-cat
      if(news_vars.adParameter instanceof Array && /ok/.test(news_vars.adParameter[0])) {
        googletag.pubads().setTargeting('ads', ['ok']);
      } else {
        googletag.pubads().setTargeting('ads', ['ng']);
      }
    } else {
      // ok-cat
      if(news_vars.adParameter instanceof Array && /ng/.test(news_vars.adParameter[0])) {
        googletag.pubads().setTargeting('ads', ['ng']);
      } else {
        googletag.pubads().setTargeting('ads', ['ok']);
      }
    }
    googletag.pubads().disableInitialLoad();
    googletag.pubads().enableSingleRequest();
    googletag.pubads().collapseEmptyDivs();
    googletag.enableServices();
  });  
}]);
}
</script>

