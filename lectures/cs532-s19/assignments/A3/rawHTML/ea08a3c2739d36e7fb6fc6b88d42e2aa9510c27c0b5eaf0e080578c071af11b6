    slotID: 'div-gpt-ad-1506649784091-0',
    slotName: '/43042345/nlab_belowART_left',
    sizes: [[336, 280], [300, 250]]
},
{
    slotID: 'div-gpt-ad-1506997124240-0',
    slotName: '/43042345/nlab_belowART_right',
    sizes: [[300, 250]]
