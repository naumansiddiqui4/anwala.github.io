<script>
function article_id(){
	var d = document;
	var str = false;
	var my_url = d.URL;
	var split_url;
	var article_url;
	var yymm;
	var dd;
	var news;

	// y[W^Cv»è
	// largeimage
	if(my_url.indexOf('image.' + 'itmedia.co.jp') && d.getElementById('pcvurl')){
		article_url = d.getElementById('pcvurl').content;

	// article|summary|reference
	}else{
		article_url = my_url;
	}
	split_url = article_url.replace(/(\?|#).*$/,'').split('/');
	yymm = split_url[5]; // 1612
	dd = split_url[6]; // 12
	news = split_url[7].replace(/(news\d{2,3})(\_\d{1,3})?\.html/,'$1'); // 001
	str = yymm + '/' + dd + '/' + news; // 1612/12/001
	return str;
};

(function(d){
	var my_art_id = article_id();
	// var my_art_url = thisDomain() + getUrl()['path_cutparam'];
	var pos_cnt = d.getElementById('masterSocialbuttonTop').getElementsByTagName('tr')[0];
	var my_cnt = d.createElement('td');
	var ref_url = getUrl().path_cutpageing_cutparam.replace('.html','_00.html');
	my_cnt.id = 'msbBtnCommentTop';
	my_cnt.className = 'msbBtn msbBtnComment';
	my_cnt.innerHTML = '<div id="reflink"><a href="' + ref_url + '"><div class="comment_counter" style="margin-right:3px;font-family:Gill Sans, \'CI\', Meiryo;"></div><div class="comment_btn">Rg</div></a></div>';
	pos_cnt.appendChild(my_cnt);

/*--WIDGET--*/
	var _ul_comment_config = _ul_comment_config||{};
	_ul_comment_config['id'] = 'CM-M6vRUXvV';
	_ul_comment_config['article_id'] = my_art_id;
	// _ul_comment_config['url'] = my_art_url;
	_ul_comment_config['author'] = '';
	_ul_comment_config['published_at'] = '';
	(function (c, n, s) {if (c[n] === void 0) {c['ULObject'] = n;
	c[n] = c[n] || function () {(c[n].q = c[n].q || []).push(arguments)};
	c[n].l = 1 * new Date();var e = d.createElement('script');e.async = 1;
	e.src = s + "/comment.js";var t = d.getElementsByTagName('script')[0];
	t.parentNode.insertBefore(e, t);}})
	(window, 'ul_comment_widget', 'https://cm-widget.nakanohito.jp/cm');
	ul_comment_widget('init', _ul_comment_config);
/*--/WIDGET--*/

/*--COUNTER--*/
	window.addEventListener('DOMContentLoaded',function(){
	var e_com_count = d.getElementsByClassName('comment_counter');
	var api_hash = [my_art_id];
	var xhr = new XMLHttpRequest();
	xhr.onreadystatechange = function(){
		if(this.readyState == 4 && this.status == 200){
			if(this.response){
				for(var i = 0; i < e_com_count.length; i++){
					e_com_count[i].innerHTML = this.response.data[api_hash];
				}
				console.log(this.response);
			}
		}
	};
	xhr.open('GET','https://cm-beacon.nakanohito.jp/cm/comment_count/?group_id=CM-M6vRUXvV&article_ids=' + encodeURIComponent(api_hash),true);
	xhr.responseType = 'json';
	xhr.send(null);
	},false);
/*--/COUNTER--*/

})(document);
</script>

