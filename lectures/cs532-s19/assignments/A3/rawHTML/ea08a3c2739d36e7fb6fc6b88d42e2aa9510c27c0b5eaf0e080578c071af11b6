var fb_callback = function(action,url,html_element) {
	if (html_element.parentNode.id == 'thirdPartyWidget') {
		var test_id = 'rm44215_FB_main_' + action;
		cx_click_test(test_id);
	} else if (html_element.parentNode.className == 'colBoxShareboxBtnFb') {
		var test_id = 'rm44215_FB_main_' + action;
		cx_click_test(test_id);
	} else if (html_element.parentNode.className.match(/colBoxFollowBtnFacebook/)) {
		var test_id = 'rm44215_FB_sub_' + action;
		cx_click_test(test_id);
	}
};
var interval = setInterval(function(){
	if(typeof FB !== 'undefined'){
		FB.Event.subscribe('edge.create',fb_callback.bind(null,'like'));
		FB.Event.subscribe('edge.remove',fb_callback.bind(null,'remove'));
		clearInterval(interval);
	}
},500);
