<script>
  (function($) {
    "use strict";

    function mkFormatter(num) {
      if (num > 999999) {
        return parseFloat((num/1000000).toFixed(1)) + 'm'
      } else if (num > 999) {
        return parseFloat((num/1000).toFixed(1)) + 'k'
      } else {
        return num;
      }
    }

    var getArticleShares = function() {
      $.ajax({
        url: 'https://api.parsely.com/v2/shares/post/detail',
        jsonp: 'callback',
        dataType: 'jsonp',
        data: {
          'apikey': 'dailywire.com',
          'url': location.href,
        },
        success: function(response) {
          var shares = response.data[0];
          var fb = !shares || shares.fb < 10 ? 0 : mkFormatter(shares.fb);
          var tw = !shares || shares.tw < 10 ? 0 : mkFormatter(shares.tw);
          $('.share .share-facebook a').addClass(!fb ? 'in no-stats' : 'in stats').find('small').text(fb);
          $('.share .share-twitter a').addClass(!tw ? 'in no-stats' : 'in stats').find('small').text(tw);
        }
      });
    };

    var getArticleViews = function() {
      $.ajax({
        url: 'https://api.parsely.com/v2/analytics/post/detail',
        jsonp: 'callback',
        dataType: 'jsonp',
        data: {
          'apikey': 'dailywire.com',
          'url': location.href,
          'period_start': '90d',
        },
        success: function(response) {
          var postDetail = response.data[0];
          if (postDetail && postDetail.metrics.views) {
            var views = mkFormatter(postDetail.metrics.views); // test/tmp
            $('.view-count').removeClass('hide').find('span').text(views);
          }
        }
      });
    };

    $(window).on('parsely_ready', function(){
      getArticleShares();
      if ($('.article-full .view-count').length) {
        getArticleViews();
      }
    });

  })(jQuery);
</script>
</div>
<div class="sidebar fx-s-0-l">
<section class="hotwire"></section>
<script id="hotwire-template" type="text/x-handlebars-template">
  <section class="article-hot-wires mb-4-ns">
    <h3 class="block-title">Hot Wire</h3>
    {{#each articles}}
      <article class="article-hot-wire fx pos-r pb-3 pb-2-l">
        <div class="media as-fs beaufort-heavy f-min pos-r fx-s-0 mr-2 mr-3-m">
          <a href="{{ url }}">
            <img src="{{ metadata.image_style_urls.169medium }}" alt="{{ title }}" />
          </a>
        </div>
        <div class="as-c">
          <h3 class="f-5 f-sm-l mb-1"><a href="{{ url }}">{{ title }}</a></h3>
          {{#each authors}}
            <address class="author">
              <span class="label">By</span> {{ this }}
            </address>
          {{/each}}
        </div>
      </article>
    {{/each}}
  </section>
</script>
<script>
  (function($, Drupal) {
    "use strict";

    var source   = $("script#hotwire-template").html();
    var template = Handlebars.compile(source);

    var cleanUpArticles = function(articles){
      var cleaned = [];
      for (var i in articles){
        articles[i].metadata = JSON.parse(articles[i].metadata);
        cleaned.push(articles[i]);
      }
      return cleaned;
    };

    $.ajax({
      url: 'https://api.parsely.com/v2/analytics/posts',
      jsonp: 'callback',
      dataType: 'jsonp',
      data: {
        'apikey': 'dailywire.com',
        'pub_date_start': '1w', // 90 days
        'limit': 5,
        'page': 1,
        'section': 'News',
        'sort': 'views',
        'period_start': '30m'
      },
      success: function(response) {
        var articles = cleanUpArticles(response.data);
        console.log(articles);
        $('.hotwire').append(template({ articles: articles }));
      }
    });

  })(jQuery, Drupal);
</script>
