  /**** 無理矢理(1) スマフォでGoogleフォームがはみ出すのを矯正 ****/
  jQuery(window).resize(function() {
    jQuery('iframe').width(jQuery(window).width());
  });
  jQuery('iframe').css("width", "100% !important");

  /**** 無理矢理(2) 日本語化 ****/
  jQuery('.mk-readmore').each(function(){
    var elms = jQuery(this).get(0).childNodes;
    for (var i=0; i<elms.length ;i++){
      if ( elms[i].nodeType == Node.TEXT_NODE ) {
        if ( elms[i].nodeValue == "Read More" ) {
          elms[i].nodeValue = "続きを読む";
        }
      }
    }
  });
  jQuery('.page-introduce-subtitle').each(function(){
    var txt = jQuery(this).text();
    if ( txt==='Category Archive for: "ブログ"' ){
      jQuery(this).text('記事のカテゴリー：ブログ');
    } else if ( txt==='Category Archive for: "イベント"' ) {
      jQuery(this).text('記事のカテゴリー：イベント');
    }
  });

  /**** 無理矢理(3) 幅狭画面で月額プラン料金表を全画面表示できるように。 ****/
  jQuery("#monthly-plan-wrapper").click(function(){
   if( jQuery(window).width() <= 600 ){
    var table = jQuery("table#monthly-plan");
    var wrapper = table.parent();
    var scr = jQuery("<div/>")
        .css("height","100%")
        .css("width","100%")
        .css("position","fixed")
        .css("top","0px")
        .css("left","0px")
        .css("overflow-x","auto")
        .css("z-index","600")
        .css("background-color","white");
    var btn = jQuery("<button/>")
        .text("戻る")
        .click(function(){
          wrapper.append(table);
          scr.remove();
        });
    scr.append(btn).append(table);
    jQuery("html").append(scr);
   }
  });

});</script> <script type="text/javascript">window.get = {};	window.get.captcha = function(enteredCaptcha) {
