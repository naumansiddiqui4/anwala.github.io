<script type="text/javascript">
$(window).load(function(){
	/* 年月日取得 */
	objDate = new Date();
	var y = objDate.getFullYear();
	var m = objDate.getMonth()+1;
	m = ('0'+ m).slice(-2);
	var d = objDate.getDate();
	d = ('0'+ d).slice(-2);
	var famtime1 = y+'/'+m+'/'+d;
	/* 年月日を表示 */
	$('.fam_thumb_left a div div').after('<p class="fam_time">'+famtime1+'</p>');
});
</script>
