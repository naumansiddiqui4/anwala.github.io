<script type="text/javascript">
//<![CDATA[
$(function() {
	$("#tab li").click(function() {
		var num = $("#tab li").index(this);
		$(".content_wrap").addClass('disnon');
		$(".content_wrap").eq(num).removeClass('disnon');
		$("#tab li").removeClass('select');
		$(this).addClass('select')
	});
});
//]]>

</script>

<script type="text/javascript">
jQuery(document).ready(function() {
    var nav = jQuery('#top_menu');
    var navTop = nav.offset().top;
    jQuery(window).scroll(function () {
      var winTop = jQuery(this).scrollTop();
      if (winTop >= navTop) {
        nav.addClass('fixed01')
      } else if (winTop <= navTop) {
        nav.removeClass('fixed01')
      }
    });
});
</script>

<script type="text/javascript">
 jQuery(document).ready(function() {
    var nav2 = jQuery('#cate_menu');
    var navTop2 = nav2.offset().top;
    jQuery(window).scroll(function () {
      var winTop2 = jQuery(this).scrollTop();
      if (winTop2 + 101 >= navTop2) {
        nav2.addClass('fixed02')
      } else if (winTop2 + 101　<= navTop2) {
        nav2.removeClass('fixed02')
      }
    });
});
</script>

<script type="text/javascript">
$(function() {
	//クリックしたときのファンクションをまとめて指定
	$('.footer_tab li').click(function() {

		//.index()を使いクリックされたタブが何番目かを調べ、
		//indexという変数に代入します。
		var index = $('.footer_tab li').index(this);

		//コンテンツを一度すべて非表示にし、
		$('.footer_tab_content li').css('display','none');

		//クリックされたタブと同じ順番のコンテンツを表示します。
		$('.footer_tab_content li').eq(index).css('display','block');

		//一度タブについているクラスselectを消し、
		$('.footer_tab li').removeClass('select');

		//クリックされたタブのみにクラスselectをつけます。
		$(this).addClass('select');
	});
});
</script>

<script type="text/javascript">
$(function() {
	//クリックしたときのファンクションをまとめて指定
	$('.footer_tab2 li').click(function() {

		//.index()を使いクリックされたタブが何番目かを調べ、
		//indexという変数に代入します。
		var index = $('.footer_tab2 li').index(this);

		//コンテンツを一度すべて非表示にし、
		$('.footer_tab_content2 .footer_tab_content2_inn').css('display','none');

		//クリックされたタブと同じ順番のコンテンツを表示します。
		$('.footer_tab_content2 .footer_tab_content2_inn').eq(index).css('display','block');

		//一度タブについているクラスselectを消し、
		$('.footer_tab2 li').removeClass('select');

		//クリックされたタブのみにクラスselectをつけます。
		$(this).addClass('select');
	});
});
</script>

