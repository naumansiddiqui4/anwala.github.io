<script type="text/javascript">
(function(){
  // byline 광고(기사하단 5줄 광고)
  var adByline = '<div id="AD_LIVE_AREA_21544480" style="height:150px; position:relative;left:0px;top:0px;"><iframe src="https://cdn.new-star.co.kr/data/adnew/2154/4480/adtext_2154_4480.html" frameborder="0" width="100%" height="150" scrolling="no"></iframe></div>';
  $(".article-body.read").append(adByline);
  // /byline 광고(기사하단 5줄 광고)

  // 기사내 광고 4, 12, 20 번째 문단에 삽입.
  var adLoaderFirst = '<iframe frameborder="0" marginwidth="0" marginheight="0" scrolling="NO" width="300" height="250" src="/ad/ad-nth-1-pc.html" allowtransparency="true" style="display:block; margin:0 -1em 10px 0; float:right;"></iframe>';
  var adLoaderSecond = '<iframe frameborder="0" marginwidth="0" marginheight="0" scrolling="NO" width="300" height="250" src="/ad/ad-nth-2-pc.html" allowtransparency="true" style="display:block; margin:0 -1em 10px 0; float:right"></iframe>';
  var adLoaderThird = '<iframe frameborder="0" marginwidth="0" marginheight="0" scrolling="NO" width="300" height="250" src="/ad/ad-nth-3-pc.html" allowtransparency="true" style="display:block; margin:0 -1em 10px 0;  float:right"></iframe>';

  var paragraphLength = $(".article-body p").length;

  if ( paragraphLength >= 8 ) {
    $(".article-body p:eq(4)").after(adLoaderFirst);
  }
  if ( paragraphLength >= 16 ) {
    $(".article-body p:eq(12)").after(adLoaderSecond);
  }
  if ( paragraphLength >= 24 ) {
    $(".article-body p:eq(20)").after(adLoaderThird);
  }
  // /기사내 광고 4, 12, 20 번째 문단에 삽입.

  var adArticleListBlock1 = '<div id="taboola-below-article-thumbnails"></div>'

  // 기사 하단 광고 첫번째 (taboola)
  $("#dpArticleBlocksWrapper .articleBlock:eq(0)").html( adArticleListBlock1 );

  window._taboola = window._taboola || [];
  _taboola.push({
    mode: 'alternating-thumbnails-a',
    container: 'taboola-below-article-thumbnails',
    placement: 'Below Article Thumbnails',
    target_type: 'mix'
  });


})();
</script>

