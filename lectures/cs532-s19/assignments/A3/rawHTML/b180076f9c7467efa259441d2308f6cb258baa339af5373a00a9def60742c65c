<script type="text/javascript">
var photoad_onclick = function(oAdid, adLanding){


__dispatch.push(['event', 'axfkoreaPC']);
ga('send', 'event', 'clickAD', 'axfkoreaPC', ( ( location && location.pathname ) ? location.pathname : '' ));
// __sauron.push(['event', 'axfkoreaPC']);

  // __dispatch.push(['event', 'clickAD|' + oAdid]);
  // wcs.event('clickAD', oAdid);
  // ga('send', 'event', 'clickAD', oAdid, ( ( location && location.pathname ) ? location.pathname : '' ));

  window.open(adLanding);
  //location.href = adLanding;
};
/* 아이콘 광고 */
(function(){
  var padding = {  //아이콘의 이미지 우단, 하단 에 대한 상대적 간격
    right:5, top:5
  }, getAd = function(){
    var imageIconAds = [{ // 광고 추가시 배열에 아래 형식에 맞춰 추가할것.
      adid:'PhotoAD-dispatch-dispatchapp',
      landing:'https://www.axf-korea.com/shop/axf-%EB%AA%A9%EA%B1%B8%EC%9D%B4%EA%B8%B4-%EB%B0%98%EC%82%AC%ED%8C%90/',
      icon:'<img src="https://dispatch.cdnser.be/cms-content/uploads/2019/02/15/47886564-fb2a-454b-992c-e825be40f44b.gif" style="width:60px; height:60px;"/>'
    }, {
      adid:'PhotoAD-dispatch-ildong',
      landing:'http://www.meditouch.kr/main.do',
      icon:'<img src="https://dispatch.cdnser.be/ad/photoad/medipatch-170905-1.gif" style="width:60px; height:60px;" />'
    }], adWho = (dpGetRandIntRange(1, 10) <= 7) ? 0 : 0; /* 7:3 비율로 0번 혹은 1번 광고*/
    return imageIconAds[adWho];
  }, adjustIcons = function(){
    var imageLoaded = function(_this){
      try{
        var $this = $(_this), $parent = $this.parent(), selectedAd = getAd(), clsid=".abc" + ($this.attr("src") + $this.index()).replace(/[^a-zA-Z0-9]/g,""), $el = null;
        if($parent.hasClass("iconAd")) return;
        if($(clsid).length > 0){
          $el = $(clsid);
        }else{
          $el = $this.after($('<div class="abc ' + clsid.replace(".", "") + '" style="position:absolute;"><a onclick="photoad_onclick(\'' + selectedAd.adid + '\', \'' + selectedAd.landing + '\');" class="iconAd">' + selectedAd.icon + '</a></div>'));  // 2. 이후 생성
        }
        //console.log($el);
        var iconTop = $this.offset().top + $this.height() - $parent.find(clsid + ":eq(0) img").height() - padding.right;
        var iconLeft = $this.offset().left + $this.width() - $parent.find(clsid + ":eq(0) img").width() - padding.top;
        $parent.find(clsid + ":eq(0)").css({
          "top": iconTop + "px",
          "left":iconLeft + "px"
        });
      }catch(e){
        console.log("exception:", e);
      }
    };

    $("section.column.read img").each( function(){
      if(this.complete){
        var _this = this;
        setTimeout(function(){imageLoaded(_this);}, 0);
      }
    });
  };

  $(document).ready(adjustIcons); // 맨처음 페이지 로딩시 아이콘 배열
  window.onload = adjustIcons;  //
  $(window).resize(adjustIcons);  // 윈도우 리사이즈시 아이콘 배열
})();
/* /아이콘 광고 */
</script>


