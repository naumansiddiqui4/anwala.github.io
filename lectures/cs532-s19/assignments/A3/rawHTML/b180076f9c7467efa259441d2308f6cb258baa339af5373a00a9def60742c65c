<script type="text/javascript">
// Hamburger menu (side menu)
(function(){
  var toggleMobileModalSideMenu = function(){
    var $toggleBtn = $(".mobileModalSideMenuToggler");

    var getState = function(){
      return $toggleBtn.hasClass("on") ? "on" : "off";
    },
    toggle = function(){
      var todo = getState() == "on" ? "off" : "on";
      $toggleBtn.removeClass("on off");
      $toggleBtn.addClass(todo);
      return todo;
    },
    showMenu = function(){
      $sideMenu.addClass("on");
      $modalOverlay.addClass("on");
    },
    hideMenu = function(){
      $sideMenu.addClass("off");
      $modalOverlay.addClass("off");
    };

    $modalOverlay.removeClass("init on off");
    $sideMenu.removeClass('init on off');
    if(toggle() == "on"){
      showMenu();
    }else{
      hideMenu();
    }
  },
  modalOverlay = "<div id='modalOverlay'></div>",
  $modalOverlay = null,
  $sideMenu = $("#mobileModalSideMenu");

  $(".mobileModalSideMenuToggler").on("click", toggleMobileModalSideMenu);
  $("body").append(modalOverlay);
  $modalOverlay = $("#modalOverlay").addClass("init");
  $modalOverlay.on("click", toggleMobileModalSideMenu);
})();
// /Hamburger menu (side menu)
</script>
<div id="modalOverlay" class="init"></div>
<style type="text/css">
nav ul.scroll {
  position:static;
}
@media only screen and (max-width:30em){
  nav ul.scroll {
    position:absolute;
  }
}
@media only screen and (min-width:30em){
  nav ul.scroll {
    position:static;
  }
}
</style>
<script type="text/javascript">
(function(){
  var initScrollMenu = function(){
    var isMenuVisible = function(){
      try{
        var $wrapper = $("nav");
        var $scroll = $("nav ul.scroll");
        var maxScroll = $scroll.width();
        var availableViewport = {
          l:0,
          r:$wrapper.width()
        };
        var $activeMenu  = $("nav ul li.active");
        if($activeMenu.length > 0){}else{
          return;
        }
        var activeMenuOffset = $activeMenu.offset();
        //console.log("activeMenuOffset:", activeMenuOffset);
        var activeMenuPos = {
          l:activeMenuOffset.left,
          r:activeMenuOffset.left + $activeMenu.width()
        };
        //console.log("initialValues:" , initialValues);
        if(activeMenuPos.r > availableViewport.r){
          var currentOffset = $target.offset();
          var togo = availableViewport.r - activeMenuPos.r;
          //console.log("togo:", togo);
          $target.offset({
            "left":( togo <= initialValues.min ) ? initialValues.min : ( ( togo >= initialValues.max ) ? initialValues.max : togo),
            "top":initialValues.top
          });
        }
      }catch(e){console.log(e);}
    };
    var onSwipeHorizontal = function($el, cb){
      var start, dist;
      var ontouchstart = function(evt){
        var obj = evt.changedTouches[0];
        dist = 0;
        start = obj.pageX; // * smooth;
      },
      ontouchmove = function(evt){
        var obj = evt.changedTouches[0];
        dist = start - obj.pageX; // * smooth; // / 4.0 for smooth scroll
        cb(dist, 'touchmove');
      },
      ontouchend = function(evt){
      };

      $el.off('touchstart', ontouchstart);
      $el.off('touchmove', ontouchmove);

      $el.on('touchstart', ontouchstart);
      $el.on('touchmove', ontouchmove);
    };
    var $el = $('nav.menu-tabs');
    var $target = $("nav ul.scroll");
    var initialValues = $target.offset();
    initialValues.w = $target.width();
    initialValues.min = -1 * (initialValues.w - $target.parent().width());
    initialValues.max = initialValues.left;
    onSwipeHorizontal($el, function(dist, touchstate){
      var currentOffset = $target.offset();
      var togo = currentOffset.left - dist / 2;
      $target.offset({
        "left":( togo <= initialValues.min ) ? initialValues.min : ( ( togo >= initialValues.max ) ? initialValues.max : togo),
        "top":initialValues.top
      });
    });

    isMenuVisible();
  },
  setActiveMenu = function(){
    var menuToAll = [];
    menuToAll.push('/1603162');
    menuToAll.push('/1603742');
    menuToAll.push('/1603400');
    menuToAll.push('/1603234');
    menuToAll.push('/1603725');
    var referrer = document.referrer, pathname = referrer.split("dispatch.co.kr")[1];
    var notToActivate = ['/youthpolicy', '/privacy', '/dispatch'],cat = pathname, $activeAnchor = false,
    hrefs = $.map($("nav.menu-tabs li a"), function(a){
      //console.log(a.pathname);
      if(a.pathname != "/" && location.pathname.indexOf(a.pathname) == 0){
        $activeAnchor = $(a);
      }
      return a.pathname;
    });
    // -2. 검색일경우(search)
    if(location.pathname.indexOf("/search") == "0" ){
      //console.log("menu case -2");
      return;
    }
    // -1. 오른쪽의 today hot 에서 온 경우
    //console.log("!", location.pathname,$.inArray(location.pathname, menuToAll));
    if($.inArray(location.pathname , menuToAll) != -1){
      //console.log("menu case -1");
      $("nav.menu-tabs ul li:eq(0)").addClass("active");
      return;
    }
    if(location.pathname === "/"){  // 0. 화면이 첫페이지인경우
      //console.log("menu case 0");
      //$("nav.menu-tabs ul li:eq(0)").addClass("active");
      return;
    }
    if($activeAnchor !== false){ // 1. 메뉴에 해당 페이지의 주소가 있는경우.
      //console.log("menu case 1");
      $activeAnchor.parent().addClass("active");
      return;
    }
    if(notToActivate.indexOf(location.pathname) !== -1){ // 3. 하이라이트 될 필요가 없는 메뉴들
      //console.log("not to activate");
      //console.log("menu case 3");
      return;
    }
    if(cat !== ""){  // 2. 기사면인데 카테고리 순서를 물고온경우
      //console.log("menu case 2", hrefs.indexOf(cat).toString());
      //console.log(hrefs,"cat:", cat, "indexof:", hrefs.indexOf(cat));
      if(hrefs.indexOf(cat) != -1){
    $("nav.menu-tabs ul li:eq(" + hrefs.indexOf(cat).toString() + ")").addClass("active");
    return;
      }
      //console.log("indexof:", hrefs.indexOf(cat));
      //$activeAnchor = $.map($("nav.menu-tabs li a"))
    }
    //console.log("notToActivate:", notToActivate, notToActivate.indexOf(location.pathname) );
    // 4. 이외의 경우는 모두 최신으로.
    $("nav.menu-tabs ul li:eq(0)").addClass("active");
  }

  $(window).on('orientationchange resize', function(evt){
    setActiveMenu();
    initScrollMenu();
  });

  $(document).ready(function(){
    setActiveMenu();
    initScrollMenu();
  });
})();

</script>



