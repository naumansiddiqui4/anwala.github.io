      success: function(data){ 
	 if($.trim(data)=="success"){
	  alert('ì§ë¬¸ì´ ì ê³ ëììµëë¤.');
	  history.go(0);
      }else if($.trim(data)=="error:2"){
	 alert('ë¡ê·¸ì¸ ì ë³´ê° ë§ë£ëììµëë¤. \n ë¤ì ë¡ê·¸ì¸ í´ì£¼ì¸ì.');
	 location.href="https://asked.kr";
	}
    }
})
}



