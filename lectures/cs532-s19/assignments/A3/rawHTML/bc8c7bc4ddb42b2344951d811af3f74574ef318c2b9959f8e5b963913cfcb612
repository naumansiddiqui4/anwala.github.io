adscrol=1;
function open_ad(){
	$('#askedTitle').css("display", "block");
	$('.ask_selector').css("display", "block");
	adscrol=1;
}

function close_ad(){
	$('#askedTitle').css("display", "none");
	$('.ask_selector').css("display", "none");
	adscrol=0;
}

function cash_scroll_down(){
 $('.cash_ask_div').slideDown(300);
 $(".makarong_pay_btn").addClass("makarong_pay_btn_chk");
 $(".makarong_btn").addClass("makarong_up_btn");
 $('.makarong_pay_btn').attr('onclick' , 'cash_scroll_up();');
 $('.min_maka').css("display", "block"); 
 $('#counter').css("display", "none"); 
 close_ad();
}
function cash_scroll_up(){
 open_ad();
 $('.cash_ask_div').slideUp(300);
 $(".makarong_pay_btn").removeClass("makarong_pay_btn_chk");
 if(makarong_bat==-1){
	$(".makarong_btn").removeClass("makarong_up_btn");
 }
 $('.makarong_pay_btn').attr('onclick' , 'cash_scroll_down();');
 $('.min_maka').css("display", "none"); 
 $('#counter').css("display", "block"); 
}


//ìê°ê°ì ¸ì¤ê¸°



    function toMMSS(sec_num) {
        var pad = function(x) {
            return (x < 10) ? "0" + x : x;
        }
        return pad(parseInt(sec_num / (60 * 60))) + ":" +
            pad(parseInt(sec_num / 60 % 60)) + ":" +
            pad(sec_num % 60)
    }


    function refresh_timer() {
		$('.makarong_time_bar').each(function(index) {
		makarong_time = $(this).attr("time");
		if (makarong_time <= 18000) {
            $(this).attr('id', 'makarong_core_nantime');  
        }
		
		if (makarong_time <= 7200) {
            $(this).attr('id', 'makarong_core_nantime2');  
        } 

            $(this)
                .width(100 * (makarong_time / 86400) + "%")
        
        if (makarong_time > 0){
		        makarong_time--;
				$(this).children().first().html(toMMSS(makarong_time));
				$(this).attr("time",makarong_time);
		}else{
			$(this).children().first().html('00:00:00');
		}
		

		});

		
    }


    var timer = setInterval(refresh_timer, 1000);


makarong_bat = -1;
function makarong_batting(){
	makarong_bat=$('.makarong_input').val();
	if(Number($('.makarong_input').val()) != '0'){
		if(Number($('.makarong_input').val()) > Number($('.my_makarong_input').val())){
			alert("ë³´ì íì  ë§ì¹´ë¡±ë³´ë¤ ë§ìµëë¤.");
		}else{
			$('.makarong_pay_btn').html(makarong_bat+'ê°');
			cash_scroll_up();
			$(".makarong_btn").addClass("makarong_up_btn");
		}
	}else{
		cash_scroll_up();
		makarong_bat = -1;
		$(".makarong_btn").removeClass("makarong_up_btn");
	}
}


