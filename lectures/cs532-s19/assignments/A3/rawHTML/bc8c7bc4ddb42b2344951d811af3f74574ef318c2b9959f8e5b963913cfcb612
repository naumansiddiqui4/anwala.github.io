      type: "POST",   
      url: "./query.php?query=7",   
      data: { id: id, listnum: listnum },
      success: function(data){ 
	 if($.trim(data)=="success"){
	  $('#card_'+listnum).remove();
	  $('#bottomline_'+listnum).remove();
	  document.body.style.overflow= 'visible';
	  $("body").unbind('touchmove');
	  $('#asked_back3').css("display", "none");
	  cnt_re();
      }else if($.trim(data)=="error:2"){
	 alert('ë¡ê·¸ì¸ ì ë³´ê° ë§ë£ëììµëë¤.\në¤ì ë¡ê·¸ì¸ í´ì£¼ì¸ì.');
