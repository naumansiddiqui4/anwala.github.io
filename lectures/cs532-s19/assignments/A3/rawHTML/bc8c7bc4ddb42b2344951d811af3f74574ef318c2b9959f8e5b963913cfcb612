function ask_post(id){

$('.ask_bottom_buttom').attr('disabled',true);

if(show_user == true){
	post_show_user='1';
}else{
	post_show_user='0';
}

var content=$('#ask_content').val();
$.ajax({   
      type: "POST",   
      url: "./query.php?query=0",   
      data: { id: id, content: content, makarong_bat: makarong_bat, show_user: post_show_user },
      success: function(data){ 
	 if($.trim(data)=="success"){
		if($('.makarong_input').val()!='0'){
			downmakarong = Number($('.sidebar_makarong_cnt').html())-Number($('.makarong_input').val());
			$('.sidebar_makarong_cnt').html(downmakarong);
			$('.my_makarong_input').val(downmakarong);
		 }
		$('.ask_bottom_buttom').attr('disabled',false);
		if(/android/.test(ua)){
				ask_cnt=get_ask_cnt();
				if(ask_cnt==0 || ask_cnt < 0){
				 $('#ask_content').attr('readonly','true');
				 $('#ask_content').attr('onclick' , 'go_app();');
				 $('.ask_btn').attr('onclick' , 'go_app();');
				}
		}

      $('#ask_content').val("")

  	    var ua = String( navigator.userAgent ).toLowerCase();
		//ìëë¡ì´ëì ê²½ì°
		if(/android/.test(ua)){
			$('#asked_back3').css("display", "block");
			$('.new_ask_div').css("display", "block");
			document.body.scrollTop = 0;

			history.pushState(null, null, location.href);
			window.onpopstate = function(event) {
				close_back();

			ga('send','event','ì§ë¬¸ë³´ë´ê¸°','click','ë¹ë¡ê·¸ì¸');
			};

		}else if(/iphone/.test(ua)){
			$('#new_bottom_div_right1').attr('onclick', 'location.href=\'https://goo.gl/uEVPq4\';');
			$('#asked_back3').css("display", "block");
			$('.new_ask_div').css("display", "block");
			document.body.scrollTop = 0;
			ga('send','event','ì§ë¬¸ë³´ë´ê¸°','click','ë¹ë¡ê·¸ì¸');
		}else{
			alert('ë±ë¡ëììµëë¤.');
			ga('send','event','ì§ë¬¸ë³´ë´ê¸°','click','ë¹ë¡ê·¸ì¸');		}

      $('#counter').html('0' + '/500');
	  cnt_re();
	  com_ask();
      }else{
	 alert($.trim(data));
	 $('.ask_bottom_buttom').attr('disabled',false);
	}
    }
})

}

function go_app(){
	location.href='market://details?id=com.mobions.asked&url=asked%3a%2f%2fstart%3flinkType%3d1%26contentId%3d'+199891666;
}

function close_back(){
 $('#asked_back3').css("display", "none");
 $('.new_ask_div').css("display", "none");
 $('.new_ask_div2').css("display", "none");
}

function get_ask_cnt(){

var cookie = document.cookie;
var startIndexOf = cookie.indexOf('ask_cnt');
 
 
var endIndexOf = cookie.indexOf(';',startIndexOf);
if(endIndexOf == -1)endIndexOf = cookie.length;
 
var ask_cnt=cookie.substring(startIndexOf+'ask_cnt='.length, endIndexOf);
return ask_cnt;

}


function com_ask(){
			
			$("#ask_complete").addClass("active");
			$("#ask_new").removeClass("active");
			$("#ask_rej").removeClass("active");

var pages=0;
$.ajax({
            url:'./query.php?query=1&page='+pages+'&id=eksldi_love' ,
            dataType:'html',
            success:function(data){
			$("#cards").html($.trim(data));
			cnt_re();
			page=0;
			viewpage=0;

			if($.trim(data)==''){
				$("#cards").html('<img src="./img/no_ask.png" style="left: calc(50% - 125px);width:250px;position: relative;">');
			}
            }

        })

}

function new_ask(){

			$("#ask_complete").removeClass("active");
			$("#ask_new").addClass("active");
			$("#ask_rej").removeClass("active");

var pages=0;
$.ajax({
            url:'./query.php?query=2&page='+pages+'&id=eksldi_love' ,
            dataType:'html',
            success:function(data){
			$("#cards").html($.trim(data));
			cnt_re();
			page=0;
			viewpage=1;
			if($.trim(data)==''){
				$("#cards").html('<img src="./img/no_ask.png" style="left: calc(50% - 125px);width:250px;position: relative;">');
			}
            }

        })

}

function rej_ask_load(){

			$("#ask_complete").removeClass("active");
			$("#ask_new").removeClass("active");
			$("#ask_rej").addClass("active");

var pages=0;
$.ajax({
            url:'./query.php?query=3&page='+pages+'&id=eksldi_love' ,
            dataType:'html',
            success:function(data){
			$("#cards").html($.trim(data));
			cnt_re();
			page=0;
			viewpage=2;
			if($.trim(data)==''){
				$("#cards").html('<img src="./img/no_ask.png" style="left: calc(50% - 125px);width:250px;position: relative;">');
			}
            }

        })

}
//cnt load
function cnt_re(){
$.ajax({
            url:'./query.php?query=4&id=eksldi_love' ,
            dataType:'json',
            success:function(data){
			var cnt_re = new Array();
			var cnt_re = data;
			$('#compelte_count').html(cnt_re[0]);
			$('#new_count').html(cnt_re[1]);
			$('#reject_count').html(cnt_re[2]);
			if(cnt_re[3]==1){
			 $('.alram').addClass("nalram");
			 $('.nalram').removeClass("alram"); 
			}else{
			 $('.nalram').addClass("alram");
			 $('.alram').removeClass("nalram"); 
			}
            }
        })
			refresh_timer();
}


function registration_post(listnum,chk){

var id ='eksldi_love';
var content=$('#content_'+listnum).val();
$.ajax({   
      type: "POST",   
      url: "./query.php?query=5",   
      data: { id: id, content: content, listnum: listnum },
      success: function(data){ 
	 if($.trim(data)=="success"){
		 if($('#time_'+listnum).html()!='00:00:00'){
			upmakarong = Number($('#mcnt_'+listnum).html())+Number($('.sidebar_makarong_cnt').html());
			$('.sidebar_makarong_cnt').html(upmakarong);
			$('.my_makarong_input').val(upmakarong);
		 }
	  var ua = String( navigator.userAgent ).toLowerCase();
		//ìëë¡ì´ëì ê²½ì°
		if(/android/.test(ua)){
			$('#asked_back3').css("display", "block");
			$('.new_ask_div2').css("display", "block");

			history.pushState(null, null, location.href);
			window.onpopstate = function(event) {
				close_back();
				ga('send','event','ëµë³íê¸°','click','ìëë¡ì´ë');
			};

		}else if(/iphone/.test(ua)){
			$('#new_bottom_div_right2').attr('onclick', 'location.href=\'https://goo.gl/tjsmWp\';');
			$('#asked_back3').css("display", "block");
			$('.new_ask_div2').css("display", "block");
			document.body.scrollTop = 0;
			ga('send','event','ëµë³íê¸°','click','ìì´í°');

		}else{
			alert('ëµë³ì´ ë±ë¡ëììµëë¤.');
			ga('send','event','ëµë³íê¸°','click','ì¹');
		}
	  cnt_re();
	   if(chk!=1){
	     $('#card_'+listnum).remove();
	     $('#answer_'+listnum).remove();
	     $('#answer_selector_'+listnum).remove();
	      new_ask();
	    }else{
		  $('#card_'+listnum+'>.card_answer').html(content);
		  $('#close_more_'+listnum).attr('onclick' , 'javascript:more_askre('+listnum+')');
	    }

      }else if($.trim(data)=="error:2"){
	 alert('ë¡ê·¸ì¸ ì ë³´ê° ë§ë£ëììµëë¤.\në¤ì ë¡ê·¸ì¸ í´ì£¼ì¸ì.');
	 location.href="https://asked.kr";
	}else if($.trim(data)=="error:-3"){
		alert('ë§ì¹´ë¡±ì§ë¬¸ì ìì íì¤ ì ììµëë¤.');
	}else if($.trim(data)=="app"){
		alert('ì±ìì ëµë³íì¤ ì ììµëë¤.');
	}
    }
})





}


