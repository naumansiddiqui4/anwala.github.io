function close_up(listnum){

if (confirm('ëµë³ ë´ì©ì ì­ì íìê² ìµëê¹?\nì­ì í ì§ë¬¸ì ìì§ë¬¸ì¼ë¡ ì´ëí©ëë¤.') == true){    //íì¸

