function rating(num){

$.ajax({   
      type: "POST",   
      url: "./query.php?query=12",   
      data: { num: num },
      success: function(data){ 
	 if($.trim(data)=="success"){
		 rating_complete(num);
      }else{
	if (confirm('ì±ìì ì´ì©í  ì ììµëë¤.\nì± ë¤ì´ íë©´ì¼ë¡ ì´ëí©ëë¤.') == true){    //íì¸
		
		var ua = String( navigator.userAgent ).toLowerCase();
		//ìëë¡ì´ëì ê²½ì°
		if(/android/.test(ua)){
			location.href="https://ref.ad-brix.com/v1/referrallink?ak=621347963&ck=2704199&Deeplink=true";
		}else if(/iphone/.test(ua)){
			location.href="https://goo.gl/NpNJAs";
		}
	
		}else{   //ì·¨ì
		 return;
		}
	}
    }
})


}

function rating_complete(num){
  var rating = $('#rating_'+num);
  var rating_cnt = $('#rating_cnt_'+num);
  var cnt=$('#rating_cnt_'+num).html();
  if(!rating.is(".heart_click")){
	rating.addClass("heart_click");
	rating_cnt.addClass("rating_cnt_click");
	$('#rating_cnt_'+num).html(++cnt);
  }else{
    rating.removeClass("heart_click");
	rating_cnt.removeClass("rating_cnt_click");
	$('#rating_cnt_'+num).html(--cnt);
  }
}



function view_pto(){
	var view_pto = $('#asked_back2');
	if( view_pto.is(":visible") ){
	$('#asked_back2').css("display", "none");
	document.body.style.overflow= 'visible';
	$("body").unbind('touchmove');
	}else{
	$('#asked_back2').css("display", "block");
	document.body.style.overflow= 'hidden';
	$("body").css({overflow:'hidden'}).bind('touchmove', function(e){e.preventDefault()});
	}
}

