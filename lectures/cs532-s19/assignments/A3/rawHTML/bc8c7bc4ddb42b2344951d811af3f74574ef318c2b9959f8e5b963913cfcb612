function following(num){
var fw = $('#fw_'+num);

$.ajax({   
      type: "POST",   
      url: "./query.php?query=22",   
      data: { num: num },
      success: function(data){ 
	 if($.trim(data)=="success"){
		 //rating_complete(id);
		fw.removeClass("fwmoff");
		fw.removeClass("fwmon")
		
		fw.addClass("fwmon");
      }else if($.trim(data)=="success2"){
		 //rating_complete(id);
		 fw.removeClass("fwmon");
		 fw.removeClass("fwmoff");

		fw.addClass("fwmoff");
      }else{
	 alert('ë¡ê·¸ì¸í ì¬ì©í  ì ììµëë¤.');
	}
    }
})


}


