	}
}


function peep_ask(id){

	if (confirm('ë§ì¹´ë¡± 2ê°ë¥¼ ì´ì©í´ ëµë³ì íì¸íìê² ìµëê¹?') == true){
		$.ajax({   
			  type: "POST",   
			  url: "./query.php?query=26",   
			  data: { id: id },
			  success: function(data){ 
			 if($.trim(data)=="success"){
				location.href='/deep_link.php?contents='+id;
			  }else if($.trim(data)=="login"){
				  if (confirm('ë¡ê·¸ì¸ í ì¬ì©íì¤ ì ììµëë¤.\në¡ê·¸ì¸ íë©´ì¼ë¡ ì´ëíìê² ìµëê¹?') == true){
					location.href='/';
				  }
			  }else if($.trim(data)=="ncash"){
				  if (confirm('ë§ì¹´ë¡±ì´ ë¶ì¡±í©ëë¤.\në§ì¹´ë¡±êµ¬ë§¤ íë©´ì¼ë¡ ì´ëíìê² ìµëê¹?') == true){
					location.href='/';
				  }
			  }else{
			 alert($.trim(data));
			}
			}
		})
	}else{   //ì·¨ì
		return;
	}

}

