<script>
function mobilead_loc(){
	var ua = String( navigator.userAgent ).toLowerCase();
	if(/android/.test(ua)){
		location.href='https://goo.gl/fRo1AY';
	}else if(/iphone/.test(ua)){
		location.href='https://goo.gl/oIiZ0Z';
	}
}
</script>

