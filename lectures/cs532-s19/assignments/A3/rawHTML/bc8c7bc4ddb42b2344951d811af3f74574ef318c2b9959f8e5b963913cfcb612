function report_ask_confirm(num){
	var report_type=$(':radio[name="report"]:checked').val();
	$('#report_type').val(report_type);
	$('#report_num').val(num);

var chks=1;
	if(chks==0){
	$('.control_div').html('<div class="report_div_tab1" onclick="report_ask(\''+report_type+'\',\''+num+'\',\'0\');">ê²ìë¬¼ ì ê³ </div><div class="report_div_tab2" onclick="report_ask(\''+report_type+'\',\''+num+'\',\'1\');">ê²ìë¬¼ ì ê³  ë° ìì±ì ì°¨ë¨</div>');
	$('.control-group').css("height","180px"); 
	}else if(chks==1){
	$('.control_div').html('<div class="report_div_tab1" onclick="report_ask(\''+report_type+'\',\''+num+'\',\'2\');">ê²ìë¬¼ ì ê³ </div>');
	$('.control-group').css("height","130px"); 
	}

	$('.bottom_div').remove();
	
	$('.control_div').css("padding-left","0px"); 
}



