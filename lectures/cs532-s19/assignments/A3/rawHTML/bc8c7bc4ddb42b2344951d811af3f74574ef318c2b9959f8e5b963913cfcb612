<script>
var chk = 0;
	function viewMenu(){
	 $( "#toggle" ).toggle( "slide" );
	}	

   function sideMenu()
   {
    viewMenu();
    if(chk == 0)
    {
     document.getElementById("myarea").style.display = 'block';
	 document.getElementById("asked_back").style.display = 'block';
	 $("body").css({overflow:'hidden'}).bind('touchmove', function(e){e.preventDefault()});
	 document.body.style.overflow= 'hidden';
     chk=1;
    }
    else
    {
     document.getElementById("myarea").style.display = 'none';
	 document.getElementById("asked_back").style.display = 'none';
	 document.body.style.overflow= 'visible';
	 $("body").unbind('touchmove');
     chk=0;
    }
   }

function appDown(){

var ua = String( navigator.userAgent ).toLowerCase();
		//ìëë¡ì´ëì ê²½ì°
		if(/android/.test(ua)){
			location.href="https://ref.ad-brix.com/v1/referrallink?ak=621347963&ck=6496173&Deeplink=true";
		}else if(/iphone/.test(ua)){
			location.href="https://goo.gl/JGKM92";
		}

}




