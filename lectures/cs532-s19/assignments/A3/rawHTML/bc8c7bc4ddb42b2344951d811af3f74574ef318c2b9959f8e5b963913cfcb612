function showRe(num){

    // html dom ì´ ë¤ ë¡ë©ë í ì¤íëë¤.
        // menu í´ëì¤ ë°ë¡ íìì ìë a íê·¸ë¥¼ í´ë¦­íìë
            var submenu = $('#answer_'+num);
            // submenu ê° íë©´ìì ë³´ì¼ëë ìë¡ ë³´ëëê² ì ê³  ìëë©´ ìëë¡ ë³´ëëê² í¼ì¹ê¸°
            if( submenu.is(":visible") ){
                submenu.slideUp(250);
            }else{
                submenu.slideDown(250);
				$('html, body').animate({
			      scrollTop: $("#answer_"+num).offset().top - document.body.clientHeight + $("#answer_"+num).height()+210
				}, 600);
            }
}

//cntcheck
$(function() {
      $('#ask_content').keyup(function (e){
          var content = $(this).val();
          $('#counter').html(content.length + '/500');
      });
      $('#ask_content').keyup();
});



function close_more(num){
			$('.new_ask_div').css("display", "none");
			var asked_back3 = $('#asked_back3');
            var close_more = $('#closemore_'+num);


            if( asked_back3.is(":visible") ){
	            asked_back3.css("display", "none");
	            document.body.style.overflow= 'visible';
				$("body").unbind('touchmove');
                close_more.css("display", "none");
				$('#report_post_div').css("display", "none");
				$('.join_pop').css("display", "none");
            }else{
			    asked_back3.css("display", "block");
			    document.body.style.overflow= 'hidden';
				$("body").css({overflow:'hidden'}).bind('touchmove', function(e){e.preventDefault()});
			    close_more.css("display", "block");
				asked_back3.attr('onclick','close_more('+num+')');
            }
}

function login_pop(){
	close_more();
	$('.join_pop').css("display", "block");
	document.body.scrollTop = 0;
}

