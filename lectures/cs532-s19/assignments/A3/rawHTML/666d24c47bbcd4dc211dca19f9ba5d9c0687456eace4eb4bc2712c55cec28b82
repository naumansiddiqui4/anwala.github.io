<script type="text/javascript">
	$(function(){
		$("#tab_areaList > .target").css("display", "none");
		$("#tab_areaList > ul > .tab").click(function(){
			var index = $("#tab_areaList > ul > .tab").index(this);
			$("#tab_areaList > .target").eq(index).siblings("#tab_areaList > .target").css("display", "none");
			$("#tab_areaList > .target").eq(index).slideToggle();
			$("#tab_areaList > ul > .tab").eq(index).toggleClass("current_tab");
			$("#tab_areaList > ul > .tab").eq(index).siblings("#tab_areaList > ul > .tab").removeClass("current_tab");
		});

	});
</script>


