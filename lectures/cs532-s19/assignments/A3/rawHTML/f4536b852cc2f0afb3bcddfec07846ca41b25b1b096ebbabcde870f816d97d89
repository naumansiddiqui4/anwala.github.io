<script>
jQuery(function($){
	$("#imageurl").on("keyup",function(){
		$.post("http://booktweeters.com/tweet-image/index.php",{ "tweet_url":$("#imageurl").val() },function(data){
			if(data){
				$("#message").html(data);
			}
		});
	});
});
</script>

