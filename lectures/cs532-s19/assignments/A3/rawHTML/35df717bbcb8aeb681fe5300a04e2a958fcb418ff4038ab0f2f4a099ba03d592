<script type="text/javascript">

function countOfFacebook(url, class_name)
{
    var url = url ? url : location.href;
    var id = url.match(/\/[0-9]+/);
    url = 'http://news.livedoor.com/article/detail'+ id + '/';
    jQuery.ajax({
        type: 'GET',
        url: '/api/fb_count/article/?format=json&url=' + url,
        dataType: 'json',
        success: function(data){
            var result = 0;
            result = data.total_count;
            $(class_name).text(result); 
        }
    });
}

$(window).ready(function(){
    var permalink = "";
        countOfFacebook(permalink, '.fb_counts');
});



$(window).ready(function(){
    jQuery.ajax({
        type: 'GET',
        url: '/api/tweet_count/article/?format=json&id=16021608',
        dataType: 'json',
        success: function(data){
			if(data.count > 0){
				$('.tweet_counts').css('display', 'inline-block');
				$('.countImgSpace').css('margin-right', '8px');
			}else{
				$('.tweet_counts').css('display', 'none');
				return;
			}
				$('.tweet_counts').text(data.count);
        } 
    });
});
</script>

