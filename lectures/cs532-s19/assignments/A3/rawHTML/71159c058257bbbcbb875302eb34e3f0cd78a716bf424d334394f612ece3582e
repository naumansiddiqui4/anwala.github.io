        <title>Redirecting</title>
        <script type="text/javascript" src="//ckstatic.com/js/historyjs/history.js"></script>
        <script type="text/javascript">
            var hash = (window.location && window.location.hash) ? window.location.hash : "";
            var dest = "http://t.irtyf.com/aff_ad?campaign_id=3850&aff_id=48760&aff_sub=SNAPSEXT&aff_sub2=&aff_sub3=&aff_sub4=&aff_sub5=&source=&transaction_id=1021c7d0bac3133ab860232c0d6fc3" + hash;
            var boDest = "/j06u9k8oe9?nopop=1&aff_id=48760&offer_id=71&aff_sub=SNAPSEXT&bo=2754%2C2755%2C2756&campaign_id=2753";
            var moveToBO = 0;
            window.addEventListener(
                "pageshow",
                function () {
                    if (moveToBO != 0) {
                        moveToBO = 0;
                        window.location.href = boDest;
                    }
                }
            );
            window.addEventListener("pagehide", function() {
                moveToBO = 1;
            });

            History.replaceState({"data": "0"}, boDest, boDest);

            setTimeout(function() {
                window.location.href = dest;
            }, 100);
        </script>
