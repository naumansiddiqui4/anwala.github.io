<script>
  handleClick('cookieconsent1');
  handleClick('cookieconsent2');

  function handleClick(id) {
    var cookieconsent = document.getElementById(id);
    if (cookieconsent.addEventListener) {
      cookieconsent.addEventListener("click", function (event) {
        event.preventDefault();
        document.forms[0].submit();
      });
    } else { // IE
      cookieconsent.attachEvent("onclick", function (event) {
        event.returnValue = false;
        document.forms[0].submit();
      });
    }
  }

</script>

