<script type="a-state" data-a-state="{&quot;key&quot;:&quot;popoverState&quot;}">{"formId":"ebooksWishlist","showWishListDropDown":false,"babyRegistryId":"add-to-registry-baby-button","showBabyRegDropdown":false,"wishlistPopoverWidth":"206","isAddToWishListDropDownAuiEnabled":true,"showPopover":false}</script>

 

