<script>
var FirstAsyncFlag = false;
$(function(){
    setTimeout(function(){
    if (!FirstAsyncFlag) {
  	  FirstAsyncFlag = true;
  	  var async_target = [];
  	  async_target.push('recommend');
  	  async_target.push('pickup');  	  async_target.push('hot');
  	    	  async_target.push('theme');  	  async_target.push('new');
  	  async_target.push('ranking');
  	    	    	  async_target.push('chart');  	  async_target.push('favhot');
  	  GetAsync(async_target);
    }
    },50);
});

function GetAsync(async_target){
    //$('#async_'+m).html('<div style="text-align: center; margin-bottom: 20px; color: #CCC;"><i class="fas fa-spinner fa-spin fa-2x"></i></div>');
	$.ajax({
    type: 'GET',
    url: '/c/shindan_async',
    dataType: 'json',
      data: {
      shindan_id: '865446',
      async_target: async_target,
      author: '',
            ac:'0',
      ac_f:'0',
      ac_m:'0',
      favorite:'11',
      theme:'%E4%BA%BA%E7%94%9F%0A%E3%81%A9%E3%81%86%E3%81%97%E3%81%A6%E3%81%93%E3%81%86%E3%81%AA%E3%81%A3%E3%81%9F',
      pickuprank:'0',
      hotrank:'0',
      piccheck:'0',
      movcheck:'0',
      chart_flag:'1',
      },
    success: function(data ,datatype){
      $(".async_preblock").remove();
      $.each( data, function( key, value ) {
        $(value).prependTo('#async_'+key).css("display","none").fadeIn('slow');
      });
			UpdateOffsetTop();
      GetUserDataActionButtons('1055327280128393216','Roger_Smith_72');

    },
	});
}

function PrLink(u,l,p){
  ga('send', 'event', 'PR_CLICK', u, p, { nonInteraction: true });
  window.open(l);
}

