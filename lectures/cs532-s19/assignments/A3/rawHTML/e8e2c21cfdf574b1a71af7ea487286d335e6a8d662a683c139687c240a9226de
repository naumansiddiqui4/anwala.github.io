backgroundColor: ['rgba(231,244,255,0.9)', 'rgba(255,0,236,0.9)'],
pointBackgroundColor: 'rgba(231,244,255,1)',
              pointBorderColor: 'rgba(231,244,255,1)',
              pointHoverBackgroundColor: '#fff',
              pointHoverBorderColor: 'rgba(179,181,198,1)',
              pointRadius : 5,
              pointHoverRadius : 8,
              data: [100, 0],
          },]
      },
      options: {
          animation: {
              duration: 1500,
              onComplete: function(animation) {
                  window.setTimeout(function() {
                  }, 500);
              }
          },
          tooltips : {

          },
          legend: {              display: true,              position: 'right',          }, 
          scale: {
            reverse: false,
            pointLabels: {
              fontColor: '#222',
              fontSize: 18,
              
            },
            ticks: {min: 0,

            beginAtZero: true,
            maxTicksLimit: 5,
            fontColor: '#999',
            lineWidth: 2,
            },
            gridLines : {
              color: 'rgba(0,0,0,0.1)',
            },
          },
      },
  };
    
$(function() {
  new Chart(document.getElementById('canvas'), config);
});

function ChartUpload(){
  if(!isUploaded){
    var canvasData = $('canvas').get(0).toDataURL('image/png');
    var data = {};
    data.image = canvasData.replace(/^data:image\/png;base64,/,'');
    data.v = '4WFZOtOXSc%2B%2FzyYcKCVMDLvpl%2BNEn82Ge8iwNTHhUZKpAPA%2FJuIjp%2BQuZHCWzoop%2FB4FcrcWTO503OOUEkt2BVFt8qsPL%2FFtKRyx4u5J3tn%2FnDTegcRVcU0MeMlK36vqm%2BeTFcjpLNiAcgFh5HSMMwfy8J5EuRJ3zctFVLPrhEnEV7f42J3%2B1TUTfPAGn7mrktlqtaqicTiD4nX2dzpC2RJemEEYiPq4Trc9JUt7TxXyDqqCu1qHMetBKs1GDJg1cuU2tYlYYRGZ7J2YbI942WUhwvfd0XO0dHzoanYqMivq8WlZlKPKlNybbi5pQYXi';
    $.ajax({
      url: '/c/chart_upload',
      type: 'POST',
      success: function(data) {
        $('#canvas_block').after(data);
        isUploaded = true;
      },
      error: function() {
      },
      data: data
    });
  }
}
