function UpdateOffsetTop(){ 
        Slot_sm_pc_foot_728x90_topOffset =  $("#div-gpt-ad-1352514192773-0_box").offset().top;
        Slot_sm_pc_foot_728x90_adAlmostVisibleScrollValue = Slot_sm_pc_foot_728x90_topOffset - $(window).height();
        
}

      
          Slot_sm_pc_foot_728x90_refreshed = false;
          Slot_sm_pc_foot_728x90_topOffset =  $("#div-gpt-ad-1352514192773-0_box").offset().top;
          Slot_sm_pc_foot_728x90_listener = function() {
            if (window.scrollY >= (Slot_sm_pc_foot_728x90_topOffset - $(window).height() - 10) && !Slot_sm_pc_foot_728x90_refreshed) {
              googletag.cmd.push(function() {
              googletag.pubads().refresh([Slot_sm_pc_foot_728x90]);
              });
              Slot_sm_pc_foot_728x90_refreshed = true;window.removeEventListener('scroll', Slot_sm_pc_foot_728x90_listener);
  						UpdateOffsetTop();
            }

          }
          window.addEventListener('scroll', Slot_sm_pc_foot_728x90_listener);

window.onload = function() {
  UpdateOffsetTop();
}

$(document).ready(function(){


});





function SendEventGa(eventCategory,eventAction,eventLabel){
  
  if(eventAction === undefined){
    eventAction = "";
  }

  if(eventLabel === undefined){
    eventLabel = "";
  }

  if(eventCategory !== undefined) {
    ga('send', 'event', eventCategory, eventAction, eventLabel);
  }
}


</script>




<script>
function ShowUserActionModal(user_id,user_screen_name){
  $('#Modal-Useraction').modal('show');
  GetUserDataForModal(user_id,user_screen_name);
}
function UserAction(action,del,user_id,user_screen_name){
  $(".UserActionStatus_"+user_id).html('<span style="margin-left: 3px;"><i class="fas fa-spinner fa-spin"></i></span>');
  $(".userdata-"+user_id+"-useraction").html('<div class="useractionbuttons_block" style="padding-left: 30px;"><i class="fas fa-spinner fa-spin"></i></div>');
  $('#Modal-Useraction').modal('hide');
  var date_obj = new Date();
  var milliseconds = date_obj.getTime();
  var alert_box_id = 'alert_box_'+milliseconds;
  $("#alert_box").empty();
  
  setTimeout(function(){
    $.ajax({
      type: 'POST',
      url: '/c/useraction_update',
      dataType: 'json',
        data: {
        action: action,
        del: del,
        user_id: user_id,
        user_screen_name: user_screen_name,
        },
      success: function(data) {
        $(".UserActionStatus_"+user_id).empty();
        $(".userdata-"+user_id+"-useraction").empty();
        $(".UserActionStatus_"+user_id).html(data.UserActionStatus).hide().fadeIn();
        $(".userdata-"+user_id+"-useraction").html(data.UserActionButtons).hide().fadeIn();
        

        var alert_msg = '<div id="'+alert_box_id+'" class="alert alert-'+data.msg_alert+' alert_text">' + data.msg+ '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>';
        $("#alert_box").html(alert_msg).hide().fadeIn();
          setTimeout(function() {
              $('#'+alert_box_id).fadeOut(5000);
        }, 4000);
    
      },
    });
  },1000);
}
function GetUserDataForModal(user_id,user_screen_name){
    $("#Modal-Useraction-header").html('<div style="padding: 30px; text-align: center;"><i class="fas fa-spinner fa-spin fa-2x"></i></div>');
    $.ajax({
      type: 'POST',
      url: '/c/useraction_getuserdata',
      dataType: 'json',
        data: {
        user_id: user_id,
        user_screen_name: user_screen_name,
        dataflag: 1,
        buttonsflag: 1,
        },
      success: function(data) {
        $("#Modal-Useraction-header").html(data.userdata);
        $(".userdata-"+user_id+"-useraction").html(data.UserActionButtons);
      },
    });
}
function GetUserDataActionButtons(user_id,user_screen_name){
    $(".userdata-"+user_id+"-useraction").html('<div class="useractionbuttons_block" style="padding-left: 30px;"><i class="fas fa-spinner fa-spin"></i></div>');
    $.ajax({
      type: 'POST',
      url: '/c/useraction_getuserdata',
      dataType: 'json',
        data: {
        user_id: user_id,
        user_screen_name: user_screen_name,
        buttonsflag: 1,
        },
      success: function(data) {
        $(".userdata-"+user_id+"-useraction").html(data.UserActionButtons);
      },
    });
}
</script>

