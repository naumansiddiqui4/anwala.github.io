<script>
	function CreateIllust(nOpenId) {
		$.ajaxSingle({
			"type": "post",
			"data": { "ID":-1, "OD":nOpenId},
			"url": "/f/FCreateIllust.jsp",
			"dataType": "json",
			"success": function(data) {
				var nResId = parseInt($.trim(data), 10);
				if(nResId > 0) {
					// complete
					location.href = "/VEditIllust.jsp?ID=-1&TD="+nResId;
				} else if(nResId==-2) {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロードは2,000件までとなっております。');
				} else if(nResId==-3) {
					HideModal('#IllustUploadDlg');
					DispMsg('協賛会員のアップロードは10,000件までとなっております。');
				} else {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
				}
			},
			"error": function(req, stat, ex){
				DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
			}
		});
		return false;
	}

	function CreateNovel(nOpenId) {
		$.ajaxSingle({
			"type": "post",
			"data": { "ID":-1, "OD":nOpenId},
			"url": "/f/FCreateNovel.jsp",
			"dataType": "json",
			"success": function(data) {
				var nResId = parseInt($.trim(data), 10);
				if(nResId > 0) {
					// complete
					location.href = "/VEditNovel.jsp?ID=-1&TD="+nResId;
				} else if(nResId==-2) {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロードは100件までとなっております。');
				} else if(nResId==-3) {
					HideModal('#IllustUploadDlg');
					DispMsg('協賛会員のアップロードは1,000件までとなっております。');
				} else {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
				}
			},
			"error": function(req, stat, ex){
				DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
			}
		});
		return false;
	}

	function CreateManga(nOpenId) {
		$.ajaxSingle({
			"type": "post",
			"data": { "ID":-1, "OD":nOpenId},
			"url": "/f/FCreateManga.jsp",
			"dataType": "json",
			"success": function(data) {
				var nResId = parseInt($.trim(data), 10);
				if(nResId > 0) {
					// complete
					location.href = "/VEditManga.jsp?ID=-1&TD="+nResId;
				} else if(nResId==-2) {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロードは100件までとなっております。');
				} else if(nResId==-3) {
					HideModal('#IllustUploadDlg');
					DispMsg('協賛会員のアップロードは1,000件までとなっております。');
				} else {
					HideModal('#IllustUploadDlg');
					DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
				}
			},
			"error": function(req, stat, ex){
				DispMsg('アップロード中にエラーが発生しました。<br /><br />インターネットへの接続が切断されていませんか?ログアウトしていませんか?ウィルス防御ソフトが過剰な保護を行っていませんか?');
			}
		});
		return false;
	}
</script>

