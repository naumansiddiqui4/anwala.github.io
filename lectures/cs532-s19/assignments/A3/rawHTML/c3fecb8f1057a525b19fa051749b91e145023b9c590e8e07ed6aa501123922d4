			var idx = 0; var counter = 0;
			$(".thumbnails-wrapper.thumbnails-slider--active .product-single__thumbnails-product-template li a").each(function(){
				if( $(this).hasClass('active-thumb') )
					idx = counter;
				counter++;
			}); 
			nextIdx = idx+1;
			if( nextIdx >= totalImgs ) nextIdx = 0;
			$(".thumbnails-wrapper.thumbnails-slider--active .product-single__thumbnails-product-template li a").eq( nextIdx ).trigger('click');
			
		}
    });
