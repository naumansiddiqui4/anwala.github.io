  </script>
  <script type='text/javascript'>
    onload=function(){
    //  showLoginButton();
      billingAgreementCheck();
    };
    function billingAgreementCheck(){
        var billingAgreementId = billingAgreement.getAmazonBillingAgreementId();
	httpObj = new HXMLHttpRequest();
	httpObj.open("get","https://ibarakinews.jp/pay/billingAgreementCheck.php?param="+billingAgreementId);
        document.write(httpObj.responseText);
    };
   
    function showLoginButton() {
        var authRequest;
        OffAmazonPayments.Button("AmazonPayButton", "A2EJPZN0L50DEG", {
          type:  "PwA",
          color: "Gold",
          size:  "large",

          authorization: function() {
            loginOptions = {scope: "profile payments:widget payments:shipping_address", popup: true};
            if(0){
                 if(window.confirm("Amazon Payを使って記事を５０円で購入しますか？\nお買い上げ後、２週間程度閲覧することが可能です\n※期間内であっても記事は前触れなく訂正、削除されることがあります")){
                        //authRequest = amazon.Login.authorize (loginOptions,"../pay/selectcard.php?auto=1");
                        authRequest = amazon.Login.authorize (loginOptions,"../pay/payment.php");
                  }else{
		//	window.alert('キャンセルされました');
                  }
                //});
            }else{
              authRequest = amazon.Login.authorize (loginOptions, "../pay/selectcard.php");
            }
          }
        });
    };
  </script>
