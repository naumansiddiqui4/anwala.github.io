        color: #D0D0D0;
    } 
    @media screen and (max-width: 950px) { 
    	#barra-estadao-parceiros > nav{width:950px; margin:0 auto;} 
    } 
    @media screen and (max-width: 830px) { 
    	#barra-estadao-parceiros > nav{width:830px; margin:0 auto;} 
    } 
	@media screen and (max-width: 767px) { 
        #barra-estadao-parceiros {
            display: none;
        }
    }
</style> 
<div id="barra-estadao-parceiros" class="barra-estadao-parceiros"> 
    <nav> 
        <ul>
            <li class="assine-estadao"><a class="title" href="https://assine.estadao.com.br/?utm_source=portal_estadao_selo_barra_de_parceiros&utm_medium=portal_estadao_selo_barra_de_parceiros&utm_campaign=portal_estadao_selo_barra_de_parceiros" target="_blank" >Assine o Estadão</a></li>
            <li class="assine-estadao">●</li>
            <script type="text/javascript">
                (function(){
                    var getCookie = function(cname) {
                        var name = cname + "=";
                        var decodedCookie = decodeURIComponent(document.cookie);
                        var ca = decodedCookie.split(';');
                        for(var i = 0; i <ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0) == ' ') {
                                c = c.substring(1);
                            }
                            if (c.indexOf(name) == 0) {
                                return c.substring(name.length, c.length);
                            }
                        }
                        return null;
                    };

                    try{
                        var cookieData = getCookie('OESP_INFO');
                        if(cookieData) {
                            cookieData = JSON.parse(cookieData);
                            if (cookieData.produtos !== 'undefined' &&
                                cookieData.produtos != "" &&
                                cookieData.produtos.indexOf('Not_') > -1
                            ) {
                                var li_assine = document.querySelectorAll('#barra-estadao-parceiros .assine-estadao');
                                for(var i in li_assine){
                                    li_assine[i].style.display = "none";
                                }
                            }
                        }
                    }catch (e) {}
                })();
            </script>
