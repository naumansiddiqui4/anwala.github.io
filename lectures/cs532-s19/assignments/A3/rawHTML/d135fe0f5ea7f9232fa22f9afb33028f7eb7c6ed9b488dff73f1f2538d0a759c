                               show_page_set = 1;
        }
    
        var endTriggerNodes = $(endTriggerNode);
        if (endTriggerNodes.length > 0) {
            var endTriggerNodePosition = endTriggerNodes.offset().top - $(window).height();
            if (scrollTop > endTriggerNodePosition && end_page_set == 0) {
                
