function review_load(pos, sort, no_review_button)
{
    if(typeof no_review_button === 'undefined') no_review_button = 0;

    var url = 'https://www.dmm.com/review/-/list/ajax-list/';
    var params = {
        'cid'             :'n\u005f691lcdv40905',
        'title'           :'Lovely\u0020Blue\u002f\u9752\u5c71\u3072\u304b\u308b',
        'service'         :'pc',
        'page'            :pos,
        'limit'           :'5',
        'sort'            :sort,
        'no_review_button':no_review_button,
        'shop_name'       :'mono\u005fgdvd'
    };

    $("#review").load(url, params);
}

function review_evaluate(key, params)
{
    var url = 'https://www.dmm.com/review/-/list/ajax-evaluate/';
    $("#evaluate" + key).html('読み込み中です...');
    $("#evaluate" + key).load(url, params);
}

// 並び替え用
function review_desc(desc)
{
    review_load(1, desc.value);
}

$(function() {
    //レビュー：もっと見る
    var $target = $('.fn-d-review__many-text'),
        maxH = 220,
        maxHClass = 'd-review__unit__comment--max-height',
        active = 'd-review__unit__comment--open',
        toggleOpen = 'もっと見る',
        toggleClose = '閉じる',
        toggleOpenClass = 'd-modtogglelink-open',
        toggleCloseClass = 'd-modtogglelink-close',
        $toggleBtn = $('<span class="d-modtogglelink-open">もっと見る</span>'),
        $toggleItem = $('<p class="d-review__unit__more"></p>'),
        toggleItemClass = '.d-review__unit__more';

    var fnOpen = function() {
            var $el = $(this),
                $target = $el.parent().prev();

            if ($target.hasClass(active) === true) {
                $target.removeClass(active);
                $el.text(toggleOpen).addClass(toggleOpenClass).removeClass(toggleCloseClass);
            } else {
                $target.addClass(active);
                $el.text(toggleClose).removeClass(toggleOpenClass).addClass(toggleCloseClass);
            }
        };

    var fnCheckH = function(item) {
            $el = item;
            if ($el.hasClass(maxHClass) === false) {
                if ($el.height() > maxH) {
                    $el.addClass(maxHClass).after($toggleItem.clone(true));
                }
            } else {
                if ($el.height() < maxH) {
                    $el.removeClass(maxHClass).removeClass(active).next(toggleItemClass).remove();
                }
            }
        };

    $toggleBtn.bind('click', fnOpen);
    $toggleItem.append($toggleBtn);

    //リサイズごとに実行
    $(window).unbind('resize.d-review__set').bind('resize.d-review__set', function() {
        $target.each(function() {
            var $el = $(this);
            fnCheckH($el);
        });
    }).trigger('resize.d-review__set');

    //レビュー：ネタバレ
    var $spoilerBtn = $('.fn-d-review__spoiler > span'),
        fnSpoilerOpen = function() {
            var $el = $(this),
                $target = $el.parent().prev().prev('.fn-d-review__unit__caution');
            $target.remove();
            $el.remove();
        };
    $spoilerBtn.bind('click', fnSpoilerOpen);

    //スムーススクロール
    $('#fn-d-review--anchor').smoothScroll();
});
//]]>
</script>
<!-- ad-display script begin test-->
<div class="mg-b12 center">
    <div id="footer-ad-container"></div>
</div>
<script type="text/javascript">
//広告設置済みの場合true, 未設置の場合false
var isFooterAd = false;

$(function(){
    //遅らせてレンダリング関数を実行する
    setTimeout(insertAlternateFooterAd, 5000);
});

function createFooterBanner(ad)
{
    var img = document.createElement("img");
    var link = document.createElement("a");
    link.href = ad.clickUrl;

    if (ad.creative.images && ad.creative.images[0]) {
        //コンボクリエイティブの場合
        if (ad.creative.content) {
            for (contentIndex in ad.creative.content) {
                //オプションが設定されている場合
                switch (ad.creative.content[contentIndex].key) {
                case "target":
                    //管理画面でtargetの値を登録しないとcontent属性値が空配列になることに注意！
                    link.target = ad.creative.content[contentIndex].value;
                    break;
                case "説明":
                    //説明が登録されていたら代替テキストをセット
                    img.alt = ad.creative.content[contentIndex].value;
                    break;
                default:
                }
            }
        }

        img.src = ad.creative.images[0].source;
    } else {
        //イメージクリエイティブの場合
        img.src = ad.creative.source;

        if (ad.creative.description) {
            //説明が登録されていたら代替テキストをセット
            img.alt = ad.creative.description;
        }
    }

    link.appendChild(img);
    return link;
}

//広告配信サーバへのリクエスト送信に対するコールバック関数
function renderFooterAd(data, context) {
    if (isFooterAd) return;
    if (data && data.searchResult && data.searchResult.spaces && data.searchResult.spaces[0] && data.searchResult.spaces[0].ads) {
        //広告スペースが存在する場合は広告オブジェクトを取得
        var ads = data.searchResult.spaces[0].ads;
        if (ads[0]) {
            //フッターバナー
            document.querySelector("#footer-ad-container").appendChild(createFooterBanner(ads[0]));
            isFooterAd = true;
        }
    } else {
        //登録バナーが足りないときやアドサーバ側のバグによって意図する広告を取得できなかった場合
        insertAlternateFooterAd();
    }
}

//サーバー、ネットワーク障害発生時に代替広告を表示
function insertAlternateFooterAd()
{
    try {
        if (isFooterAd) return;

        var alternateAd = new Object();
        alternateAd.creative = new Object();
        alternateAd.creative.source = "//p.dmm.com/p/bnr/ad/bottom/common_bottom_g.jpg";
        alternateAd.clickUrl = "http://ad.dmm.com/ad/p/r?_site=242&_article=9743&_link=197000&_image=197084";
        var adElement = createFooterBanner(alternateAd);

        if (adElement) {
            //広告枠が空であれば代替広告をセット
            $("#footer-ad-container").append(adElement);
            isFooterAd = true;
        }
    } catch (e) {
        //何もしない
    }
}

var cX = cX || {};
cX.callQueue = cX.callQueue || [];
cX.callQueue.push(['insertAdSpace', {
    adSpaceId:'000000015fcc20fc',
    baseAdDeliveryUrl:'http://display.dmm.com/adserver/search',
    secureBaseAdDeliveryUrl:'https://display.dmm.com/adserver/search',
    adUnitWidth:540,
    adUnitHeight:70,
    initialHorizontalAdUnits:1,
    initialVerticalAdUnits:0,
    renderFunction: renderFooterAd,
}]);
</script>
