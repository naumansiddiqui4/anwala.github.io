<script>
//_ubL[ÜIÊy[WÅC³·éÉ`FbNÅüÍÂ\
$('#check').change(function() {
    // `FbNªüÁÄ¢½çLø»
    if ( $(this).is(':checked') ){
        // {^ðLø»
        $('#firstname').prop('disabled', false);
        $('#lastname').prop('disabled', false);
        $('#T1').prop('disabled', false);
        $('#T2').prop('disabled', false);
        $('#T3').prop('disabled', false);
        $('#zipcode1').prop('disabled', false);
        $('#zipcode2').prop('disabled', false);
        $('#address').prop('disabled', false);
        $('#revise-button').prop('disabled', false);
    } else {
        // {^ð³ø»
        $('#firstname').prop('disabled', true);
        $('#lastname').prop('disabled', true);
        $('#T1').prop('disabled', true);
        $('#T2').prop('disabled', true);
        $('#T3').prop('disabled', true);
        $('#zipcode1').prop('disabled', true);
        $('#zipcode2').prop('disabled', true);
        $('#address').prop('disabled', true);
        $('#revise-button').prop('disabled', true);
    }
});
</script>


