<script>
//ægå\¦ÈµÌêÌTlCØèÖ¦
  jQuery(function($){
    $("#photo img").bind("load",function(){
      var ImgHeight = $(this).height();
      $('#photo').css('height',ImgHeight);
    });

    $('#navi a').click(function(){
      if($(this).hasClass('over') == false){
        $('#navi a').removeClass('over');
        $(this).addClass('over');
        $('#photo img').attr('src',$(this).attr('href'));
      };
      return false;
    }).filter(':eq(0)').click();
  });
</script>

<script>
//^u
$(function() {
  $('.tab li').click(function() {
    var index = $('.tab li').index(this);
    $('.contents_lists .contents_list').css('display','none');
    $('.contents_lists .contents_list').eq(index).css('display','block');
    $('.tab li').removeClass('select');

    $(this).addClass('select')
    var myColor = $(this).css("background-color");
    $(".contents_lists").css("border-color", myColor);
  });
});
</script>

