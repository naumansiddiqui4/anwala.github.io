<script>
// XN[ÅirQ[VÌæðí
$(window).scroll(function(e){
  var start_pos = 0;
  var current_pos = $(this).scrollTop();
  if (current_pos > start_pos) {
    $(".header_nav img").addClass("noicon");
  } else {
    $(".header_nav img").removeClass("noicon");
  }
  start_pos = current_pos;
});
</script>


<script>
//XN[Åy[Wgbv{^ðt^
$(function() {
    var topBtn = $('#page-top');
    topBtn.hide();
    $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
            topBtn.fadeIn();
        } else {
            topBtn.fadeOut();
        }

        scrollHeight = $(document).height();
        scrollPosition = $(window).height() + $(window).scrollTop();
        footHeight = $(".main_footer__copylight").innerHeight();

        if ( scrollHeight - scrollPosition  <= footHeight ) {
            $(".pagetop-btn").css({
                "position":"absolute",
                "border-radius":"5px 5px 0 0",
                "bottom": footHeight
            });
        } else {
            $(".pagetop-btn").css({
                "position":"fixed",
                "border-radius":"5px",
                "bottom": "10px"
            });
        }
    });

    topBtn.click(function () {
        $('body,html').animate({
            scrollTop: 0
        }, 500);
        return false;
    });
});
</script>

