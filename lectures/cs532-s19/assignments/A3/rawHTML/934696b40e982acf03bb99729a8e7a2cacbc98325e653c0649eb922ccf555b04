<div class="row">
<div class="large-12 columns">
 <div class="id-0 widget" data-show="all" data-widgetid="0" style=""> <div id="Halfpage" data-vis="all" data-position="Halfpage" style="margin:0 auto" class="cm-pad-15-t cm-pad-15-b text-center banner-area ">
<script type="text/javascript">
if (typeof allAdsSet === "undefined") allAdsSet = false;
var started_Halfpage = false, interval_Halfpage = 'undefined', timeout_Halfpage = 'undefined', showIfInactive_Halfpage = false;
function add_Halfpage () {
if (allAdsSet && !started_Halfpage) {
//console.log("Halfpage");
started_Halfpage = true;
window.clearInterval(interval_Halfpage);
window.clearTimeout(timeout_Halfpage);
var thisAd = allAds.Halfpage;
if (thisAd) {
if ((("Halfpage".toUpperCase().search("OUTOF") > -1 || "Halfpage".toUpperCase().search("SLIDER_") > -1 || "Halfpage".toUpperCase().search("NATIVE_") > -1 || "Halfpage".toUpperCase().search("ADLINK") > -1))) {
googletag.cmd.push(function () { googletag.display('Halfpage'); });
} else if (showIfInactive || showIfInactive_Halfpage) {
googletag.cmd.push(function () { googletag.display('Halfpage'); });
} else {
//console.log("Creativo: " + showIfInactive);
thisAd.almostVisibleScrollValue = window.scrollY + window.innerHeight;
if (thisAd.almostVisibleScrollValue > $("#Halfpage").position().top &&
(window.scrollY - window.innerHeight) < $("#Halfpage").position().top &&
$("#Halfpage").position().top > window.scrollY) {
if (!document.hidden || showIfInactive_Halfpage) {
//console.log("Tela: " + thisAd.almostVisibleScrollValue + " | Top: " + window.scrollY + " | Tamanho: " + window.innerHeight + " | Formato: Halfpage | Posição: " + $("#Halfpage").position().top);
//console.log("refresh: ativo");
googletag.cmd.push(function () {
googletag.display('Halfpage');
googletag.pubads().enableSingleRequest();
});
} else {
thisAd.hasShown = false;
thisAd.focusListener = function() {
//alert("_____focus listener Halfpage 'cause focus");
if (!thisAd.hasShown)
googletag.cmd.push(function () {
googletag.display('Halfpage');
googletag.pubads().enableSingleRequest();
});
thisAd.hasShown = true;
window.removeEventListener('focus', thisAd.focusListener);
}
window.addEventListener('focus', thisAd.focusListener);
thisAd.mousemoveListener = function() {
if(!thisAd.hasShown)
{
googletag.cmd.push(function () {
googletag.display('Halfpage');
googletag.pubads().enableSingleRequest();
});
}
thisAd.hasShown = true;
window.removeEventListener('mousemove', thisAd.mousemoveListener);
}
window.addEventListener('mousemove', thisAd.mousemoveListener);
}
} else {
thisAd.refreshed = false;
thisAd.scrollListener = function() {
if ((($("#Halfpage").position().top < (window.scrollY + window.innerHeight) && $("#Halfpage").position().top > window.scrollY) && !thisAd.refreshed) || $("#Halfpage").position().top <= 5) {
//console.log("refresh: scroll ativo");
googletag.cmd.push(function () {
googletag.display('Halfpage');
googletag.pubads().enableSingleRequest();
});
thisAd.refreshed = true;
window.removeEventListener('scroll', thisAd.scrollListener);
}
}
window.addEventListener('scroll', thisAd.scrollListener);
}
}
}
}
};
 
interval_Halfpage = window.setInterval(add_Halfpage, 50);
timeout_Halfpage = window.setTimeout(function(){
window.clearInterval(interval_Halfpage);
}, 60000);

 </script>
</div>
