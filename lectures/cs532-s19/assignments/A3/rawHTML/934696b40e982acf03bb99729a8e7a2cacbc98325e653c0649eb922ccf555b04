<script type="text/javascript">
window._taboola = window._taboola || [];
_taboola.push({ flush: true });
</script>
 <div class="id-0 widget" data-show="all" data-widgetid="0" style=""> <div id="OutOfPage" data-vis="all" data-position="OutOfPage" style="margin:0 auto" class="text-center ">
<script type="text/javascript">
if (typeof allAdsSet === "undefined") allAdsSet = false;
var started_OutOfPage = false, interval_OutOfPage = 'undefined', timeout_OutOfPage = 'undefined', showIfInactive_OutOfPage = false;
function add_OutOfPage () {
if (allAdsSet && !started_OutOfPage) {
//console.log("OutOfPage");
started_OutOfPage = true;
window.clearInterval(interval_OutOfPage);
window.clearTimeout(timeout_OutOfPage);
var thisAd = allAds.OutOfPage;
if (thisAd) {
if ((("OutOfPage".toUpperCase().search("OUTOF") > -1 || "OutOfPage".toUpperCase().search("SLIDER_") > -1 || "OutOfPage".toUpperCase().search("NATIVE_") > -1 || "OutOfPage".toUpperCase().search("ADLINK") > -1))) {
googletag.cmd.push(function () { googletag.display('OutOfPage'); });
} else if (showIfInactive || showIfInactive_OutOfPage) {
googletag.cmd.push(function () { googletag.display('OutOfPage'); });
} else {
//console.log("Creativo: " + showIfInactive);
thisAd.almostVisibleScrollValue = window.scrollY + window.innerHeight;
if (thisAd.almostVisibleScrollValue > $("#OutOfPage").position().top &&
(window.scrollY - window.innerHeight) < $("#OutOfPage").position().top &&
$("#OutOfPage").position().top > window.scrollY) {
if (!document.hidden || showIfInactive_OutOfPage) {
//console.log("Tela: " + thisAd.almostVisibleScrollValue + " | Top: " + window.scrollY + " | Tamanho: " + window.innerHeight + " | Formato: OutOfPage | Posição: " + $("#OutOfPage").position().top);
//console.log("refresh: ativo");
googletag.cmd.push(function () {
googletag.display('OutOfPage');
googletag.pubads().enableSingleRequest();
});
} else {
thisAd.hasShown = false;
thisAd.focusListener = function() {
//alert("_____focus listener OutOfPage 'cause focus");
if (!thisAd.hasShown)
googletag.cmd.push(function () {
googletag.display('OutOfPage');
googletag.pubads().enableSingleRequest();
});
thisAd.hasShown = true;
window.removeEventListener('focus', thisAd.focusListener);
}
window.addEventListener('focus', thisAd.focusListener);
thisAd.mousemoveListener = function() {
if(!thisAd.hasShown)
{
googletag.cmd.push(function () {
googletag.display('OutOfPage');
googletag.pubads().enableSingleRequest();
});
}
thisAd.hasShown = true;
window.removeEventListener('mousemove', thisAd.mousemoveListener);
}
window.addEventListener('mousemove', thisAd.mousemoveListener);
}
} else {
thisAd.refreshed = false;
thisAd.scrollListener = function() {
if ((($("#OutOfPage").position().top < (window.scrollY + window.innerHeight) && $("#OutOfPage").position().top > window.scrollY) && !thisAd.refreshed) || $("#OutOfPage").position().top <= 5) {
//console.log("refresh: scroll ativo");
googletag.cmd.push(function () {
googletag.display('OutOfPage');
googletag.pubads().enableSingleRequest();
});
thisAd.refreshed = true;
window.removeEventListener('scroll', thisAd.scrollListener);
}
}
window.addEventListener('scroll', thisAd.scrollListener);
}
}
}
}
};
 
interval_OutOfPage = window.setInterval(add_OutOfPage, 50);
timeout_OutOfPage = window.setTimeout(function(){
window.clearInterval(interval_OutOfPage);
}, 60000);

 </script>
</div>
