window.clearTimeout(timeout_ARROBA_MOBILE3);
var thisAd = allAds.ARROBA_MOBILE3;
if (thisAd) {
if ((("ARROBA_MOBILE3".toUpperCase().search("OUTOF") > -1 || "ARROBA_MOBILE3".toUpperCase().search("SLIDER_") > -1 || "ARROBA_MOBILE3".toUpperCase().search("NATIVE_") > -1 || "ARROBA_MOBILE3".toUpperCase().search("ADLINK") > -1))) {
googletag.cmd.push(function () { googletag.display('ARROBA_MOBILE3'); });
} else if (showIfInactive || showIfInactive_ARROBA_MOBILE3) {
googletag.cmd.push(function () { googletag.display('ARROBA_MOBILE3'); });
} else {
//console.log("Creativo: " + showIfInactive);
thisAd.almostVisibleScrollValue = window.scrollY + window.innerHeight;
if (thisAd.almostVisibleScrollValue > $("#ARROBA_MOBILE3").position().top &&
(window.scrollY - window.innerHeight) < $("#ARROBA_MOBILE3").position().top &&
$("#ARROBA_MOBILE3").position().top > window.scrollY) {
if (!document.hidden || showIfInactive_ARROBA_MOBILE3) {
//console.log("Tela: " + thisAd.almostVisibleScrollValue + " | Top: " + window.scrollY + " | Tamanho: " + window.innerHeight + " | Formato: ARROBA_MOBILE3 | Posição: " + $("#ARROBA_MOBILE3").position().top);
//console.log("refresh: ativo");
googletag.cmd.push(function () {
googletag.display('ARROBA_MOBILE3');
googletag.pubads().enableSingleRequest();
});
} else {
thisAd.hasShown = false;
thisAd.focusListener = function() {
//alert("_____focus listener ARROBA_MOBILE3 'cause focus");
if (!thisAd.hasShown)
googletag.cmd.push(function () {
googletag.display('ARROBA_MOBILE3');
googletag.pubads().enableSingleRequest();
});
thisAd.hasShown = true;
window.removeEventListener('focus', thisAd.focusListener);
}
window.addEventListener('focus', thisAd.focusListener);
thisAd.mousemoveListener = function() {
if(!thisAd.hasShown)
{
googletag.cmd.push(function () {
googletag.display('ARROBA_MOBILE3');
googletag.pubads().enableSingleRequest();
});
}
thisAd.hasShown = true;
window.removeEventListener('mousemove', thisAd.mousemoveListener);
}
window.addEventListener('mousemove', thisAd.mousemoveListener);
}
} else {
thisAd.refreshed = false;
thisAd.scrollListener = function() {
if ((($("#ARROBA_MOBILE3").position().top < (window.scrollY + window.innerHeight) && $("#ARROBA_MOBILE3").position().top > window.scrollY) && !thisAd.refreshed) || $("#ARROBA_MOBILE3").position().top <= 5) {
//console.log("refresh: scroll ativo");
googletag.cmd.push(function () {
googletag.display('ARROBA_MOBILE3');
googletag.pubads().enableSingleRequest();
});
thisAd.refreshed = true;
window.removeEventListener('scroll', thisAd.scrollListener);
}
}
window.addEventListener('scroll', thisAd.scrollListener);
}
}
}
}
};
 
if (is_mobile()) {
$('body').on({
'touchmove': function (e) {
if ((window.scrollY + window.innerHeight > $("#ARROBA_MOBILE3").position().top && $("#ARROBA_MOBILE3").position().top > window.scrollY) || $("#ARROBA_MOBILE3").position().top <= 5) {
interval_ARROBA_MOBILE3 = window.setInterval(add_ARROBA_MOBILE3, 50);
}
}
});
}

 </script>
</div>
</div>
</div>
