<script type="text/javascript">
if (typeof allAdsSet === "undefined") allAdsSet = false;
var started_MobileLeaderboard = false, interval_MobileLeaderboard = 'undefined', timeout_MobileLeaderboard = 'undefined', showIfInactive_MobileLeaderboard = false;
function add_MobileLeaderboard () {
if (allAdsSet && !started_MobileLeaderboard) {
//console.log("MobileLeaderboard");
started_MobileLeaderboard = true;
window.clearInterval(interval_MobileLeaderboard);
window.clearTimeout(timeout_MobileLeaderboard);
var thisAd = allAds.MobileLeaderboard;
if (thisAd) {
if ((("MobileLeaderboard".toUpperCase().search("OUTOF") > -1 || "MobileLeaderboard".toUpperCase().search("SLIDER_") > -1 || "MobileLeaderboard".toUpperCase().search("NATIVE_") > -1 || "MobileLeaderboard".toUpperCase().search("ADLINK") > -1))) {
googletag.cmd.push(function () { googletag.display('MobileLeaderboard'); });
} else if (showIfInactive || showIfInactive_MobileLeaderboard) {
googletag.cmd.push(function () { googletag.display('MobileLeaderboard'); });
} else {
//console.log("Creativo: " + showIfInactive);
thisAd.almostVisibleScrollValue = window.scrollY + window.innerHeight;
if (thisAd.almostVisibleScrollValue > $("#MobileLeaderboard").position().top &&
(window.scrollY - window.innerHeight) < $("#MobileLeaderboard").position().top &&
$("#MobileLeaderboard").position().top > window.scrollY) {
if (!document.hidden || showIfInactive_MobileLeaderboard) {
//console.log("Tela: " + thisAd.almostVisibleScrollValue + " | Top: " + window.scrollY + " | Tamanho: " + window.innerHeight + " | Formato: MobileLeaderboard | Posição: " + $("#MobileLeaderboard").position().top);
//console.log("refresh: ativo");
googletag.cmd.push(function () {
googletag.display('MobileLeaderboard');
googletag.pubads().enableSingleRequest();
});
} else {
thisAd.hasShown = false;
thisAd.focusListener = function() {
//alert("_____focus listener MobileLeaderboard 'cause focus");
if (!thisAd.hasShown)
googletag.cmd.push(function () {
googletag.display('MobileLeaderboard');
googletag.pubads().enableSingleRequest();
});
thisAd.hasShown = true;
window.removeEventListener('focus', thisAd.focusListener);
}
window.addEventListener('focus', thisAd.focusListener);
thisAd.mousemoveListener = function() {
if(!thisAd.hasShown)
{
googletag.cmd.push(function () {
googletag.display('MobileLeaderboard');
googletag.pubads().enableSingleRequest();
});
}
thisAd.hasShown = true;
window.removeEventListener('mousemove', thisAd.mousemoveListener);
}
window.addEventListener('mousemove', thisAd.mousemoveListener);
}
} else {
thisAd.refreshed = false;
thisAd.scrollListener = function() {
if ((($("#MobileLeaderboard").position().top < (window.scrollY + window.innerHeight) && $("#MobileLeaderboard").position().top > window.scrollY) && !thisAd.refreshed) || $("#MobileLeaderboard").position().top <= 5) {
//console.log("refresh: scroll ativo");
googletag.cmd.push(function () {
googletag.display('MobileLeaderboard');
googletag.pubads().enableSingleRequest();
});
thisAd.refreshed = true;
window.removeEventListener('scroll', thisAd.scrollListener);
}
}
window.addEventListener('scroll', thisAd.scrollListener);
}
}
}
}
};
 
if (is_mobile()) {
$('body').on({
'touchmove': function (e) {
if ((window.scrollY + window.innerHeight > $("#MobileLeaderboard").position().top && $("#MobileLeaderboard").position().top > window.scrollY) || $("#MobileLeaderboard").position().top <= 5) {
interval_MobileLeaderboard = window.setInterval(add_MobileLeaderboard, 50);
}
}
});
}

