googletag.cmd.push(function () {
googletag.pubads().addEventListener("slotOnload", function (event) {
var isSuper = false;
for (var key in event.slot) {
if (event.slot.hasOwnProperty(key) && typeof(event.slot[key]) === "string" && event.slot[key].toUpperCase().search("SUPER") > -1) {
isSuper = true;
break;
}
}
if(isSuper){
var hasFixTo = false
, superAd = undefined;
if ($('[data-stickykit-super="parent"]').length > 0)
superAd = $('[data-stickykit-super="column"]');
else if ($('[data-stickykit="parent-super-home"]').length > 0)
superAd = $('[data-stickykit="column-super-home"]');
if (typeof superAd != 'undefined' && superAd.length > 0) {
var fixToInterval = window.setInterval(function(){
if(superAd.fixTo && !hasFixTo){
hasFixTo = true;
window.clearInterval(fixToInterval);
if (superAd.height() <= 150)
stickySuper = true;
if (superAd.data("stickykitSuper") == "column") { //Super
if (superAd.height() > 150){
if(!alreadyStickedBilboard){
stickySuper = true; alreadyStickedBilboard = true;
stickerTimer(billboardStickyTimer);
superAd.fixTo('[data-stickykit-super="parent"]', options_fixto);
setTimeout(function(){ superAd.fixTo('stop') }, (billboardStickyTimer + 1) * 1000);
}
}else{
superAd.fixTo('[data-stickykit-super="parent"]', options_fixto);
}
if (!stickySuper) superAd.fixTo('stop');
} else if (superAd.data("stickykit") == "column-super-home") { //Super Home
if (superAd.height() > 150){
if(!alreadyStickedBilboard){
stickySuper = true; alreadyStickedBilboard = true;
stickerTimer(billboardStickyTimer,superAd);
superAd.fixTo('[data-stickykit="parent-super-home"]', {
zIndex: 9,
useNativeSticky: false
});
setTimeout(function(){ superAd.fixTo('stop') }, (billboardStickyTimer + 1) * 1000 );
}
}else{
superAd.fixTo('[data-stickykit="parent-super-home"]', {
zIndex: 9,
useNativeSticky: false
});
}
if (!stickySuper) superAd.fixTo('stop');
}
}
}, 50);
}
}
});
});
function stickerTimer(countclose,superAd)
{
if (superAd) {
var msg = 'Fechará automaticamente em {0} segundos | ';
$('#SUPER_HOME_1SCROLL').append('<div style="display:none" id="billboardtimer"><span class="billboardtimer">'+msg.format(countclose)+'</span><a style="display:none" href="#" class="billboarclose"> X </a></div>');
var billboardtimer = setInterval(function () {
if(superAd.css('position') !== 'fixed'){
$('#billboardtimer').hide();
$('.billboarclose').hide();
}else{
$('#billboardtimer').show();
$('.billboarclose').show();
}
if (countclose <= 0) {
$('#billboardtimer').remove();
$('.billboarclose').remove();
clearInterval(billboardtimer);
return;
}
$('.billboardtimer').html(msg.format(countclose));
$('.billboarclose').on('click',function(e){
e.preventDefault();
superAd.fixTo('stop');
});
countclose = countclose - 1;
}, 1000);
}
}

