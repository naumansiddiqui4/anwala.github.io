 <div class="show-for-small-only id-0 " data-show="mobile" data-widgetid="0" style=""> <div id="ARROBA_MOBILE3" data-vis="mobile" data-position="ARROBA_MOBILE3" style="margin:0 auto" class="cm-pad-15-t cm-pad-15-b text-center banner-area show-for-small-only">
<script type="text/javascript">
if (typeof allAdsSet === "undefined") allAdsSet = false;
var started_ARROBA_MOBILE3 = false, interval_ARROBA_MOBILE3 = 'undefined', timeout_ARROBA_MOBILE3 = 'undefined', showIfInactive_ARROBA_MOBILE3 = false;
function add_ARROBA_MOBILE3 () {
if (allAdsSet && !started_ARROBA_MOBILE3) {
//console.log("ARROBA_MOBILE3");
started_ARROBA_MOBILE3 = true;
