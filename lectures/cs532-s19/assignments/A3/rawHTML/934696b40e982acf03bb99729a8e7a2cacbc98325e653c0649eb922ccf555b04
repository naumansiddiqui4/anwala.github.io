 <div class="show-for-large id-0 " data-show="desktop" data-widgetid="0" style=""> <div id="SUPER_NEGOCIOS_1SCROLL" data-vis="desktop" data-position="SUPER_NEGOCIOS_1SCROLL" style="margin:0 auto" class="cm-pad-15-t cm-pad-15-b text-center banner-area hide-for-small">
<script type="text/javascript">
if (typeof allAdsSet === "undefined") allAdsSet = false;
var started_SUPER_NEGOCIOS_1SCROLL = false, interval_SUPER_NEGOCIOS_1SCROLL = 'undefined', timeout_SUPER_NEGOCIOS_1SCROLL = 'undefined', showIfInactive_SUPER_NEGOCIOS_1SCROLL = false;
function add_SUPER_NEGOCIOS_1SCROLL () {
if (allAdsSet && !started_SUPER_NEGOCIOS_1SCROLL) {
//console.log("SUPER_NEGOCIOS_1SCROLL");
started_SUPER_NEGOCIOS_1SCROLL = true;
window.clearInterval(interval_SUPER_NEGOCIOS_1SCROLL);
window.clearTimeout(timeout_SUPER_NEGOCIOS_1SCROLL);
var thisAd = allAds.SUPER_NEGOCIOS_1SCROLL;
if (thisAd) {
if ((("SUPER_NEGOCIOS_1SCROLL".toUpperCase().search("OUTOF") > -1 || "SUPER_NEGOCIOS_1SCROLL".toUpperCase().search("SLIDER_") > -1 || "SUPER_NEGOCIOS_1SCROLL".toUpperCase().search("NATIVE_") > -1 || "SUPER_NEGOCIOS_1SCROLL".toUpperCase().search("ADLINK") > -1))) {
googletag.cmd.push(function () { googletag.display('SUPER_NEGOCIOS_1SCROLL'); });
} else if (showIfInactive || showIfInactive_SUPER_NEGOCIOS_1SCROLL) {
googletag.cmd.push(function () { googletag.display('SUPER_NEGOCIOS_1SCROLL'); });
} else {
//console.log("Creativo: " + showIfInactive);
thisAd.almostVisibleScrollValue = window.scrollY + window.innerHeight;
if (thisAd.almostVisibleScrollValue > $("#SUPER_NEGOCIOS_1SCROLL").position().top &&
(window.scrollY - window.innerHeight) < $("#SUPER_NEGOCIOS_1SCROLL").position().top &&
$("#SUPER_NEGOCIOS_1SCROLL").position().top > window.scrollY) {
if (!document.hidden || showIfInactive_SUPER_NEGOCIOS_1SCROLL) {
//console.log("Tela: " + thisAd.almostVisibleScrollValue + " | Top: " + window.scrollY + " | Tamanho: " + window.innerHeight + " | Formato: SUPER_NEGOCIOS_1SCROLL | Posição: " + $("#SUPER_NEGOCIOS_1SCROLL").position().top);
//console.log("refresh: ativo");
googletag.cmd.push(function () {
googletag.display('SUPER_NEGOCIOS_1SCROLL');
googletag.pubads().enableSingleRequest();
});
} else {
thisAd.hasShown = false;
thisAd.focusListener = function() {
//alert("_____focus listener SUPER_NEGOCIOS_1SCROLL 'cause focus");
if (!thisAd.hasShown)
googletag.cmd.push(function () {
googletag.display('SUPER_NEGOCIOS_1SCROLL');
googletag.pubads().enableSingleRequest();
});
thisAd.hasShown = true;
window.removeEventListener('focus', thisAd.focusListener);
}
window.addEventListener('focus', thisAd.focusListener);
thisAd.mousemoveListener = function() {
if(!thisAd.hasShown)
{
googletag.cmd.push(function () {
googletag.display('SUPER_NEGOCIOS_1SCROLL');
googletag.pubads().enableSingleRequest();
});
}
thisAd.hasShown = true;
window.removeEventListener('mousemove', thisAd.mousemoveListener);
}
window.addEventListener('mousemove', thisAd.mousemoveListener);
}
} else {
thisAd.refreshed = false;
thisAd.scrollListener = function() {
if ((($("#SUPER_NEGOCIOS_1SCROLL").position().top < (window.scrollY + window.innerHeight) && $("#SUPER_NEGOCIOS_1SCROLL").position().top > window.scrollY) && !thisAd.refreshed) || $("#SUPER_NEGOCIOS_1SCROLL").position().top <= 5) {
//console.log("refresh: scroll ativo");
googletag.cmd.push(function () {
googletag.display('SUPER_NEGOCIOS_1SCROLL');
googletag.pubads().enableSingleRequest();
});
thisAd.refreshed = true;
window.removeEventListener('scroll', thisAd.scrollListener);
}
}
window.addEventListener('scroll', thisAd.scrollListener);
}
}
}
}
};
 
if (!is_mobile()){
interval_SUPER_NEGOCIOS_1SCROLL = window.setInterval(add_SUPER_NEGOCIOS_1SCROLL, 50);
timeout_SUPER_NEGOCIOS_1SCROLL = window.setTimeout(function(){
window.clearInterval(interval_SUPER_NEGOCIOS_1SCROLL);
}, 60000);
}

 </script>
