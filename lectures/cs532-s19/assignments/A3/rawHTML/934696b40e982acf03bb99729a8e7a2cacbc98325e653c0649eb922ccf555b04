pbjs.onEvent('bidResponse', function (bid) {
window.bidRender = bid.renderer;
if (bid.mediaType == 'video' && bid.renderer) {
bid.renderer.setEventHandlers({
ended: function () {
console.log("End of the video and doing the refresh for new demand...");
var slots = googletag.pubads().getSlots(),
slot = null;
for (var index = 0; index < slots.length; index++) {
if (slots[index] && slots[index].getSlotElementId() == bid.adUnitCode) {
slot = slots[index];
}
}
if (!slots) {
console.log("Don't found the slot " + bid.adUnitCode);
return;
}
pbjs.requestBids({
adUnitCodes: [bid.adUnitCode],
timeout: 3000,
bidsBackHandler: function () {
googletag.cmd.push(function () {
pbjs.que.push(function () {
pbjs.setTargetingForGPTAsync([bid.adUnitCode]);
googletag.pubads().refresh([slot]);
});
});
}
});
}
});
}
});
pbjs.requestBids({
bidsBackHandler: function (bidResponses) { startAdPlacement(); },
timeout: PREBID_TIMEOUT
});

setTimeout(function () {
startAdPlacement;
}, FAILSAFE_TIMEOUT);
});
}
function sendAdserverRequest() {
if (pbjs.adserverRequestSent) return;
pbjs.adserverRequestSent = true;
googletag.cmd.push(function() {
pbjs.que.push(function() {
pbjs.setTargetingForGPTAsync();
googletag.pubads().refresh();
});
});
}
function startAdPlacement(){

if(adPlacementStarted) return;
adPlacementStarted = true;
googletag.cmd.push(function () {
googletag.pubads().setTargeting("Portal", "InfoMoney");
if(ttLoaded){
googletag.pubads().setTargeting('age', _ttprofiles.getAge);
googletag.pubads().setTargeting('gender', _ttprofiles.getGender);
googletag.pubads().setTargeting('cluster', _ttprofiles.getProfiles);
googletag.pubads().setTargeting('subjects', _ttprofiles.getSubjects);
googletag.pubads().setTargeting('team', _ttprofiles.getTeam);
googletag.pubads().setTargeting('equipment', _ttprofiles.getEquipment);
googletag.pubads().setTargeting('socialclass', _ttprofiles.getSocialClass);
googletag.pubads().setTargeting('microsegments', _ttprofiles.getMicrosegments);
googletag.pubads().setTargeting('lists', _ttprofiles.getLists);
googletag.pubads().setTargeting('tt_CustomAudience', _ttprofiles.getCustomAudience);
} else console.log("tt not loaded.");
 googletag.pubads().setTargeting("Pages","negocios/grandes-empresas/noticia,negocios,grandes-empresas,noticia");
googletag.pubads().setTargeting("keyword","bancos,acoes,balancos");

allAds.OutOfPage = googletag.defineOutOfPageSlot('/8003922/OutOfPage', 'OutOfPage').addService(googletag.pubads());
//allAds.AdLink = googletag.defineOutOfPageSlot('/8003922/AdLink', 'AdLink').addService(googletag.pubads());
