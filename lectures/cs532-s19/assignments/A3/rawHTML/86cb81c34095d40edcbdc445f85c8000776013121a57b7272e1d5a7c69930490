<script>
    function goTop(){
		window.scrollTo(0, 0);
		var topEl = document.getElementById('u_skip');
		if (topEl) {
			topEl.tabIndex = -1;
			topEl.focus();
			topEl.removeAttribute('tabIndex');
		}
    }
</script>

