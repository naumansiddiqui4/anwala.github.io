    window.attachEvent ? window.attachEvent('onload', loadHandler) : window.addEventListener('load', loadHandler, false);

    function loadHandler() {
        var aLoadJs = new Array();

        aLoadJs.push('https://ssl.pstatic.net/img.tvcast/pc/js/jindo/jindo.component.1_13_0.20140930.min.js');
        aLoadJs.push('/resources/release/js/lcslog_20190221061205.js');
        aLoadJs.push('/resources/release/js/tvcast.Namespace_20190221061205.js');
        aLoadJs.push('/resources/release/js/tvcast.pc.Gnb_20190221061205.js');

        
        
        aLoadJs.push('/resources/release/js/clipEnd/tvcast.pc.ClipEnd.all_20190221061205.js');
        
        
        
        jindo.LazyLoading.load(aLoadJs, function() {
            tvcast.pc.Gnb = new Gnb();
            tvcast.pc.ClipEnd = new ClipEnd();
            tvcast.pc.LikeIt = new LikeIt(ghtEnv);
            tvcast.pc.Comment = new Comment();
            tvcast.pc.RecommendStats = new tvcast.RecommendStats({
                apiDomain : 'https://logapi.tv.naver.com'
                , clipNo : '5375306'
                , device : 'PC'
                , brandId : 'cjenm'});
            tvcast.pc.Subscription = new Subscription();
            tvcast.pc.CreatorSubscription = new CreatorSubscription();
            //tvcast.pc.pplCommerce.init(channelPPL);

			tvcast.pc.AnimateThumb.init({
                "baseEl" : "container"
            });

			if (typeof rmcPlayer == "undefined") {
				var playLoggerOptions = {
					apiDomain: 'https://logapi.tv.naver.com',
					clipNo: '5375306',
					playlistNo: '',
					beforeClipNo: jindo.$Cookie().get("NTC_BEFORE_CLIP"),
					device: Boolean(isPc) ? 'PC' : 'MO'
				};
				tvcast.PlayLogger = new PlayLogger(playLoggerOptions);
				typeof WrapPlayer != "undefined" && WrapPlayer.init(playerOptions);
			}

		});

        aLoadJs = null;
    }
