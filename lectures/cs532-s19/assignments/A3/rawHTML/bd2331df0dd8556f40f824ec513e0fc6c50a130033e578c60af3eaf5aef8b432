    }else{
      if (window["html_teads"].indexOf("definePassback") >= 0){
        //onsole.log("Janoppix: ----->Banner footer Blocked por traer googletag Passback");
      }else{
        window["html_teads"] =devuelve.html;
        defer(function () {

          $("#ad_teads").html(html_teads);

        });
      }
    }
  }

  if(devuelve.div == "ad_layer"){

    //wind ow["html_layer"] =devuelve.html;
    window["html_layer"] =devuelve.html;


    document.getElementById("ad_layer").innerHTML="";
    defer(function () {

      $("#ad_layer").prepend(html_layer);
    });

  }


  if(devuelve.div == "ad_skin" && !devuelve.isEmpty){

    window["html_ad_skin"] =devuelve.html;
    //window["html_ad_skin"] =event.slot.ic._html_;

    document.getElementById("ad_skin").innerHTML="";
    defer(function () {
      setTimeout(function(){
        if (window["html_ad_skin"].indexOf("richmediastudio") >= 0){
          document.getElementById("skin-branding").classList.add("ads-skin");
        }
        document.getElementById("ad_skin").innerHTML="";
        $("#ad_skin").html(html_ad_skin);
      }, 5000);
    });
  }


  if(devuelve.div == "ad_top"){


    window["html_ad_top"] =devuelve.html;
    window["div_ad_top"] =devuelve.div;
    if (window["html_ad_top"].indexOf("document.write") >= 0){

    }else{
      
      if (window["html_ad_top"].indexOf("push_") >= 0){
        defer(function () {

          $("#ad_top").html(html_ad_top);

        });
      }
      
    }
  }

  if(devuelve.div == "ad_footer"){
    //window["html_ad_footer"] =devuelve.html;
    window["html_ad_footer"] =devuelve.html;
    if (window["html_ad_footer"].indexOf("document.write") >= 0){

    }else{
        defer(function () {

          $("#ad_footer").html(html_ad_footer);

        });
      }
  }

}else if(devuelve.div == "ad_header" && !devuelve.isEmpty){
  if(htmladHeader.indexOf("adnxs") >= 0 ){
    window["htmladHeader"] =devuelve.html;
    document.getElementById("ad_header").innerHTML="";
    defer(function () {
        jQuery("#ad_header").html("");
        jQuery("#ad_header").prepend(htmladHeader);
    });
  }


}else if((devuelve.size.toString() == "300,250" || devuelve.size.toString() == "300,600") && !devuelve.isEmpty){
  //window["ad_300x250_html"] =devuelve.html;
  window["ad_300x250_div"] =devuelve.div;
  window["ad_300x250_html"] = devuelve.html;
  //25 de julio 2017 tavo pidio sacar del iframe para cuandotraiga slot_ad_layer 300x250 layer video

  if (window["ad_300x250_html"].indexOf("global_push") >= 0){

    //defer(function () {
      //$("#"+ad_300x250_div).html("").html(ad_300x250_html);
    //});

  }

}

      });
    
    (function(w) {
        try {
            var name, col, persona = JSON.parse(window.localStorage.getItem("nvgpersona51231"));
            for (col in persona) {
                name = "nvg_" + col;
                name = name.substring(0, 10);
                if (typeof(googletag) == "object")
                    googletag.pubads().setTargeting(name, persona[col]);
                if (typeof(GA_googleAddAttr) == "function")
                    GA_googleAddAttr(name, persona[col]);
            }
        } catch (e) {}
    })(window)
    googletag.enableServices();
  });
