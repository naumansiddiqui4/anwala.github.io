								Kakao.init('003a73f65be09e80f1150220e5042488');
								function goSNS(type)
								{
									var snsurlval = $("#sns_url").val();
									var snstitleval = $("#sns_title").val();
									var snsimageval = $("#sns_image").val();
									var snsReUrl = encodeURIComponent(snsurlval);
									var snsReImg = encodeURIComponent(snsimageval);

									var SNSUrl = '';
									var popupOptions = '';
									if(type=='facebook'){
										SNSUrl = 'http://www.facebook.com/sharer.php?u='+snsReUrl+'';
									}
									else if(type=='twitter'){
										SNSUrl = 'http://twitter.com/intent/tweet?text='+snstitleval+'%0D%0A'+snsReUrl+'';
									}
									else if(type=='naver'){
										SNSUrl = 'http://blog.naver.com/openapi/share?url='+snsReUrl+'&title='+snstitleval+'';
										popupOptions = 'left=0, top=0, width=550, height=850, scrollbars=0, resizable=1';
									}
									else if(type=='kakaostory'){
										SNSUrl = 'https://story.kakao.com/share?url='+snsReUrl+'';
										popupOptions = 'width=600, height=750, resizable=no, scrollbars=yes, status=no';
									}
									else if(type=='kakaotalk'){
 
										Kakao.Link.sendDefault({
											objectType: 'feed',
											content: {
												title: snstitleval,
												imageUrl: snsimageval,
												link: {
													mobileWebUrl: snsurlval
												}
											}
										});
									}
									else if(type=='cyworld'){
										SNSUrl = 'http://csp.cyworld.com/bi/bi_recommend_pop.php?url='+snsReUrl+'&thumbnail='+snsReImg+'';
										popupOptions = 'width=400, height=364, scrollbars=no, resizable=no';
									}
									else if(type=='pin'){
										SNSUrl = 'https://www.pinterest.co.kr/pin/create/button/?media='+snsReImg+'&url='+snsReUrl+'&description='+snstitleval+'';
										popupOptions = 'status=yes,width=750,height=350';
									}
									else if(type=='flip'){
										SNSUrl = 'https://share.flipboard.com/bookmarklet/popout?v=2&title='+snstitleval+'&url='+snsReUrl+'&utm_campaign=widgets&utm_medium=web&utm_source=flipit&utm_content=';
										popupOptions = 'status=yes,width=650,height=750';
									}

									if(SNSUrl!=''){
										window.open(SNSUrl, type, popupOptions);
									}
								}
								</script>
