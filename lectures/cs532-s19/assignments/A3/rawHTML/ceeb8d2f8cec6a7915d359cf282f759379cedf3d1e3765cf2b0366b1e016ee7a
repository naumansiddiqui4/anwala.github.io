	<script>
	function setType(v){
		$("#stlist li").removeClass("all");
		$("#type").val(v);
		$("#st"+v).addClass("all");
	}
	function goSearch(){
		$("#sn").val($("#sni").val());
		document.schFrm.submit();
	}
	function enterSubmit(f){
		if(event.keyCode == 13){
			goSearch();
		}
	}

	</script>
