					<script type="text/javascript">
					/* ë§ì´ ë³¸ ê¸°ì¬ í­ë©ë´ */
					$(function () {
						tab('.daily_ranking #tab',2);
					});

					function tab(e, num){
						var num = num || 0;
						var menu = $(e).children();
						var con = $(e+'_cont').children();
						var select = $(menu).eq(num);
						var i = num;

						select.addClass('on');
						con.eq(num).show();

						menu.click(function(){
							if(select!==null){
								select.removeClass("on");
								con.eq(i).hide();
							}

							select = $(this);
							i = $(this).index();

							select.addClass('on');
							con.eq(i).show();
						});
					}
					</script>
				<!--// ë§ì´ ë³¸ ê¸°ì¬ -->

				<!-- ì¸ë¤ì¼ ë¦¬ì¤í¸ ë¸ì¶ìì­1 stage photo, hot photo-->
				<div class="article_viewlist">
