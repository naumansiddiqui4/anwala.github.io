								$('.zoom_out').click(function(){
									var getSize = $('.va_cont').css('fontSize');
									if(getSize=='15px')
										$('.va_cont').css('fontSize','17px');
									else if(getSize=='17px')
										$('.va_cont').css('fontSize','19px');
									else if(getSize=='19px')
										alert('ìµëí¬ê¸°ìëë¤.');
								});

								$('.zoom_in').click(function(){
									var getSize = $('.va_cont').css('fontSize');
									if(getSize=='19px')
										$('.va_cont').css('fontSize','17px');
									else if(getSize=='17px')
										$('.va_cont').css('fontSize','15px');
									else if(getSize=='15px')
										alert('ìµìí¬ê¸°ìëë¤.');
								});
							});
							</script>
						</div>
					</div>
