function onClickEventRanking() {
  try {
    var event = event || window.event;
    if (event) {
      var target = event.target || event.srcElement;
      for (var i = 0; (i < 10) && (typeof target !== 'undefined') && (target); i++) {
        if (target.nodeType === 1 && (target.nodeName === 'a' || target.nodeName === 'A') && typeof target.href === 'string') {
          if (target.id.indexOf("rank-r-") != -1) {
            cxSendClickAccessRankBody(target);
            break;
          }
        }
        target = target.parentNode;
      }
    }
  } catch (e) {}
  return true;
};
window.addEventListener('mouseup',onClickEventRanking,false);
