function doPickupSort(maxSize, ulSelector){
	var ulObj = jQuery(ulSelector);
	if (ulObj.size() != 1) return;
	var randomLi = jQuery(ulSelector + " li.random");
	var totalCount = jQuery(ulSelector + " li").size();
	if (totalCount == 0) return;
	var rndCount = maxSize - (totalCount - randomLi.size());
if ((randomLi.size() == 0) || (rndCount <= 0)){
		randomLi.remove();
		ulObj.show();
		return;
	}
	
	var sortedList = randomLi.clone(true).sort(function(a, b){
		return Math.round(Math.random()) - 0.5;
	});
	
	randomLi.remove();
	var boolList = getRandomBoolList(rndCount, sortedList.size());
	sortedList.each(function(i, val){
		if (boolList[i]) ulObj.append(val);
	});
	ulObj.show();
}
function getRandomBoolList(size, total) {
	var boolList = [];
	for (var n = 0; n < total; n++){
		boolList[n] = (size-- > 0);
	}
	for (var n = 0; n < total; n++){
		var tmp = boolList[n];
		var rnd = Math.floor(Math.random() * total);
		boolList[n] = boolList[rnd];
		boolList[rnd] = tmp;
	}
	return boolList;
}

jQuery(document).ready(function(){
	doPickupSort(9, "ul.pickupList");
});
//-->

