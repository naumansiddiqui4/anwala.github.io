<script type="text/javascript">
        $(document).ready(function(){
	    $('#menu').slicknav({
                init: function(){
                    $('.slicknav_btn').addClass('turn');
                    $("div, html").on("click", function (event) {
                        if(!$(event.target).hasClass("#menu a") &&
                            !$(event.target).hasClass("ul.slicknav_nav li a") &&
                            !$(event.target).hasClass("slicknav_menutxt") &&
                            !$(event.target).hasClass("slicknav_icon") &&
                            !$(event.target).hasClass("slicknav_icon-bar") &&
                            !$(event.target).hasClass("slicknav_item") &&
                            !$(event.target).hasClass("slicknav_row") &&
                            !$(event.target).hasClass("slicknav_parent") &&
                            !$(event.target).hasClass("slicknav_collapsed") &&
                            !$(event.target).hasClass("li") &&
                            !$(event.target).hasClass("i") &&
                            !$(event.target).hasClass("span") &&
                            !$(event.target).hasClass("slicknav_btn") &&
                            !$(event.target).hasClass("slicknav_arrow") &&
                            !$(event.target).hasClass("fa-dollar") &&
                            !$(event.target).hasClass("fa-trophy") &&
                            !$(event.target).hasClass("fa-list")) {
                            $("#menu").slicknav('close');
                        }
                    });
                },
                beforeOpen: function(trigger){
                    $('.slicknav_btn').removeClass('turn');
                },
                beforeClose: function(trigger){
                    $('.slicknav_btn').addClass('turn');
                }
              });
        });
        </script>
