<script type="text/javascript">
    var paycosearchAutocompleteUrl = 'https://api-suggest-ac.search.nhnent.com/ac/v1/appkeys/godo000000000000/domains/goldraum.godomall.com/suggest';
    $(function() {
        var targetForm = '';
        var _search = $("#frmSearchTop");

        _search.find('input[name="keyword"]').on('keyup focus', function(e, mode) {
            targetForm = e.target.form.name;
            if($.trim($(this).val())) {
                if (e.keyCode == '13' || e.keyCode == '27') {
                    $('div[class^="autocomplete-area"]').addClass("dn");
                } else {
                    $(".search-area").addClass("dn");
                    $('div[class^="autocomplete-area"]').addClass("dn");
                    $(".autocomplete-area-top").removeClass("dn");
                }
            } else {
                $(".search-area").removeClass("dn");
                $('div[class^="autocomplete-area"]').addClass("dn");
            }
        }).blur(function(e){
            $('body').click(function(e){
                if (!$('.search-area').has(e.target).length && e.target.name != 'keyword') {
                    $('.search-area').addClass('dn');
                    $(".autocomplete-area-top").addClass("dn");
                }
            });
            $('input[name="keyword"]').on('focus', function(e){
                if (targetForm != e.target.form.name) {
                    $('form[name="' + targetForm + '"]').find('.search-area').addClass('dn');
                }
            });
        });
        _search.find(".autocomplete-close-btn").click(function(e){
            e.preventDefault(e);
            $(".search-area").removeClass("dn");
            $(".autocomplete-area-top").addClass("dn");

        });
    });
</script>
