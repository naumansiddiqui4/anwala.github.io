    }else {
        footerLogin.prependHTML('<a href="https://nid.naver.com/nidlogin.login?svctype=262144" class="u_ftlk" id="news_login" onclick="nclk(event,\'fot.login\',\'\',\'\');" >로그인</a>')
    }
})();


