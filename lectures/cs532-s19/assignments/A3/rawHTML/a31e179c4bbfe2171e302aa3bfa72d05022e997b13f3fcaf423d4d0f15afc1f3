function setNotFoundImage(obj, size) {
	var defaultImageUrl = "https://mimgnews.pstatic.net/image/news/m/2011/renew/preparing_97x60.png";

	if(size == 52) {
		defaultImageUrl = "https://mimgnews.pstatic.net/image/news/m/2011/renew/preparing_52x52.png";
	} else if(size == 72) {
		defaultImageUrl = "https://mimgnews.pstatic.net/image/news/m/2011/renew/preparing_72x72.png";
	} else if(size == 92) {
		defaultImageUrl = "https://mimgnews.pstatic.net/image/news/m/2011/renew/preparing_92x70.png";
	} else if(size == 97) {
		defaultImageUrl = "https://mimgnews.pstatic.net/image/news/m/2011/renew/preparing_97x60.png";
	} else if(size == 115) {
		defaultImageUrl = "https://ssl.pstatic.net/static.news/image/news/m/2012/06/tmp_230_160.jpg";
	} else if(size == 430) {
		defaultImageUrl = "https://ssl.pstatic.net/static.news/image/news/m/2012/06/tmp_600_260.jpg";
	} else if(size == 230) {
		defaultImageUrl = "https://ssl.pstatic.net/static.news/image/news/m/2012/06/tmp_230_254.jpg";
	}

	jindo.$(obj).src = defaultImageUrl;
	jindo.$(obj).onerror = "";
}


