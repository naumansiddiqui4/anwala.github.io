!function() {
	var weGoTop = jindo.$Element("goTop");
	if(!weGoTop ) { return; }
	new nhn.m.news.ScrollChangeDetecter().attach("scrollChanged", jindo.$Fn(function(oCustomEvent){
	    weGoTop[(oCustomEvent.position.top > 1) ? "addClass" : "removeClass"]("show");
	},this).bind());
	
}();

jindo.$Fn(function(){
	var isMoveScroll = false;
	function moveScroll(weEvent) {
		isMoveScroll = true;
	}

	var welDoc = jindo.$Element(document);
	welDoc.attach("touchmove", moveScroll);

	setTimeout(function() {
		welDoc.detach("touchmove", moveScroll);
		if (window.pageYOffset == 0 && isMoveScroll == false) {
			topPage();
		}
	}, 400);

}).attach(window,'load');


(function(){
	var oNSearchUserStorage = new UserStorage({ sStorageType : "localStorage" });
	window.removeRefererNaverSearch = function() {
		return oNSearchUserStorage.removeItem('newsSearchFromNaver');
	};

