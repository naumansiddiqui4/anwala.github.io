		<script type="text/javascript">
            doAfterLoadEnd(function() {
                var oLnb = new Lnb('._lnb_scroll', {sSortCookieKey: 'user_lnb', defaultMenuIdForNoSelected: 'HOME'});

                var lnbCookie = jindo.$Cookie();
                var isShownNewLnb = lnbCookie.get("isShownNewLnb");
                if (isShownNewLnb == null) {
                    isShownNewLnb = "Y";
                    lnbCookie.set("isShownNewLnb", isShownNewLnb, 3650, "/");
                    $ElementList("._isNew").addClass("is_new");
                }
            });
		</script>
		

		
		<script type="text/javascript">
            doAfterLoadEnd(function() {
                var ImageLoader = (typeof nhn != 'undefined') && nhn.ImageLazyLoader;
                if (!ImageLoader) {
                    return;
                }
                new ImageLoader().load(jindo.$Element(document.body).queryAll("img[data-src]"), {
                    loadAll: function (aImgList) {
                    }
                });

            // iframe lazy-loading
            (function(){
                try {
                        Array.prototype.slice.apply(document.querySelectorAll('iframe[data-src]'))
                        .forEach(function(iframeEl) {
                            var originSrc = iframeEl.getAttribute('data-src');
                            if(originSrc) {
                                iframeEl.setAttribute('src', originSrc);
                            }
                        });
                }catch(e) {}
            })();

            // 본문 상단 댓글 카운트
            CommentCountList.init({
                fCallback : {
                    'readPageViewComment' : function(commentCnt) {
                        var targetTopEl = jindo.$Element("comment_count");
                        if (commentCnt == 0) {
                            targetTopEl.addClass("is_zero");
                            targetTopEl.html("댓글");
                        } else {
                            targetTopEl.removeClass("is_zero");
                            targetTopEl.html(commentCnt);
                        }
                    }
                }
            });
        })
		</script>
