<script type="text/javascript" language="javascript"><!--
(function(){
  var url = 'http://blog.livedoor.jp/takosaburou/archives/popular_articles.json'
         + '?rangetype=week0'
         + '&from=0'
         + '&range=1'
         + '&limit=10'
