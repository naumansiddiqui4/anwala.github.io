<script type="text/javascript">
// <![CDATA[
jQuery(function($){
	$(document).ready(function(){
		var gnb = $("#cGnb");
		gnb.find(">ul>li").find("button.menu_toggle").click(function(){
			$(this).find("i").toggle();
			$(this).parent().next("ul").slideToggle(200);
		});
	});
});
// ]]>
</script>
<script type="text/javascript">
// <![CDATA[
jQuery(function($){
	$(document).ready(function(){
				var offset = $("nav.fix").offset().top-20;
				$(window).scroll(function(){
			if($(window).scrollTop() > offset){
				$("nav.fix").addClass("fixed");
			}else{
				$("nav.fix").removeClass("fixed");
			}
		});
	});
});
// ]]>
</script>
