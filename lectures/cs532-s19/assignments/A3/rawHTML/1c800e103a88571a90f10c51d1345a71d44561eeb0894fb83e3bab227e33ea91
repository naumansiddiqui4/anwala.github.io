<script>
	//업데이트 플래그
	function fn_update(type){
		var	is_alert=0;
		var	is_cancel=0;
		var alert_text="";
		var like_count_clicked = jQuery("#like_count_clicked").val();
		var well_count_clicked = jQuery("#well_count_clicked").val();
		var spam_count_clicked = jQuery("#spam_count_clicked").val();
		var declared_count_clicked = jQuery("#declared_count_clicked").val();
		var duplicate_count_clicked = jQuery("#duplicate_count_clicked").val();
		if( type == "like" && like_count_clicked=="y"){
			alert_text="좋아요 취소";
			is_alert=1; is_cancel=1;
		}
		if( type == "well" && well_count_clicked=="y" ){
			alert_text="글쎄요 취소";
			is_alert=1; is_cancel=1;
		}
		if( type == "spam" && spam_count_clicked=="y" ){
			alert_text="스팸신고 취소";
			is_alert=1; is_cancel=1;
		}
		if( type == "declared" && declared_count_clicked=="y" ){
			alert_text="신고 취소";
			is_alert=1; is_cancel=1;
		}
		if( type == "duplicate" && duplicate_count_clicked=="y" ){
			alert_text="중복글 취소";
			is_alert=1; is_cancel=1;
		}
		
		var is_confirm=1;
		if(is_alert){
			( confirm(alert_text) ) ? is_confirm=1 : is_confirm=0;
		}
		if( is_confirm ){
			jQuery.get("/xe/_util/proxy.php", { act : "vote" , type : type , target_srl : 43924488, is_cancel : is_cancel },
			function(d){
				if(d.trim()=="already_vote"){
					//alert('이미 선택을 했습니다.');
					//location.reload();
				}else{
					console.log(d+ " #"+type+"_count")
					jQuery("#"+type+"_count").html(d.trim());
					if(!is_cancel){
						jQuery("#"+type+"_count_clicked").val('y');
					}else{
						jQuery("#"+type+"_count_clicked").val('n');
						//location.reload();
					}
				}
			});
		}
	}
</script>
