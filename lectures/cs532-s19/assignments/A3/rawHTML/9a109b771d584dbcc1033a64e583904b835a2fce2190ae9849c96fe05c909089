        }
    }
</script>

    <script>
        var google_custom_params = window.linkfire.retargetingPixels.google.tags;
        var conv_handler = null;
        var gaData = window.linkfire.retargetingPixels.google.gaData;
        function TriggerGooglePixel(){
            for(var i = 0; i < gaData.length; i++){
                window.google_conversion_id = gaData[i].id;
                window.google_conversion_label = gaData[i].label;
                window.google_remarketing_only = true;
                if (typeof(conv_handler) == 'function') {
                    conv_handler();
                }
            }
        }
        window.linkfire.retargetingPixels['google'].loader = function() {
            if (!checkScriptLoaded('advertiser','google')) {
                var googlescript = document.createElement('script');
                googlescript.src='//www.googleadservices.com/pagead/conversion_async.js';
                googlescript.type='text/javascript';
                googlescript.onload = function() {
                    conv_handler = window['google_trackConversion'];
                    TriggerGooglePixel();
                }
                document.getElementsByTagName('Body').item(0).appendChild(googlescript);
            }
        }
    </script>
<script>
    var lotameData = window.linkfire.retargetingPixels.lotame.tags;
    window.linkfire.retargetingPixels['lotame'].loader = function() {
        if (!checkScriptLoaded('advertiser','lotame')) {
            Object.keys(window.linkfire.retargetingPixels.lotame.pixels).forEach(function(key) {
                var lotamescript = document.createElement('script');
                lotamescript.src=window.linkfire.retargetingPixels.lotame.pixels[key];
                lotamescript.type='text/javascript';
                document.getElementsByTagName('Body').item(0).appendChild(lotamescript);
            });
        }
    }
</script>
    <script>
        window.linkfire.retargetingPixels['twitter'].loader = function() {
            if (!checkScriptLoaded('advertiser','twitter')) {
                var twitterscript = document.createElement('script');
                twitterscript.src='//platform.twitter.com/oct.js';
                twitterscript.type='text/javascript';
                twitterscript.onload = function() {
                    triggerTwitterIds(ids);
                }
                document.getElementsByTagName('Body').item(0).appendChild(twitterscript);
            }
        }
        function triggerTwitterIds(ids){
            if(window.twttr) {
                for(var i = 0; i < ids.length; i++){
                    twttr.conversion.trackPid(ids[i], { tw_sale_amount: 0, tw_order_quantity: 0 });
                }
			}
        }
        var ids = ['numgi'];
    </script>
<script>
    window.linkfire.retargetingPixels['twitteruniversal'].loader = function() {
        if (!checkScriptLoaded('advertiser', 'twitteruniversal')) {
            !function(e,n,u,a){e.twq||(a=e.twq=function(){a.exe?a.exe.apply(a,arguments):
            a.queue.push(arguments);},a.version='1',a.queue=[],t=n.createElement(u),
            t.async=!0,t.src='//static.ads-twitter.com/uwt.js',s=n.getElementsByTagName(u)[0],
            s.parentNode.insertBefore(t,s))}(window,document,'script');
            triggerTwitterUniversal('pageview');
        }
    }
    var twitterUniversalIds = ['o18kj'];
    function triggerTwitterUniversal(event){
        for(var i = 0; i < twitterUniversalIds.length; i++){
            twq('init',twitterUniversalIds[i]);
            twq('track',event);
        }
    }
</script>
