<script>
	function triggerPixels(player, eventlabel, eventshowid) {
        if (checkConsent('advertiser', 'facebook')) {
            // Send to facebook
            loadScript('advertiser', 'facebook');
            if(window._fbdata){
                if(player == 'player'){
                    window._fbdata.action = 'player';
                    fbq('trackCustom', 'LF', window._fbdata);
                }else{
                    window._fbdata.action = 'service';
                    window._fbdata.musicservice = player;
                    window._fbdata.eventlabel = eventlabel;
                    if (eventshowid != null) {
                        window._fbdata.eventshowid = eventshowid;
                    }
                    fbq('trackCustom', 'LF', window._fbdata);
                }
            }
        }
        if (checkConsent('advertiser', 'google')) {
            loadScript('advertiser', 'google');
            // Send to google adwords
            if (typeof(TriggerGooglePixel) == 'function') {
                if(window.google_custom_params){
                    if(player == 'player'){
                        window.google_custom_params.action = 'player';
                    }else{
                        window.google_custom_params.musicservice = player;
                        window.google_custom_params.action = 'service';
                        if (eventshowid != null) {
                            window.google_custom_params.eventshowid = eventshowid;
                        }
                    }
                }
                TriggerGooglePixel();
            }
        }
        if (checkConsent('advertiser', 'googleglobal')) {
            loadScript('advertiser', 'googleglobal');
            // Send to google adwords glboal tag
            if (typeof(triggerGlobalTags) == 'function') {
                if(window.googleTagData){
                    if(player == 'player'){
                        window.googleTagData.action = 'player';
                    }else{
                        window.googleTagData.musicservice = player;
                        window.googleTagData.action = 'service';
                        if (eventshowid != null) {
                            window.googleTagData.eventshowid = eventshowid;
                        }
                    }
                }
                triggerGlobalTags('conversion');
            }
        }
        // Send to adform
        if (checkConsent('advertiser', 'adform')) {
            loadScript('advertiser', 'adform');
            if (typeof(TriggerAfPixel) == 'function') {
                window.afParams = window.afParams + '|' + player;
                TriggerAfPixel();
            }
        }

        //Twitter Universaltag
        if (checkConsent('advertiser', 'twitteruniversal')) {
            loadScript('advertiser', 'twitteruniversal');
            if (typeof(triggerTwitterUniversal) == 'function') {
                if(player == 'player'){
                    triggerTwitterUniversal('player');
                }else{
                    triggerTwitterUniversal('service');
                }
            }
        }

        // Send to Snap
        if (checkConsent('advertiser', 'snap')) {
            loadScript('advertiser', 'snap');
            if(typeof(triggerSnap) == 'function'){
                if (player !== 'pageview' && player !== 'player') {
                    triggerSnap('service');
                } else {
                    triggerSnap(player);
                }
            }
        }

        //Send DoubleClick
        if (checkConsent('advertiser', 'doubleclick')) {
            loadScript('advertiser', 'doubleclick');
            if(typeof(trackDc) == 'function'){
                trackDc(player);
            }
        }

        //Send Adelphi
        if (checkConsent('advertiser', 'mediarithmics')) {
            loadScript('advertiser', 'mediarithmics');
            if(typeof(triggerAdelphi) == 'function'){
                if(window.adelphi_properties){
                    if(player != 'player'){
                        window.adelphi_properties.musicservice = player;
                        triggerAdelphi('service');
                    }else{
                        triggerAdelphi('player');
                    }
                }

            }
        }

        // Send to adello
        if (checkConsent('advertiser', 'adello')) {
            loadScript('advertiser', 'adello');
            if (typeof(triggerAdelloIds) == 'function') {
                //Set the correct metadata and specifiy the event id, 0: Pageview, 1: Service, 2: Player
                if(player == 'player'){
                    adelloData.action = 'player';
                    triggerAdelloIds(adelloIds, 2);
                }else{
                    adelloData.action = 'service';
                    adelloData.service = player;
                    triggerAdelloIds(adelloIds, 1);
                }
            }
        }
        if (checkConsent('advertiser', 'twitter')) {
            loadScript('advertiser', 'twitter');
                                var twServiceIds = ['numuc'];
            if(player != 'player'){
                triggerTwitterIds(twServiceIds);
            }
                                    var twPlayerIds = ['numub'];
            if(player == 'player'){
                triggerTwitterIds(twPlayerIds);
            }
                            }

		
            }
</script>

