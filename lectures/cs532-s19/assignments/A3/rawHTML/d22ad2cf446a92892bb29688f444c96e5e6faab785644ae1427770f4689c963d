<script type="text/javascript"  src="/_ui/desktop/common/js/checkout/mobile.numeric.plugin.js"></script>
<script type="text/javascript">
var recommendedAddressModal= false;
var siteid="agatha393386";
var vco422errorMessage = "<br>We are unable to process your request for Visa Checkout.<br> Please try again or continue with Standard Checkout.<br>";
var vco500errorMessage = "<br>We are unable to process your request for Visa Checkout.<br> Please try again or continue with Standard Checkout.<br>";
var allowGirlDelivery = true;
var shipOnlyFirstCalc = true;
if(allowGirlDelivery || $('#shipping').is(':checked') || $('#mobile_shipping').is(':checked')){
	shipOnlyFirstCalc = false;
}

 function onVisaCheckoutReady(){
		V.init({
			apikey: "L9IJQN3TRKF973WR5KXD21WVaJmRWxZWgtqQqYnje_G99936M",
			paymentRequest:{
				currencyCode: "USD",
				total: "",
				subtotal: "",
				countryCode: "US",
				logoUrl:"https://digitalcookie.girlscouts.org/_ui/desktop/common/img/GSUSA_servicemark_150x68.png",
				displayName:"Girl Scouts of the USA",
				billingCountries:["US"],
				customData: { "nvPair": [ {"name":"siteURLID", "value":"agatha393386"}]}
			},
			settings: {
				shipping: {
				  acceptedRegions: ["US"]
				 }
			},
			payment: {
			       cardBrands: [
			        "VISA","MASTERCARD","AMEX","DISCOVER"
			        ],
			       billingCountries:["US"]
			}
		});

		V.on("payment.success", function(payment)
		  {
			 $('.shippingDiv,.cookieInput,.cookieDonate').hide();
			  var formInput = $("#jsonResponse");
			  formInput.val(JSON.stringify(payment));
			  var visaForm = $("#visaform");
			  $.blockUI({ message: '<h1><img src="/_ui/desktop/common/img/loading_graphic.gif" /></h1>' });

				$.ajax(
						{
							url : visaForm.attr("action"),
							type : 'POST',
							data: visaForm.serialize(),
							dataType: 'json',
							statusCode: {
							     200: function(data) {
									  $(location).attr('href', "/scoutcheckout/agatha393386/payment?vco=true");
								 },
								 412: function(data) {
									  $.unblockUI();
									  $('#visaCheckoutError2').html($.parseJSON(JSON.stringify(data))['responseJSON']);
									  $('#visaCheckoutError2_mobile').html($.parseJSON(JSON.stringify(data))['responseJSON']);
	  								  $('.shippingDiv,.cookieInput,.cookieDonate').show();
								 },
								 422: function(data) {
									  
									  $.unblockUI();
									  $('#visaCheckoutError2').html(vco422errorMessage);
									  $('#visaCheckoutError2_mobile').html(vco422errorMessage);
									  $('.shippingDiv,.cookieInput,.cookieDonate').show();
								 },
								 500: function(data) {
									  $.unblockUI();
									  $('#visaCheckoutError2').html(vco500errorMessage);
									  $('#visaCheckoutError2_mobile').html(vco500errorMessage);
									  $('.shippingDiv,.cookieInput,.cookieDonate').show();
								 }
							}
						});			  
		  });
		V.on("payment.cancel", function(payment)
		  {// Nothing should happen if they close the box
			canClickOnVisa = true;
			});
		V.on("payment.error", function(payment, error)
		  { $('#visaCheckoutError').html("A technical error has occurred while attempting to contact Visa");
		  	canClickOnVisa = true;
		  });
}
 
$('.cookieInput, .cookieDonate').blockInput({ regex: '[0-9]' });
// To accommodate mobile styling - type increases padding
$(".cookieInput, .cookieDonate").removeAttr("type");
</script>

<form id="visaform" role="form" action="/scout/agatha393386/visa-checkout/success" method="post">
    <input id="jsonResponse" name="jsonResponse" type="hidden" value=""/>
<input type="hidden" name="_requestConfirmationToken"  value="06459b3959d909427837713d1dfc3e76f1eb4434"/></form>
<script>
	$(document).ready(function() {
		var a = 0
		$(".tabContainer").each(function (index) {
			a = index+1;
			//console.log("#tabContainer" + a);
			$("#tabContainer" + a).Tabs();
		});
		
		$(".cookieInput").keypress(function(event) {
			return isNumberKeywithBkSp(event.charCode);
		});
		
		$(".cookieDonate").keypress(function(event) {
			return isNumberKeywithBkSp(event.charCode);
		});
		
		$(".mobile_cookieInput").keypress(function(event) {
			return isNumberKeywithBkSp(event.charCode);
		});

		$(".mobile_cookieInput").keypress(function(event) {

		});

		$('.openImgDetLoad').on('click', function(e) {
             $('.cookieImgWrapper').html('');
             $('.cookieNutritionPlaceholder').html('');
             $('.cookieDetPlaceholder').html('');

             var idCookie =  $(this).attr('id');
             
             //capture id for mobile
              if (idCookie.indexOf('mobile_') > -1) {
            	 idCookie = idCookie.substring(idCookie.indexOf('_') + 1, idCookie.length)
             }
             
             for (var i = 0, len = cookieIMgsArray.length; i < len; i++) {
                     if (cookieIMgsArray[i].id === idCookie){
                          var img = $('<img class="center_img" alt="'+cookieIMgsArray[i].id+'  Image" src="'+ cookieIMgsArray[i].imageURl +'">');
                          img.appendTo('.cookieImgWrapper');

                          var imgNut = $('<img src="'+ cookieIMgsArray[i].imageURlNut +'" alt="'+cookieIMgsArray[i].id+'  Nutritional Info"  class="center_img" style="margin-bottom:120px;"/>');
                          imgNut.appendTo('.cookieNutritionPlaceholder');

                          var imgDet = $('<img src="'+ cookieIMgsArray[i].imageURlDet +'"  alt="'+cookieIMgsArray[i].id+'" class="center_img"/>');
                          imgDet.appendTo('.cookieDetPlaceholder');

                          return false;
                    }
                }

         });
		
		


		//$("#tabContainer1, #tabContainer2, #tabContainer3, #tabContainer4, #tabContainer5, #tabContainer6, #tabContainer7, #tabContainer8").Tabs();
		//$("#tabContainer2").Tabs();
	//Start: Defect Fix for GSA-181 by IDC
		$(".close").click(function() {			
			var current = parseInt($.trim($("#closeModal").html()));
			//Start Defec fix GSA-1115 by Obika
			// var old = current - 1;
			
			var old;
			
			if (current % 2 == 0){
				old = current - 1;
			}
			else {
				old = current;
			}
			
			//end fix GSA-1115

			document.getElementById("tabHeader" + current).removeAttribute("class");
			document.getElementById("tabpage" + current).style.display="none";
			document.getElementById("tabpage" + old).removeAttribute("style");
			document.getElementById("tabHeader" + old).setAttribute("class","tabActiveHeader");
							
			//alert($("li#tabHeader" + current).closest('ul').data("current"));
			$("li#tabHeader" + current).parent('ul').removeData("current");
			$("li#tabHeader" + old).parent('ul').attr("data-current", old);						
		});
		
		
	//End: Defect Fix
	
	if ('' == 'true') {
	 $(".getShippingAdjust").click();
	}
	}); 
</script>
<!-- end center content here -->
					
