<script type="text/javascript">
	$(document).ready(function() {
		var height = screen.height;
		var width = screen.width;
		if (width >= 320 && width <= 768) {			
			$("#displayVideo iframe").attr("width", "301");
			$("#displayVideo iframe").attr("height", "169");
		}
	});
	$('.getVideo, .getShippingAdjust').magnificPopup({
		type: 'inline',
		alignTop: false,
		overflowY: 'scroll',
		closeOnContentClick: false,
		closeOnBgClick: false
	});
	$("#approve").click(function() {
		var goalMsg = $('#goalErrorMsg').val();
		var lessonMsg = $('#lessonErrorMsg').val();
		if (goalMsg.length > 0 || lessonMsg.length > 0) {
			$('#goalPreview').html($('#goalPreviewErrorMsg').html());
			$('#goalPreview').show();
			return false;
		} else {
			$("#girlGoalApproveForm").submit();
			return true;
		}
	})
</script><div class="width_260px girlInfo whiteBackGround padding_20B">
