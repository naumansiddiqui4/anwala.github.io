<script>
;(function(){
    var numBoxes = parseInt($('span.openSansReg_28').html(), 10);

    if(numBoxes > 9999 && numBoxes < 99999){
    $('#goalArrow').attr('style', 'margin-top: -8px');
        $('span.openSansReg_28').attr('style', 'font-size: 25px');
    }else if(numBoxes > 99999){
        $('#goalArrow').attr('style', 'margin-top: -5px');
        $('span.openSansReg_28').attr('style', 'font-size: 20px');
    }

    if(window.innerWidth < 959 && numBoxes < 9999){
        $('#goalArrow').attr('style', 'margin-top: 5px; display: block; float: left; padding-right: 3px;');
        $('span.openSansReg_28').attr('style', 'font-size: 20px');
    }

    $(window).on('resize', function(e){
        e.preventDefault();
        if(window.innerWidth < 959 && numBoxes < 9999){
            $('#goalArrow').attr('style', 'margin-top: 5px; display: block; float: left; padding-right: 3px;');
            $('span.openSansReg_28').attr('style', 'font-size: 20px');
        }else if(numBoxes > 99999){
            $('#goalArrow').attr('style', 'margin-top: -5px');
            $('span.openSansReg_28').attr('style', 'font-size: 20px');
        }else{
            $('span.openSansReg_28').attr('style', 'font-size: 25px');
        }
    })
})()
</script>
