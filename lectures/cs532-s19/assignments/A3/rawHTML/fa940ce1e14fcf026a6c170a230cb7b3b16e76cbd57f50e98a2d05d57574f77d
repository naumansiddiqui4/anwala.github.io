<script type="text/javascript">
  function changeTopiGnr(genre){
    document.getElementById("tpc_list_main").classList.remove("current");
    document.getElementById("tpc_list_entertainment").classList.remove("current");
    document.getElementById("tpc_list_domestic").classList.remove("current");
    document.getElementById("tpc_list_sports").classList.remove("current");
    document.getElementById("tpc_list_international").classList.remove("current");
    document.getElementById("tpc_list_economy").classList.remove("current");
    document.getElementById("tpc_list_it").classList.remove("current");
    document.getElementById("tpc_list_trend").classList.remove("current");
    document.getElementById("tpc_list_animal").classList.remove("current");
    
    document.getElementById("tpc_navi_main").classList.remove("current");
    document.getElementById("tpc_navi_entertainment").classList.remove("current");
    document.getElementById("tpc_navi_domestic").classList.remove("current");
    document.getElementById("tpc_navi_sports").classList.remove("current");
    document.getElementById("tpc_navi_international").classList.remove("current");
    document.getElementById("tpc_navi_economy").classList.remove("current");
    document.getElementById("tpc_navi_it").classList.remove("current");
    document.getElementById("tpc_navi_trend").classList.remove("current");
    document.getElementById("tpc_navi_animal").classList.remove("current");
    
    document.getElementById("tpc_navi_"+ genre).classList.add("current");
    document.getElementById("tpc_list_"+ genre).classList.add("current");
  }
</script>

