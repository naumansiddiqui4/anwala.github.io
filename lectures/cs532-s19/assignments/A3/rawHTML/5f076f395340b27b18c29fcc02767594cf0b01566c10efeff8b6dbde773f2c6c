<script type="text/javascript">
    var spMenu = [];
    
    spMenu[0] = {menuid:0, menutype:''};
    
    spMenu[1] = {menuid:-2, menutype:''};
    
    spMenu[2] = {menuid:65, menutype:'B'};
    
    spMenu[3] = {menuid:67, menutype:'B'};
    
    spMenu[4] = {menuid:40, menutype:'B'};
    
    spMenu[5] = {menuid:51, menutype:'B'};
    
    spMenu[6] = {menuid:55, menutype:'B'};
    
    spMenu[7] = {menuid:39, menutype:'B'};
    

	
	
	
		function selectSpecialMenu(menuid)  {
			for (var i=0; i<spMenu.length; i++) {
				try {
					$('special-menu-' + spMenu[i].menuid).className='default';
					$('special-menuLink-' + spMenu[i].menuid).className='off';
				} catch(e){}
			}

			try {
				$('special-menu-' + menuid).className='selected';
				$('special-menuLink-' + menuid).className='on';
			} catch(e){}
		}
	
	
    
	function alertBadMenu() {
		alert('접근 제한 중인 게시판입니다.');
	}    
</script>
					
				
			
		
			
				
				

				
				
					
						





