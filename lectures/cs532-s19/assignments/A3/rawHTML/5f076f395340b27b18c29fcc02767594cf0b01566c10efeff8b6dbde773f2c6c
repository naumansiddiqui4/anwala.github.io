<script type="text/javascript">

    // 접속멤버보기 토글
    function toggleLM()
    {
        if($('lm-list').style.display=="none")
        {   
            $("lmtoggleimg").className = "member-up"; 
            $("lmtoggleimg").alt = "접속멤버 닫기"; 
            Element.show("lm-list");
        }
        else
        {
            $("lmtoggleimg").className = "member-down";
            $("lmtoggleimg").alt = "접속멤버 열기"; 
            Element.hide("lm-list");
        }       
    }
    
    // 새로고침
    function refreshLinkedMember()
    {                
        try{
            oLinkedMember.getLinkedMember();
        }catch(e){
            try{
                parent.oLinkedMember.getLinkedMember();
            }catch(ee){}
        }
    }
    
     LH.add("setTimeout('refreshLinkedMember()',400)");

    
</script>

						
						
					
						





