		<script type="text/javascript">
            function exposeToolTip(tagElement) {
                var elParent = tagElement.parentNode,
                    nTop = Element.realPos(elParent).top + elParent.offsetHeight,
                    nLeft = Element.realPos($("cafe-menu")).left,
                    elLayer = $("menu_restirction_tip");

                if(elLayer.parentNode != document.body) {
                    document.body.appendChild(elLayer);
                }

                Element.setCSS(elLayer, {
                    "top" : nTop + "px",
                    "left" : nLeft + "px",
                    "display" : "block"
                });
            }

            function closeToolTop() {
                var fktip = document.getElementById("menu_restirction_tip");
                fktip.style.display = "none";
            }
		</script>

	


				
