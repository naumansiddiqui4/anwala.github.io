    function suggestJoin() {
        

        var clubname = "취업 - Cheer Up!";
        if (confirm('카페멤버만 이용할 수 있는 기능입니다.\n' + clubname.replace(/&lt;/g, "<").replace(/&gt;/g, ">") + ' 카페에 가입하시겠습니까?')) {
            joinCafe();
        }
    }
    

    
    
