<img id="rankMemberlcsTag" src="about:blank"  height="0" style="display:none">
<script type="text/javascript">

var rankMemberType = new Array("comment","article","visit");
var isGetRankMembercomment = true;
var isGetRankMemberarticle = false;
var isGetRankMembervisit = false;

// 지난주 멤버랭킹 탭 전환
function showRankMember(type)
{	
    callRankMember(type);
}

// 멤버랭킹 조회
function callRankMember(type)
{
	if(! eval("isGetRankMember"+type) )
	{	
		var ajax = new Ajax("/RankMember.nhn?clubid=27489327&type="+type+"&viewcount=-1", {
				suspend : "false",				
				onLoad : rankMemberCB
		});
		ajax.request();
	}else
	{
        $("rank-member").innerHTML = $("rankmember-"+type).innerHTML;
        try{
        	var etc = {};
        	etc["sti"] = "cafe_rankmember";
        	lcs_do(etc);
        }catch(e) {} 
	}
}

// 멤버랭킹 콜백
function rankMemberCB(res)
{
	if(res.status==200)
	{
		var type = res.getResponseHeader("ranktype");
		var viewcount = res.getResponseHeader("viewcount")
		
		$("rankmember-"+type+"-list").innerHTML = res.responseText;
		eval("isGetRankMember"+type+"=true");
        $("rank-member").innerHTML = $("rankmember-"+type).innerHTML;
        
        try{
        	var etc = {};
        	etc["sti"] = "cafe_rankmember";
        	lcs_do(etc);
        }catch(e) {} 
    }else
    {
       alert("멤버랭킹 조회를 실패했습니다.");
    }
}

 function firstCommentRankList()
  { 
     if($("first-comment-list").style.display=="none")
     {	
	  Element.show("first-comment-list");
	  Element.hide("second-comment-list");
	  Element.show("pre-membercomment-disable");
	  Element.show("next-membercomment-useable");
	  Element.hide("pre-membercomment-useable");
	  Element.hide("next-membercomment-disable");
	}
  }
  
  function secondCommentRankList()
  {  
     if($("second-comment-list").style.display=="none")
     {
	  Element.show("second-comment-list");
  	  Element.hide("first-comment-list");
  	  Element.show("pre-membercomment-useable");
	  Element.show("next-membercomment-disable");
	  Element.hide("pre-membercomment-disable");
	  Element.hide("next-membercomment-useable");
  	  
	}
  }
  
  function firstArticleRankList()
  { 
     if($("first-articlerank-list").style.display=="none")
     {	
	  Element.show("first-articlerank-list");
	  Element.hide("second-articlerank-list");
	  Element.show("pre-articlerank-disable");
	  Element.show("next-articlerank-useable");
	  Element.hide("pre-articlerank-useable");
	  Element.hide("next-articlerank-disable");
	}
  }
  
  function secondArticleRankList()
  {  
     if($("second-articlerank-list").style.display=="none")
     {
	  Element.show("second-articlerank-list");
  	  Element.hide("first-articlerank-list");
  	  Element.show("pre-articlerank-useable");
	  Element.show("next-articlerank-disable");
	  Element.hide("pre-articlerank-disable");
	  Element.hide("next-articlerank-useable");
  	  
	}
  }
  
  function firstVisitRankList()
