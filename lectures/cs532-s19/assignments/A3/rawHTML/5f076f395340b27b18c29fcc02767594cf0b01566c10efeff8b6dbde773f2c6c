<script type="text/javascript">
/*

    Author : ilzzang (Yu Il Sang) -07/11/16-
    		 Updated By Jeon 0ja  -08/05/06-

*/
 /** @id document.getElementByClassName ie5.5 bug fix */
document.getElementsByClassName2 = function(className, oParent) {
	var a = [].load(($(oParent).all) || ($(oParent) || document.body).getElementsByTagName('*'));
	var r = new RegExp('(^|\\s)'+className+'($|\\s)','gi');
	return a.filter(function(v){ return r.test(v.className); });
};
var HAPPYBEAN_API_URL      = "https://api.happybean.naver.com";
var HAPPYBEAN_FUND_URL     = "http://fund.happybean.naver.com";
var HAPPYBEAN_MY_URL       = "http://happybean.naver.com";
var HAPPYBEAN_DONATION_URL = "http://happybean.naver.com";
var widgetHappybean = Class({
    __init : function(opt) {
        this.opt=opt;
        this.$_widgetHappybean = $("widget-bean");
        this.$_layerPop        = $("put_list");
        this.sAjax    = "happybeanScriptInclude";
        this.sAjaxUrl = HAPPYBEAN_API_URL +"/fund/FundInfoWidget.nhn";
		this.isEnabled = false;
		this.bUserAdmin = false;

		this.$_Agent   = $Agent();
        this.iFundNo   = null;
        this.iLayerPos = {};
        this.getLayerPopupPos();
        this.$_layerPop.style.display = "none";

        // 이벤트 바인딩
        this.onClick_HappybeanStop     = this._happybeanStop.bind(this);
		this.onClick_HappybeanStop2    = this._happybeanDonation.bind(this);
		this.onClick_HappybeanRun      = this._happybeanDonationLink.bind(this);
        this.onClick_HappybeanSave     = this._happybeanSave.bind(this);
        this.onClick_HappybeanIE55Link = this._ie55link.bindForEvent(this);
        this.onClick_HappybeanPut      = this._happybeanPut.bindForEvent(this);
        this.onClose_HappyPopLayer     = this._LayerClose.bindForEvent(this);

        // 담아가기 레이어 링크
        var oLink = this.$_layerPop.getElementsByTagName("a");

		Event.register(oLink[0],"click",this._link.bindForEvent(this));
		Event.register(oLink[1],"click",this._link.bindForEvent(this));
        this.setCursor(oLink[0]);
        this.setCursor(oLink[1]);

        this.setConnectApiServer();
    },
	setCursor : function(o) {
		var mycursor = this.$_Agent.IE ? "hand" : "pointer";
		Element.setCSS(o,{cursor:mycursor});
	},
    getLayerPopupPos : function() {
        // 팝업 좌표
        var pos = Element.realPos(this.$_layerPop);
        this.iLayerPos = {
            x1 : pos.left,
            y1 : pos.top - 5,
            x2 : pos.left + 106,
            y2 : pos.top + 81
        }
    },
    setConnectApiServer : function() {
        var t = this;

        if(typeof(this.$_widgetHappybean.callback=="undefined")) {
            this.$_widgetHappybean.callback=function(v) {
                t.isEnabled = true;
                t.setConnectDone(v); // 활성화
                if($(t.sAjax)) t.$_widgetHappybean.removeChild($(t.sAjax));
            }
        }

        if($(this.sAjax)==null) {
            var oEl_DynamicScriptLoading     = document.createElement("script");
            oEl_DynamicScriptLoading.id      = this.sAjax;
            oEl_DynamicScriptLoading.charset = "euc-kr";
            this.$_widgetHappybean.appendChild(oEl_DynamicScriptLoading);
        }


        var scriptLoad = function() {
        	t.setConnectDone(); // 비활성화
           	$(t.sAjax).src = t.sAjaxUrl+"?callback=$('"+t.$_widgetHappybean.id+"').callback&info=cafe&sid=27489327&outputtype=proxy&enctype=euckr&fundno=";
		};

        Event.register(window,"load",scriptLoad);
    },
	chgShortitle : function(v) {
		var ret = "";
		var el  = document.createElement("div");
		el.innerHTML = v;
		ret = typeof(el.innerText)=="undefined" ? el.textContent : el.innerText;
		el = null;

		return ret;
	},
    setConnectDone : function(v) {
        var obj  = v;
        try {
            var oFund = obj.happybean.result.list[0].fund;
            var oHappyTitle = document.getElementsByClassName2("happy_title",this.$_widgetHappybean)[0]; // 해피빈(타이틀)
            var oCollectamount = document.getElementsByClassName2("gather",this.$_widgetHappybean)[0]; // 모은콩
            var oCollectcount = document.getElementsByClassName2("participation",this.$_widgetHappybean)[0]; // 참여횟수
            var oA=document.getElementsByClassName2("bean-btns",this.$_widgetHappybean)[0].getElementsByTagName("a");

			var oDonation = null;
			var _oDonation = document.getElementsByClassName2("admin_area",this.$_widgetHappybean)[0];
			oDonation=_oDonation.getElementsByTagName("a");


			/*if(this.opt.bUser != oFund.creatorid) { // 카페(CAFE-2076) : 펀드를 만든 사용자와 위임된 매니져 사용자와 다를경우!
				_oDonation.style.display = "none";
			}*/

			if(this.opt.bUser != oFund.creatorid) { // 카페(CAFE-2076)와 동일 : 펀드를 만든 사용자와 위임된 매니져 사용자와 다를경우!
				_oDonation.style.display = "none";
				this.bUserAdmin = false;
			}else if(this.opt.bUser == oFund.creatorid){
				_oDonation.style.display = "block";
				this.bUserAdmin = true; // 콩저금통 개설한 사람일 경우!
			}


            this.iFundNo = oFund.fundno;
			this.$_widgetHappybean.title = this.chgShortitle(oFund.shorttitle);
            oCollectamount.innerHTML 	= oCollectamount.innerHTML + oFund.collectamount;
            oCollectcount.innerHTML 	= oCollectcount.innerHTML + oFund.collectcount;
            // 해피빈 위젯 추가되면서 기능 삭제됨.
            oHappyTitle.innerHTML        = "<a href='"+HAPPYBEAN_FUND_URL+"/"+this.iFundNo+"' target='_blank'>"+oFund.shorttitle+"</a>";


            // 콩 잠금상태일 경우
            if(oFund.fundstatus!=1) {
				if(this.$_Agent.IE55 == true) {
					Event.register(oA[0],"click",this.onClick_HappybeanIE55Link);
					Event.register(oA[1],"click",this.onClick_HappybeanIE55Link);
					Event.register(oDonation[0],"click",this.onClick_HappybeanIE55Link);
				}else{
					Event.register(oA[0],"click",this.onClick_HappybeanStop);
					Event.register(oA[1],"click",this.onClick_HappybeanStop);
					Event.register(oDonation[0],"click",this.onClick_HappybeanStop2);
				}

				this.setCursor(oDonation[0]);

				this._iconDisabled(); // 아이콘 비활성화
				this.$_widgetHappybean.title = "이 저금통은 개설자의 요청으로 저금이 잠시 중단된 상태입니다.\n\n저금 중단상태에서는 저금하기, 담아가기가 불가합니다.";
            }else{
				// IE55 는 버튼이 적용 되지 않아야한다!
				if(this.$_Agent.IE55 == true) {
					Event.register(oA[0],"click",this.onClick_HappybeanIE55Link);
					Event.register(oA[1],"click",this.onClick_HappybeanIE55Link);
				} else {
					Event.register(oA[0],"click",this.onClick_HappybeanSave);
					Event.register(oA[1],"click",this.onClick_HappybeanPut);
				}

				if(oDonation!=null) {
					if(this.$_Agent.IE55 == true) {
						Event.register(oDonation[0],"click",this.onClick_HappybeanIE55Link);
						this.setCursor(oDonation[0]);
					}else{
						Event.register(oDonation[0],"click",this.onClick_HappybeanRun);
						this.setCursor(oDonation[0]);
					}
				}
				this._iconEnabled(); // 아이콘 활성화
            }

			this.setCursor(oA[0]);
			this.setCursor(oA[1]);
        }catch(e) {
			this._iconDisabled(); // 아이콘 비활성화
            this.$_widgetHappybean.title = "일시적으로 표시할 수 없습니다";
        }
    },
	_iconEnabled : function() {
		var alink=document.getElementsByClassName2("bean-btns",this.$_widgetHappybean)[0].getElementsByTagName("a");

		// 활성화
		for(var i=0;i<alink.length;i++) {
			if(alink[i].className.indexOf("-off")>-1) {
				alink[i].className=alink[i].className.replace("-off","");
			}
		}
	},
	_iconDisabled : function() {
		
		var alink=document.getElementsByClassName2("bean-btns",this.$_widgetHappybean)[0].getElementsByTagName("a");

		// 비활성화
		for(var i=0;i<alink.length;i++) {

			if(alink[i].className.indexOf("-off")==-1) {
				//alink[i].className=alink[i].className.replace(/(btn-[\S]*)(.gif$)/,"$1-off$2");
				alink[i].className=alink[i].className + "-off";
			}
		}
        
	},
	_ie55link : function(e) {
		var el              = e.element;
		var parentClassName = el.parentNode.className;
		var title           = (parentClassName == "bean-btns" ? el.getElementsByTagName("span")[0].title : el.title);
		var msg             = "";

		if(title == "저금하기") {
			msg = "저금하기";
		}else if(title == "담아가기") {
			msg = "담아가기";
		}else if(title == "콩저금통 기부하기") {
			msg = "기부하기";
		}
		alert("죄송합니다. IE 5.5에서는 "+msg+"를 할 수 없습니다.\n\n추후 지원 예정입니다.");
	},
	_link : function(e) {
		var url = "";
		var s = e.element.getAttribute("gid");
		if(s == "1") {
            
			url = HAPPYBEAN_FUND_URL+"/scrap/FundScrap.nhn?fundno="+this.iFundNo+"&info=blog";
		}else if(s == "2") {
			url = HAPPYBEAN_FUND_URL+"/scrap/FundScrap.nhn?fundno="+this.iFundNo+"&info=cafe";
		}

		this.doPopup({
			url : url,
			width : "400",
			height : "267"
		});

		this._LayerClose();
	},
    _happybeanPut : function(e) {
		e.stop();
		this.doLayerOpen();
		/*
		if(this.bUserAdmin == true) {
			alert("관리자는 담을수 없습니다.");
		}else{
			this.doLayerOpen();
		}
		*/
    },
    doLayerOpen : function(e) {
        Element.setCSS(this.$_layerPop,{display : "block",marginBottom : "-21px"});
        Event.register(document.body,"click",this.onClose_HappyPopLayer);
    },
    _LayerClose : function(e) {
        if(typeof(e)=="undefined") {
            Element.setCSS(this.$_layerPop,{display : "none"});
            return;
        }

        var pos = this.iLayerPos;
        var e_x  = e.page_x;
        var e_y  = e.page_y;

        if(pos.x1 < e_x &&
            pos.y1 < e_y &&
            pos.x2 > e_x &&
            pos.y2 > e_y
        ) {

            return;
        }

        Element.setCSS(this.$_layerPop,{display : "none"});
        Event.unregister(document.body,"click",this.onClose_HappyPopLayer);
    },
    _happybeanStop : function() {
        if(this.bUserAdmin == true) {
            if(confirm("이 저금통을 중단하셨습니다.\n\n저금하기, 담아가기, 기부하기를 하려면 저금통을 진행중\n\n상태로 변경하셔야 합니다.\n\nMY 해피빈에서 저금통 설정을 변경하시겠습니까?")) {
                this.doNewWindow(HAPPYBEAN_MY_URL+"/donation/DonateFundSummary.nhn");
            }else{
                return false;
            }
        }else{
			this.$_widgetHappybean.title = "이 저금통은 개설자의 요청으로 저금이 잠시 중단된 상태입니다.\n\n저금 중단상태에서는 저금하기, 담아가기가 불가합니다.";
            alert("이 저금통은 개설자의 요청으로 저금이 잠시 중단된 상태입니다.\n\n저금 중단상태에서는 저금하기, 담아가기가 불가합니다.");
        }
    },
	_happybeanDonationLink : function() {
    	this.doNewWindow(HAPPYBEAN_DONATION_URL+"/donation/ThemeServiceMain.nhn");
	},
	_happybeanDonation : function() {
        if(this.bUserAdmin == true) {
            if(confirm("이 저금통을 중단하셨습니다.\n\n저금하기, 담아가기, 기부하기를 하려면 저금통을 진행중\n\n상태로 변경하셔야 합니다.\n\nMY 해피빈에서 저금통 설정을 변경하시겠습니까?")) {
                //this.doNewWindow(HAPPYBEAN_DONATION_URL+"/cong/RdonaThemeList.nhn");
				this.doNewWindow(HAPPYBEAN_MY_URL+"/donation/DonateFundSummary.nhn");
            }else{
                return false;
            }
        }else{
			this.$_widgetHappybean.title = "이 저금통은 개설자의 요청으로 저금이 잠시 중단된 상태입니다.\n\n저금 중단상태에서는 저금하기, 담아가기가 불가합니다.";
            alert("이 저금통은 개설자의 요청으로 저금이 잠시 중단된 상태입니다.\n\n저금 중단상태에서는 저금하기, 담아가기가 불가합니다.");
        }
	},
    _happybeanSave : function() {
        this.doPopup({
            url : HAPPYBEAN_FUND_URL+"/deposit/FundDepositPopupForm.nhn?fundno="+this.iFundNo+"&info=cafe&clubid=27489327&step=18&themeDir1Id=20&themeDir2Id=334&areaDir1Id=-1&areaDir2Id=-1",
            width : "400",
            height : "320"
        });
    },
    doPopup : function(opt) {
        var width  = opt.width;
        var height = opt.height;
        var happybeanPopup = window.open(
            opt.url,
            'happybean',
            'width='+opt.width+',height='+opt.height+',directories=no,location=no,menubar=no,scrollbars=yes,status=no,toolbar=no,resizable=no'
        );

        happybeanPopup.focus();
    },
    doNewWindow : function(url) {
        var happybeanWindow = window.open(
            url,
            'happybean',
            'directories=yes,location=yes,menubar=yes,scrollbars=yes,status=yes,toolbar=yes,resizable=yes'
        );

        happybeanWindow.focus();
    }
});

new widgetHappybean({
    bUserAdmin : false,
    bUser : ''
});

</script>

						
						
							
						
					
						


