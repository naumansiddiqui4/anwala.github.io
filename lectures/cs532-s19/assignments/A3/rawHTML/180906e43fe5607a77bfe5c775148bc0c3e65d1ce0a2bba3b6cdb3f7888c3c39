function fnSNS(snsName) {
	
	var url = "";

	if (snsName == "twitter") {
		url = "http://twitter.com/share?text=" + encodeURIComponent('하성운 (HA SUNG WOON) 공식 팬클럽 1기 모집 티켓오픈 안내' + " " + ""  + " " + 'http://ticket.interpark.com/webzine/paper/TPNoticeView.asp?bbsno=34&no=33927&groupno=33927');
	} else if (snsName == "facebook") {
		//기존 url
		//url = "http://www.facebook.com/sharer.php?s=100&p[url]="+encodeURIComponent('http://ticket.interpark.com/webzine/paper/TPNoticeView.asp?bbsno=34&no=33927&groupno=33927')+"&p[title]="+encodeURIComponent('하성운 (HA SUNG WOON) 공식 팬클럽 1기 모집 티켓오픈 안내')+"&p[summary]=&p[images][0]="+encodeURIComponent("http://ticketimage.interpark.com/Play/image/large/19/19001805_p.gif");
		//2014.05.13 변경
		url = "https://www.facebook.com/dialog/feed?app_id=119271631481944&display=popup&caption="+encodeURIComponent(' ')+"&name="+encodeURIComponent("인터파크 티켓")+"&link="+encodeURIComponent('http://ticket.interpark.com/webzine/paper/TPNoticeView.asp?bbsno=34&no=33927&groupno=33927')+"&description="+encodeURIComponent('하성운 (HA SUNG WOON) 공식 팬클럽 1기 모집 티켓오픈 안내')+"&picture="+encodeURIComponent('http://ticketimage.interpark.com/Play/image/large/19/19001805_p.gif')+"&redirect_uri=http://ticket.interpark.com/community/sns/FBClose.asp"		
	}

	var snsPop = window.open(url, snsName, 'top=200,left=200,width=800,height=500,resizable=yes,scrollbars=yes');
	
}

</script>

