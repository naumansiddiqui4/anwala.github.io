<script type="text/javascript">
	jQuery.noConflict();
	var head$ = jQuery;

	// Ajax 공통
	var AJAXCom = {
		Update : function(url, param, target) {
			head$.ajax({
				type:"POST",
				url: url,
				data: param,
				success:function(result) {
					 head$("#"+target).html(result);
				},
				error:function(result) {
					//alert('검색상위오류');
				}
			});
		}
	}

	if("" != "N")
	{
		AJAXCom.Update('/Include/TPRightWing_New.asp?Type=openWing', '', 'NewrightWing');
	}

	//UI 바인딩
	function fnBind(gargetName, tmplName, dataValue) {


		fnDeleteData(dataValue);		

		try{
			head$(gargetName).html( head$(tmplName).render(dataValue));
		}catch (ex){
		//	alert(ex);
		}	
	}

	// 배열검색 금일날짜 아닌거 지우기
	function fnDeleteData(vData)
	{
		try{
			var index;
			for (index =0; index < vData.items.length; ++index)
			{
				if (fnPeriodYN(vData.items[index].StartDate, vData.items[index].EndDate) == "false")
				{
					vData.items.splice(index,1);
					index--;
				}
			}		
		}
		catch (e){}
	}

	
// 노출시간 제어
function fnPeriodYN(startdate, enddate) {

	try
	{
		if(enddate == '') {
			enddate = '999912310000';
		}

		var now = new Date();   //현재시간

		year = now.getFullYear();   //현재시간 중 4자리 연도
		month = now.getMonth()+1;   //현재시간 중 달. 달은 0부터 시작하기 때문에 +1 

		if((month+"").length < 2){
			month="0"+month;   //달의 숫자가 1자리면 앞에 0을 붙임.
		}

		date = now.getDate();      //현재 시간 중 날짜.
		if((date+"").length < 2){
			date="0"+date;      
		}

		hour = now.getHours();   //현재 시간 중 시간.
		if((hour+"").length < 2){
			hour="0"+hour;      
		}

		minutes = now.getMinutes();   //현재 시간 중 분.
		if((minutes+"").length < 2){
			minutes="0"+minutes;      
		}

		today = year + "" + month + "" + date+ "" +hour+ "" +minutes;      //오늘 날짜 완성.

		// 시간비교
		if (  
				( eval(today) > eval(startdate) ) && ( eval(today) < eval(enddate) )
		)  {  
			return "true";
		}
		else {

			return "false";
		}
	}
	catch (e)
	{
		return false;
	}
}



	//GNB 배너
	try{fnBind("#target_gnbbanner", "#tmpl_gnbbanner", data_gnbbanner);}catch(e){}

	// 메뉴추천 뮤지컬

	try{fnBind("#target_gnbmenumusical_title", "#tmpl_gnbmenumusical_title", data_gnbmenunewmusical_big);}catch (ex){}			
	try{fnBind("#target_gnbmenumusical", "#tmpl_gnbmenugoods", data_gnbmenunewmusical);}catch(e){}						
	try{fnBind("#target_gnbmenumusical_big", "#tmpl_gnbmenugoods_big", data_gnbmenunewmusical_big);}catch(e){}		

	// 메뉴추천 콘서트
	try{fnBind("#target_gnbmenuconcert_title", "#tmpl_gnbmenuconcert_title", data_gnbmenunewconcert_big);}catch (ex){}	
	try{fnBind("#target_gnbmenuconcert", "#tmpl_gnbmenugoods", data_gnbmenunewconcert);}catch(e){}
	try{fnBind("#target_gnbmenuconcert_big", "#tmpl_gnbmenugoods_big", data_gnbmenunewconcert_big);}catch(e){}

	// 메뉴추천 연극
	try{fnBind("#target_gnbmenudrama_title", "#tmpl_gnbmenudrama_title", data_gnbmenunewdrama_big);}catch (ex){}		
	try{fnBind("#target_gnbmenudrama", "#tmpl_gnbmenugoods", data_gnbmenunewdrama);}catch(e){}
	try{fnBind("#target_gnbmenudrama_big", "#tmpl_gnbmenugoods_big", data_gnbmenunewdrama_big);}catch(e){}

	// 메뉴추천 클래식무용
	try{fnBind("#target_gnbmenuclassic_title", "#tmpl_gnbmenuclassic_title", data_gnbmenunewclassic_big);}catch (ex){}		
	try{fnBind("#target_gnbmenuclassic", "#tmpl_gnbmenugoods", data_gnbmenunewclassic);}catch(e){}
	try{fnBind("#target_gnbmenuclassic_big", "#tmpl_gnbmenugoods_big", data_gnbmenunewclassic_big);}catch(e){}

	// 메뉴추천 스포츠
	try{fnBind("#target_gnbmenusports_title", "#tmpl_gnbmenusports_title", data_gnbmenunewsports_big);}catch (ex){}	
	try{fnBind("#target_gnbmenusports", "#tmpl_gnbmenugoods", data_gnbmenunewsports);}catch(e){}
