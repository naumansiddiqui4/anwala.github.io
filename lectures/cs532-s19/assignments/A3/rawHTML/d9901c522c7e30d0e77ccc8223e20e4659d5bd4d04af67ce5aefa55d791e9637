      <div id="visual">
        <div class="visual__inner">
          <div class="visual__content">
            <div class="tagline">
              <span class="tagline__top">10 QUESTIONS</span>
              <span class="tagline__bottom">to know about</span>
            </div>
            <div class="actor__name">
              <img src="assets/images/name.svg" alt="RYUNOSUKE KAMIKI">
            </div>
          </div>
          <img src="assets/images/kv.jpg" alt="" class="kv" data-object-fit="cover">
        </div>
