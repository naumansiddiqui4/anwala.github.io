<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
<script>
(function($){$(document).ready(function(){af();});
function af(){
var a,ah,b,bh,c,cb,ch,l,p,t,w,wt,wl,wh,pa,pt,pl,nf,s,r;
a=$('#gad-fixed');b=$('#sub');c=$('#main');p='static';
ah=600+30+45;t=a.offset().top+100;w=$(window);l=b.offset().left+0;
pa=b.offsetParent();pt=pa.offset().top;pl=pa.offset().left;
s=function(){
bh=b.height();ch=c.height();cb=c.offset().top+ch;
wt=w.scrollTop();wl=w.scrollLeft();wh=w.height();
nf=(wt>t)&&(ch>bh)?true:false;p=!nf?'static':(wt+ah)>cb?'absolute':'fixed';
if(p==='fixed'){b.css({position:p,top:'',bottom:wh-ah,left:l-wl});}
else if(p==='absolute'){b.css({position:p,top:cb-bh-pt,bottom:'',left:l-pl});}
else{b.css({position:p});}
};
r=function(){
b.css({position:'static'});wl=w.scrollLeft();l=b.offset().left+0;pl=pa.offset().left;
if(p==='fixed'){b.css({position:p,left:l-wl});}
else if(p==='absolute'){b.css({position:p,left:l-pl});}
};
w.on('load',s);w.on('scroll',s);w.on('resize',r);}})(jQuery);
</script>








