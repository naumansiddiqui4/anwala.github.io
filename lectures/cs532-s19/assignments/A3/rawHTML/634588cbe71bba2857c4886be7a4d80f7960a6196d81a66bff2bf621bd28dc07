            <script src="http://parts.blog.livedoor.jp/js/anime.min.js?_v="></script>
            <script src="http://parts.blog.livedoor.jp/js/comment2/heart_animation.js?_v="></script>
<script>
var __comment_animation = new Animation.Hover('.comment-like');
if (!window['jQuery']) { document.write('<script src="http://parts.blog.livedoor.jp/js/jquery-1.7.1.min.js"><\/script>'); }
jQuery(window).on('message', function(event) {
  if (event.originalEvent.origin !== 'http://comment.blogcms.jp') { return; }
  var data = JSON.parse(event.originalEvent.data);
  for (var i in data) {
    var d = data[i];
    var $targetCount = jQuery('#comment-like-count-'+d.no)
    $targetCount.html(d.like_count);
    if (d.like_count > 0) {
      $targetCount.addClass('on');
      var $targetIcon = jQuery('.comment-like-icon[data-comment-no="'+d.no+'"]');
      $targetIcon.find('.off').css({ opacity: 0, display: 'none' });
      $targetIcon.find('.hover').css({ opacity: 1, display: 'block' });
    }
  }
});
jQuery(document).ready(function() {
  var $textarea         = jQuery('#comment-form-textarea');
  var $commentForm      = jQuery('#comment-form');
  var $commentFormOuter = jQuery('#comment-form-outer');
  jQuery(document).on('click', '.toggle-reply-form', function() {
    var $this = jQuery(this);
    $textarea.val('');
    hideEmojiPanel('comment-form-textarea');
    if ($this.attr('data-open')) {
      $commentFormOuter.append($commentForm);
      $this.attr('data-open', '');
      jQuery('.reply-icon.icon-comment-v2-arrow_on').addClass('icon-comment-v2-arrow_off').removeClass('icon-comment-v2-arrow_on');
    } else {
      $commentForm.hide();
      jQuery('.reply-icon.icon-comment-v2-arrow_on').addClass('icon-comment-v2-arrow_off').removeClass('icon-comment-v2-arrow_on');
      var replyNo   = $this.attr('data-no');
      var replyText = '>>' + replyNo + "\n";
      $textarea.val(replyText);
      $textarea.focus();
      jQuery('#comment-'+replyNo).append($commentForm);
      $commentForm.slideToggle();
      $this.attr('data-open', 1);
      $this.children('.icon-comment-v2-arrow_off').addClass('icon-comment-v2-arrow_on').removeClass('icon-comment-v2-arrow_off');
    }
  });
  jQuery(document).on('click', '.comment-like-icon', function() {
    var $this  = jQuery(this);
    var $frame = jQuery('#comment-like-frame');
    var no = $this.attr('data-comment-no');
    $frame[0].contentWindow.postMessage(no, '*');
    var $likeCount = jQuery('#comment-like-count-' + no);
    $likeCount.html(parseInt($likeCount.html(), 10) + 1).addClass('on');
  });
});
</script>
<script>
jQuery(document).ready(function() {
  var star              = jQuery('#comment-form-tools .rating-value-v2 ul:last');
  var face              = jQuery('#comment-form-tools .rating-value-v2 ul:first');
  var rating_disp       = jQuery('#comment-form-tools .rating-value-v2');
  var input_rating      = jQuery('#comment-form-tools input[name="rating"]');
  var option_rating     = jQuery('#comment-form-tools .rating-value-v2 ul li');
  var input_rating_icon = jQuery('#comment-form-tools input[name="rating_icon"]');
  var radio_rating_icon = jQuery('#comment-form-tools .rating-icon-v2 div');
  jQuery('#comment-form-tools .rating-value-v2').hover(function(){
    (input_rating_icon.val() === 'star' ? star : face).show();
  }, function(){
    (input_rating_icon.val() === 'star' ? star : face).hide();
  });

  radio_rating_icon.on('click', function() {
    var $this = jQuery(this);
    input_rating_icon.val($this.data('value'));
    radio_rating_icon.removeClass('selected');
    $this.addClass('selected');
    rating_disp.find('> span.current').remove();
    rating_disp.find('> span, > div').show();
    input_rating.val('');
  });
  option_rating.on('click', function() {
    var $this = jQuery(this);
    var rating = $this.data('value');
    input_rating.val(rating);
    rating_disp.find('> span.current').remove();
    if (rating) {
      rating_disp.find('> span, > div').hide();
      var ele = $this.find('> span.rate').clone().addClass('current');
      rating_disp.append(ele);
    } else {
      rating_disp.find('> span, > div').show();
    }
  });
});
</script>
            </script>
