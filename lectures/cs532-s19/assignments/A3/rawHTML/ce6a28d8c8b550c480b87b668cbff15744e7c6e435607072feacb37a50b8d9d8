jQuery(document).ready(function( $ ){
    
    // An answer on a quizz of type 1 was clicked.
  	$(".mg_quizz_answer").click(function() {
      var question = $(this).parent();
      // If the question was already answered, then ignore the click
      if($(question).attr("answered") != "true") {
        // Set this as answered
        $(this).children().first().removeClass("fa-square").addClass("fa-check-square");
        $(question).attr("answered", "true")
        // Update answers
        var answer = this;
        $(question).children(".mg_quizz_answer").each(function(q) {
          if(answer == this) 
          {
            // This is the one the user selected

            // Update user's answer selection
            if($(this).attr("correct")=="true") {
              $(this).addClass("mg_quizz_answer_correct");
            } else {
              $(this).addClass("mg_quizz_answer_incorrect");
            }
          }
          // This is ONE of the ones the user did NOT select
          if($(question).hasClass("questiontype_single")) {
            if($(this).attr("correct")=="true") {
              $(this).addClass("mg_quizz_answer_correct");
            }
          }
          // For later ...
          if($(question).attr("questiontype") == "multiple") {
            // Ignore for now
          }
          
        });
        //alert($(this).has("question_id"));
      }
    });
