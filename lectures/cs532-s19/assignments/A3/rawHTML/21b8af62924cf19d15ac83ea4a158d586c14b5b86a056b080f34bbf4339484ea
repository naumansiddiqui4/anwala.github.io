<script>(function ($) {

  window.cxense_external_user_id = 0;
  getExternalUserId();

  // integración cxense usuario unico
  function getExternalUserId() {
    if (window.cxense_external_user_id == 0) {
      var cookie = _cxense_getCookie('_register_services', true);
      if (typeof cookie == "object" && typeof cookie.mail == "string") {
        window.cxense_external_user_id = cookie.mail;
      }
    }
  }

  // // returns the value of a specified cookie
  function _cxense_getCookie(cname, decode = true) {
    var name = cname + "=";

    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    if (decode == true) {
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          try {
            data = c.substring(name.length, c.length);
            data = JSON.parse(data);
            data.firstName = data.firstName.replace('+', ' ');
            data.lastName = data.lastName.replace('+', ' ');
            return data;
          } catch (err) {
            console.log(err);
          }
        }
      }
    }

    if (decode == false) {
      var cookies = document.cookie;
      var arrayCookies = cookies.split("; ");
      var cStatus = 0;

      for (var i = 0; i < arrayCookies.length; i++) {
        var aux = arrayCookies[i].split('=');
        if (aux[0] === cname) {
          return aux[1];
        }
      }
    }
    return "";
  }
})(jQuery);
</script>
