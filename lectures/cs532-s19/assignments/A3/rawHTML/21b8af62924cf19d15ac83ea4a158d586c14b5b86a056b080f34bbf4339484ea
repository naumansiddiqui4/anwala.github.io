    function _load_piano(user, isUserLogin) {

      var urlLogin = 'register-services/user/login';
      var current_path = Drupal.settings.piano.current_path;
      var path_is_admin = Drupal.settings.piano.path_is_admin;
      var piano_ad_blockers = Drupal.settings.piano.piano_ad_blockers;
      var piano_my_account = Drupal.settings.piano.piano_my_account;
      var user_edit = _is_user_edit(user.userId, current_path);

      if (!path_is_admin || $user_edit) {
        _ee_piano_services_js_composer();
        if (current_path != urlLogin) {
          _ee_piano_services_js_login_required();
        }
        _ee_piano_services_js_checkout_complete();
        if(isUserLogin){
          _ee_piano_services_js_custom_variables_user(user);
        }
        if (piano_ad_blockers == 1) {
          _ee_piano_services_js_ad_blockers();
        }
        if (piano_my_account == 1) {
          if (user_edit && isUserLogin) { //Mi cuenta
            _ee_piano_services_js_my_account();
          }
        }
        if(isUserLogin){
          _ee_piano_services_js_user_ref_token(user);
        }
      }
    }

      function _ee_piano_services_js_composer() {
        var piano_application_id = Drupal.settings.piano.piano_application_id;
        var piano_domain = Drupal.settings.piano.piano_domain;
        var domain = `//${piano_domain}.tinypass.com/xbuilder/experience/load?aid=${piano_application_id}`;
        (function(src) {
          var a = document.createElement("script");
          a.type = "text/javascript";
          a.async = true;
          a.src = src;
          var b = document.getElementsByTagName("script")[0];
          b.parentNode.insertBefore(a, b)
        })(domain);
        _ee_piano_services_js_composer_initiate();
      }

      function _ee_piano_services_js_composer_initiate() {
        tp = window.tp || [];

        //Cross Domain Tracking
        tp.push(["init", function () {        
          tp.enableGACrossDomainLinking();
        }]);
        //END Cross Domain Tracking
      }

      function _ee_piano_services_js_login_required() { //urls
        var base_url = Drupal.settings.piano.base_url;

        tp.push([
          "addHandler",
          "loginRequired",
          function(params) {
            // Sets the cookie with the current state of the user
            document.cookie = "__pianoParams=" + JSON.stringify(params) + "; path=/; domain=.elespectador.com";
            // Note that in order to be able to read this cookie,
            // the redirected page should be on the same domain
            location.href = `${base_url}/register-services/user/login`; // se cambio url aqui
          }
        ]);
      }

      function _ee_piano_services_js_checkout_complete() {
        tp.push([
          "addHandler",
          "checkoutComplete",
          function(conversion) {
            window.location.href = params.url;
          }
        ]);
      }
      function _ee_piano_services_js_custom_variables_user(user) { // migrar

        tp.push(['setCustomVariable', 'name', `${user.name}`]);
        tp.push(['setCustomVariable', 'firstName', `${user.firstName}`]);
        tp.push(['setCustomVariable', 'lastName', `${user.lastName}`]);
        tp.push(['setCustomVariable', 'mail', `${user.mail}`]);
        tp.push(['setCustomVariable', 'status', `${user.status}`]);
      }
      function _ee_piano_services_js_ad_blockers() {
        document.cookie = "__adblocker=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/";
        var setNptTechAdblockerCookie = function(adblocker) {
          var d = new Date();
          d.setTime(d.getTime() + 60 * 60 * 24 * 2 * 1000);
          document.cookie = "__adblocker=" + (adblocker
            ? "true"
            : "false") + "; expires=" + d.toUTCString() + "; path=/";
        }
        var script = document.createElement("script");
        script.setAttribute("async", true);
        script.setAttribute("src", "//www.npttech.com/advertising.js");
        script.setAttribute("onerror", "setNptTechAdblockerCookie(true);");
        document.getElementsByTagName("head")[0].appendChild(script);
      }

      function _ee_piano_services_js_my_account() {

        tp.push([
          "init",
          function() {
            tp.myaccount.show({displayMode: "inline", containerSelector: ".my-account-user-zone"});
          }
        ])
      }
      function _ee_piano_services_js_user_ref_token(user) {
        var user_ref = user.user_ref;

        tp.push(["setUserRef", `${user_ref}`]);
        tp.push([
          "init",
          function() {
            // If user is logged in
            if (tp.user.isUserValid()) {
              // Get the stored cookie value
              var paramsCookie = tp.util.findCookieByName('__pianoParams');
              var params;
              try {
                // Try to parse stored JSON data
                params = JSON.parse(paramsCookie);
                // Remove the old cookie
                document.cookie = "__pianoParams=null; expires=-1;" +
                  " path=/; domain=.elespectador.com";
              } catch (e) {
                params = false;
              }
              // If params object is valid - start checkout
              if (params) {
                tp.offer.startCheckout(params);
              }
            }
          }
        ]);
      }
      function _is_user_edit(userId, current_path) {
        var current = `register-services/user/${userId}/edit`;
        if (current_path == current) {
          return true;
        }
        return false;
      }
      // // returns the value of a specified cookie
      function getCookie(cname) {
        var name = cname + "=";

        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') {
            c = c.substring(1);
          }
          if (c.indexOf(name) == 0) {
            data = c.substring(name.length, c.length);
            return JSON.parse(data);
          }
        }
        return "";
      }
    }); // load
  })(jQuery);

  function send_experience(){
    tp.experience.execute();
  }

</script>
