<script>(function($) {

  $(document).ready(function() {

    var isUserLogin = false;
    var user = getCookie('_register_services');
    if (typeof user == "object" && typeof user.userId == "number") {
       isUserLogin = true;
    }
    _load_piano(user, isUserLogin);
    _piano_node_push();

    // cambian los parametros cuando se carga un nodo jscroll
    $( document ).ajaxComplete(function(event, request, settings) {
      if ( settings.url.indexOf("jscroll_view_entity/node") !== -1) {
        _piano_node_push();
        send_experience();
      }

});

    function _piano_node_push(){
      if(window.pianoIsNode){
          _ee_piano_services_js_content_tagging();
          //_ee_piano_services_js_custom_variables_page();
      }
    }
    function   _ee_piano_services_js_content_tagging(){
      tp.push(["setContentCreated", `${window.setContentCreated}`]);
      tp.push(["setContentAuthor", `${window.setContentAuthor}`]);
      tp.push(["setContentSection", `${window.setContentSection}`]);
      tp.push(["setTags", `${window.setTags}`]);
      tp.push(["setZone", `${window.setZone}`]);

      tp.push(['setCustomVariable', 'section', `${window.setContentSection}`]);
      tp.push(['setCustomVariable', 'author', `${window.setContentAuthor}`]);

    }
    // function _ee_piano_services_js_custom_variables_page(){
    //   console.log('_ee_piano_services_js_custom_variables_page');
    //   tp.push(['setCustomVariable', 'section', `${window.setContentSection}`]);
    //   tp.push(['setCustomVariable', 'author', `${window.setContentAuthor}`]);

