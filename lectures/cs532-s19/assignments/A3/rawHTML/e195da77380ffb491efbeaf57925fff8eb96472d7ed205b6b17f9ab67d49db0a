            window.onload = function() {
                if (typeof gigya != 'undefined') {
                    var containerID = "containerID-comments-pase";
                    var act1 = new gigya.socialize.UserAction();
                                                                
                    
                    
                    
                    
                    
                    act1.setTitle('Lo que la modernidad no se llevó: la última estancia porteña');
                    act1.setSubtitle('GPS');
                    act1.setLinkBack('/arq/modernidad-llevo-ultima-estancia-portena_0_IgCN_49Nu.html');
                    act1.addActionLink('VER MÁS', '/arq/modernidad-llevo-ultima-estancia-portena_0_IgCN_49Nu.html');

                    
                    $("#containerID-comments-pase").gigyaComments({
                        categoryID: "Com_03",
                        streamID: "IgCN_49Nu",
                        streamTags: "250",
                        userAction: act1,
                        activeForComment: 1
                    });
                }
            }
