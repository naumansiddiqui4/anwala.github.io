</script><script type="text/javascript">
    var refreshChecker = {
        init : function () {
            var data =  this.getValueFromCookie("autorefresh").split("|");
            if ( data[0] == '' || data[0] != window.location.pathname) {
                this.setValueToCookie("autorefresh",window.location.pathname + "|0");
            } else if (data[0] == window.location.pathname  &&  data[1] == "0" ) {
                this.setValueToCookie("autorefresh", window.location.pathname + "|1");
            }
        },
        getValueFromCookie :function (cookieName) {
            var reg = cookieName + '\\=([^\\;]+)';
            var matches = document.cookie.match(new RegExp(reg));
            return matches != null && matches.length > 1 ? matches[1] : '';
        },
        setValueToCookie: function (cookieName, value) {
            document.cookie = cookieName + "=" + value + ";path=/;";
        },
        delete_cookie: function (name) {
            document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;path=/;';
        },
        isAutorefresh: function () {
            var data = this.getValueFromCookie("autorefresh").split("|");
            return typeof data[1] != 'undefined' ? data[1] : "0";
        }
    };

    var parameters = parse_query_string(location.search.slice(1));

    refreshChecker.init();

    function getAge(dateString) {
        var today = new Date();
        var birthDate = new Date(dateString);
        var age = today.getFullYear() - birthDate.getFullYear();
        var m = today.getMonth() - birthDate.getMonth();
        if (m < 0 || (m === 0 && today.getDate() < birthDate.getDate())) {
            age--;
        }
        return age;
    }

    function parse_query_string(query) {
        var vars = query.split("&");
        var query_string = {};
        for (var i = 0; i < vars.length; i++) {
            var pair = vars[i].split("=");
            var key = decodeURIComponent(pair[0]);
            key = key.toLowerCase();
            var value = decodeURIComponent(pair[1]);
            // If first entry with this name
            if (typeof query_string[key] === "undefined") {
                query_string[key] = decodeURIComponent(value);
                // If second entry with this name
            } else if (typeof query_string[key] === "string") {
                var arr = [query_string[key], decodeURIComponent(value)];
                query_string[key] = arr;
                // If third or later entry with this name
            } else {
                query_string[key].push(decodeURIComponent(value));
            }
        }
        return query_string;
    }


    function getCampaign() {
        if (typeof parameters.kw_campaign != "undefined" ) {
            return parameters.kw_campaign;
        }
        if (typeof parameters.utm_campaign != "undefined" ) {
            return parameters.utm_campaign;
        }
        return -2;
    }

    function getReferrer() {
        if (typeof parameters.fromref != "undefined" ) {
            return parameters.fromref;
        } else  if (document.referrer.match(/t\.co/)) {
            return 'twitter';
        } else if (document.referrer.match(/facebook/)) {
            return 'facebook';
        }
        return -2;
    }

    function analyticsDtectPrivateMode(cb) {
        var db,
            on = cb.bind(null, true),
            off = cb.bind(null, false)

        function tryls() {
            try {
                localStorage.length ? off() : (localStorage.x = 1, localStorage.removeItem("x"), off());
            } catch (e) {
                navigator.cookieEnabled ? on() : off();
            }
        }

        function tryff() {
            try { localStorage.test = 2; on()  } catch (e) { return off(); }
        }

        // Blink (chrome & opera)
        window.webkitRequestFileSystem ? !webkitRequestFileSystem(0, 0, off, on)
            // FF
            : "MozAppearance" in document.documentElement.style ?  tryff()
            // Safari
            : /constructor/i.test(window.HTMLElement) || window.safari ? tryls()
                // IE10+ & edge
                : !window.indexedDB && (window.PointerEvent || window.MSPointerEvent) ? on()
                    // Rest
                    : off()
    }

    analyticsDtectPrivateMode(function (isPrivateMode) {
        window.isPrivateMode = isPrivateMode ? 1 : 0;
    });

    function mergeObject(finalObject, fieldsObject){
        /* merge manual */
        if (Object.keys(fieldsObject).length > 0)
            for (i = 0, len = Object.keys(fieldsObject).length; i < len; i++) {
                var key = Object.keys(fieldsObject)[i];
                finalObject[key] = fieldsObject[key];
            }

        return finalObject;
    }

    function getDataSource() {
        return window.innerWidth<720 ? 'mobile' : 'web';
    }

    function getValueFromCookie(cookieName){
        var reg = cookieName+'\\=([^\\;]+)';
        var matches = document.cookie.match(new RegExp(reg));
        return matches!=null && matches.length > 1 ? matches[1] : '';
    }

    function adblockFlag(){
        return typeof window.canRunAds == "undefined" ? true : false;
    }

    function sendGAPageview(customParams){
        customParams = customParams || {};
        analyticsHitParams.hitType = "pageview";
        var finalObject = mergeObject(analyticsHitParams, customParams);

        try{
            if(typeof(window.isPrivateMode) == "undefined")
                throw 'waiting bprivate';

            finalObject['dimension6'] = window.isPrivateMode;
            sendGAHit(finalObject);

        }catch(e){
            setTimeout(function(){
                sendGAPageview(customParams);
            }, 200);
        }
    }

    /**
     * Ej: { hitType: 'event', eventCategory: 'Videos', eventAction: 'play', eventLabel: 'Kaltura' }
     * @param fieldsObject object
     */
    function sendGAHit(fieldsObject, mergeWithGeneralParams){
        fieldsObject = fieldsObject || {};
        mergeWithGeneralParams = mergeWithGeneralParams || false;

        var finalObject = {hitType: 'pageview'};

        if(mergeWithGeneralParams)
            finalObject = mergeObject(finalObject, analyticsHitParams);

        finalObject = mergeObject(finalObject, fieldsObject);

        if (fieldsObject.hitType == 'event') {
            var dimensionsObject = {};
            for (let [key, value] of Object.entries(fieldsObject)) {
                if (key.startsWith("dimension")) {
                    dimensionsObject[key] = value;
                }
            }
            try {
                if(typeof fieldsObject.eventCategory == "undefined" || fieldsObject.eventCategory == "") throw "valor obligatorio";
                if(typeof fieldsObject.eventAction == "undefined" || fieldsObject.eventAction == "") throw "valor obligatorio";
                ga('send', 'event',
                    fieldsObject.eventCategory,
                    fieldsObject.eventAction,
                    fieldsObject.eventLabel,
                    dimensionsObject
                );
            }
            catch(err){

            }
        } else {
            ga('send', finalObject);
        }
    }

    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

