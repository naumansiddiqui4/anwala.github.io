    window.anPrebidAsyncInit = function () {
        var prebid = new AudienceNetworkPrebid(
            '123685264900525_315266952409021',
            document.getElementById('adContainer')
        );
        prebid.getProfile(function (profile) {
            var token = profile.token;
            if(token !== 'nobid') {
                localStorage.setItem('fbfan',1);
                localStorage.setItem('fbtoken',token);
            } else {
                localStorage.setItem('fbfan',0);
                localStorage.setItem('fbtoken',"-2");
            }
        });
        return prebid;
    };
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
