    <link href="/contents/widgets/css/cxense.css?bf340a7" type="text/css" rel="stylesheet"/>

<script type="text/javascript">
    function loadCxenseWidget(target){
        cX.callQueue.push(['insertWidget',{
            widgetId: target.attr('data-widget'),
            renderFunction:function(data){
                var items = data.response['items'];
                $(items).each(function (index) {
                    var b = $(this)[0];
                    var clase = "";
                    if(typeof b["og-image"] != "undefined" && b["og-image"].indexOf("//www.clarin.com/static/CLAClarin/images/Clarin-sahreing-fbk.jpg") != -1){
                        clase = "no-image";
                    }
                    
                    node = $('#tpi .listado li:eq('+index+')');
                    node.find('article').addClass('border');
                    node.find(".cx-item").text(b.title);
                    urlImagen = "https://images.clarin.com/collections/static/lazy_square.svg";
                    if(typeof b.dominantthumbnail != "undefined"){
                        var urlImagen  = b.dominantthumbnail.replace("http://","https://");
                    }
                    var url = b.url.replace("http://","https://");
                    node.find("a").attr("id",cX.clickTracker(b.click_url));
                    node.find("a").attr("href",b.url);
                    node.find(".mt img").attr('src', urlImagen).attr('alt', b.title).attr('class',clase).addClass('lazyload');
                    node.find(".volanta").html(typeof b['recs-volanta'] != "undefined" ? b['recs-volanta'] : "");
                    node.find(".data-txt span").html(typeof b['gca-categories'] != "undefined" ? b['gca-categories'].split('-').slice(-1)[0].replace(" ","") : "");
                });
                $('.cx-tpinteresar .listado li').removeClass('dummy');
            }
        }]);
    }
</script>

                
                        
                        
