    function triggerRenderAds(event){
        if(typeof ads != 'undefined'){
            ads.pending();
        }
    }

    document.body.dataset.adspath = 'arq/nota';

    var _dc_w_user = WAuth.getUserSync();
    var adskv = {
        section: ["arq"],
        kw_referer: [document.referrer],
