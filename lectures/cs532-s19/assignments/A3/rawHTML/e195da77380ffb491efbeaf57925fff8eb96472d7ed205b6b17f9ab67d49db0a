<script type="text/javascript">
    $("#form-profile").bind("submit", function(e){
        e.preventDefault();
        var url = $(this).attr("action");
        var params = $(this).serializeArray().reduce(function(obj, item) {
            obj[item.name] = item.value;
            return obj;
        }, {});
        $('#form-profile .form-group, #form-profile .loadComments').toggle();
        $.post(url, params)
            .done(function(data) {
                if(typeof data.result != "undefined" && data.result){
                    refreshCommentPlugin();
                }
                if(typeof WAuth != "undefined"){
                    WAuth.getUpdateUserServer(function(err, user){
                        if(!err){
                            WeizenBockCompatible.checkSiteCookies(user);
                        }
                });
                }
            })
            .fail(function(e) {
                console.log("Fail: ",e);
            })
            .always(function() {
                $('#form-profile .form-group, #form-profile .loadComments').toggle();
                $.magnificPopup.close();
            });
    });
</script>

    <script type="text/javascript">
        function refreshCommentPlugin(){
            $('#containerID-comments-pase').replaceWith('<div id="containerID-comments-pase" style="clear: both; margin:auto;"></div>');
            $("#activateComments").trigger('refreshPlugin');
        }

        $(document).on("wa.login", function(e, user){
            refreshCommentPlugin();
        });

    </script>

</section>

                        
                                        
