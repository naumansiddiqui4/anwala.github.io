<script type="text/javascript">
    (function() {
        var sb = document.getElementById('srchBox');
        if (sb && sb.className == 'searchBox watermark') {
            var si = document.getElementById('srchInput');
            var f = function() { si.className = 'searchInput nomark'; };
            var b = function() { if (si.value == '') { si.className = 'searchInput'; }};
            si.onfocus = f; si.onblur = b;
            if (!/[&?]p=[^&]/.test(location.search)) {b();} else {f();}
        }
    })();
</script>


