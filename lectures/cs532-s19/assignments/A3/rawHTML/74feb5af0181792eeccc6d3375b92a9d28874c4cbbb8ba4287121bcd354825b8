<div class="videoModule">

<div class="modalblock" style="display:none;">
<div class="inner_wrap">

<div  class="mainContentPanel"  >
  
<p><a class="buttonIcon closeWhite closeButton" onclick="closeMovieModal()" >閉じる</a></p><video controls="" src="" controlslist="nodownload" style="width:100%;" class="modalMovie" autoplay></video>

<script>
// 「modalMovieEvent」CSSを持つタグの「data-videoPath」属性に表示させたい動画パスを仕込むことで使える
$('.modalMovieEvent').click(function(){
  videoPath = $(this).attr("data-videoPath");
  $('.videoModule .modalblock .inner_wrap').css('max-width','800px');
  video = $('.modalMovie').get(0);
  video.src = videoPath;
  video.load();
  showModal('.videoModule .modalblock');
});

$('.modalblock').click(function(){
  video = $('.modalMovie').get(0);
  video.pause();
  closeModal('.modalblock');
});

$('.modalMovie').bind('click',function(e){
  e.stopPropagation();
});

function closeMovieModal(){
  video = $('.modalMovie').get(0);
  video.pause();
  closeModal('.modalblock');
}
</script>

</div>

</div>
</div><!-- /modalblock -->
<script src="https://thewonder.it/js/v3/common/wonder_layout_adjustment.js"></script>
<script type="text/javascript"> 
/*
 * モーダル等を表示させる
 * @param blockElement {string} センターに配置するmainContentPanelクラス要素の親のjQuery要素名
 * @param scrollObject {boolean} trueの場合背景の高さを設定しない（背景の半透明領域の指定）
 * @param fixedElement {string} スクロールする対象を設定する。設定しない場合は、windowが設定される
 */
function showModal(blockElement,modalBackgroundHeightFlag,fixedElement){
  if($(blockElement)[0] != null){
    $(blockElement).show();
    
    if(modalBackgroundHeightFlag !== true){
      WS.LayoutAdjust.changeContainerHeightForContent($(blockElement));
    }
    WS.LayoutAdjust.centeringHeightContent($(blockElement + ' .mainContentPanel'),fixedElement);
    
    // UAで端末判定
    var ua = navigator.userAgent;
    if (!(ua.indexOf('iPhone') > 0 || ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0)) {
      WS.LayoutAdjust.centeringHeightContent($(blockElement + ' .mainContentPanel'));
    }
    // 学年登録ダイアログの場合、開く際にエラー文言をリセットする。
    if(blockElement == '.gradeInputModal .modalblock'){
      $('.grade_input_err').html('');
    }
  }
  // throw new Error("モーダルの要素が見つかりません");
}
function closeModal(blockElement){
  if($(blockElement)[0] != null){
    $(blockElement).hide();
  }
  // throw new Error("モーダルの要素が見つかりません");
}

$('.modalblock').click(function(){
  closeModal('.modalblock');
});

$('.mainContentPanel').click(function(e){
  e.stopPropagation();
});

</script> 


</div>


