<script type="text/javascript">
$(function(){
  // 表示速度や表示までの時間を変更できる
  $(window).on("ready",function(){
    $("#under_banner").css("display","none");
  });
  $(window).on("scrollstop",function(){
    setTimeout('$("#under_banner").fadeIn(2000)',3000);
  });
  $(window).on("scroll", function () {
    $("#under_banner").stop().fadeOut(1);
  });
  
  $('#close_under_banner').click(function(){
    $("#under_banner").remove();
  });
});
</script>
