<script type="text/javascript">
    var isOldIE = false;
    /*@cc_on
        @if (@_jscript_version == 10)
            isOldIE = 10;
        @elif (@_jscript_version == 9)
            isOldIE = 9;
        @elif (@_jscript_version == 5.8)
            isOldIE = 8;
        @else
            isOldIE = false;
        @end
    @*/
    if (isOldIE === 10 || isOldIE === 9) {
        document.getElementsByClassName("alertforIE")[0].style.display = "block";
    } else if (isOldIE === 8) {
        var allElements = document.getElementsByTagName("div");
  
        for (var i = 0;  i < allElements.length; i += 1){
            if (allElements[i].className == "alertBox alertforIE"){
                allElements[i].style.display = "block";
                break;
            }
        }
    }
</script>
