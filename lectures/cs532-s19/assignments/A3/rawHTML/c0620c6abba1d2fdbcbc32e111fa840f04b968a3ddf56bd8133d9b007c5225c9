<script type="text/ng-template" id=calculateBetaModal.html>
    <div class="beta-modal-header">
        Calculate Beta for any asset
    </div>
    <div class="modal-body">
        <div class="form-horizontal">
            <fieldset>
                <div class="form-group">
                    <label for="inputEmail3"
                           class="col-md-2 col-sm-2 control-label">Ticker</label>

                    <div class="col-md-4 col-sm-4">
                        <input class="form-control"
                               placeholder="e.g., TSLA"
                               autocomplete="off"
                               ng-model="beta.symbol"
                               ng-model-options="{ debounce: { 'default': 500, 'blur': 0 } }"
                               uib-typeahead="ticker as ticker.code for ticker in getTickers($viewValue) | limitTo:10"
                               typeahead-loading="loadingSearchSymbol"
                               typeahead-input-formatter="filterEmpty($model)"
                               typeahead-on-select="go"
                               typeahead-template-url="searchTypeaheadTemplate.html">

                        <i class="fa fa-spinner fa-pulse form-control-feedback"
                           aria-hidden="true"
                           ng-show="loadingSearchSymbol"></i>
                    </div>

                    <label for="inputEmail3" class="col-md-2 col-sm-2 control-label">Start</label>

                    <div class="col-md-4 col-sm-4">
                        <p class="input-group">
                            <input type="text" class="form-control"
                                   uib-datepicker-popup="[[format]]"
                                   on-open-focus="true"
                                   ng-model="beta.dateStart"
                                   datepicker-options="dateOptions"
                                   close-text="Close"
                                   is-open="datePickerOpenedStart"
                                   placeholder="e.g, 26/04/15"/>
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-default" tabindex="-1"
                                        ng-click="datePickerOpenedStart = true">
                                    <i class="fa fa-calendar-check-o"></i>
                                </button>
                            </span>
                        </p>
                    </div>

                </div>

                <div class="form-group">
                    <label for="inputEmail3"
                           class="col-md-2 col-sm-2 control-label">Index</label>

                    <div class="col-md-4 col-sm-4">
                        <input class="form-control"
                               placeholder="e.g., GSPC"
                               autocomplete="off"
                               ng-model="beta.index"
                               ng-model-options="{ debounce: { 'default': 500, 'blur': 0 } }"
                               uib-typeahead="ticker as ticker.code for ticker in getTickers($viewValue) | limitTo:10"
                               typeahead-loading="loadingSearchIndex"
                               typeahead-input-formatter="filterEmpty($model)"
                               typeahead-on-select="go"
                               typeahead-template-url="searchTypeaheadTemplate.html">
                        <i class="fa fa-spinner fa-pulse form-control-feedback"
                           aria-hidden="true"
                           ng-show="loadingSearchIndex"></i>
                    </div>

                    <label for="inputEmail3" class="col-md-2 col-sm-2 control-label">End</label>

                    <div class="col-md-4 col-sm-4">
                        <p class="input-group">
                            <input type="text" class="form-control"
                                   uib-datepicker-popup="[[format]]"
                                   on-open-focus="true"
                                   ng-model="beta.dateEnd"
                                   datepicker-options="dateOptions"
                                   close-text="Close"
                                   is-open="datePickerOpenedEnd"
                                   placeholder="e.g, 26/04/15"/>
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-default" tabindex="-1"
                                        ng-click="datePickerOpenedEnd = true">
                                    <i class="fa fa-calendar-check-o"></i>
                                </button>
                            </span>
                        </p>
                    </div>

                </div>

                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-2 col-md-2 control-label"></label>
                    <div class="col-sm-6 col-md-6">
                        <div class="margin0 addtrade-error" ng-show="error">
                            [[ error ]]
                        </div>

                        <div class="beta-response" ng-show="result">
                            <span class="font-bold">Beta &mdash; [[result.beta]]</span><br/>
                            <span class="text-grey beta-date-font">[[result.fromDate]] -> [[result.toDate]]</span>
                        </div>
                    </div>
                    <div class="col-sm-4 col-md-4">
                        <button class="btn btn-primary form-control pull-right" type="submit"
                                ng-disabled="busy"
                                ng-click="calculateBeta()">
                            Calculate Beta
                        </button>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</script>
