<script type="text/ng-template" id=signalsTechnicalPopoverList.html>
    <div class="popover-tags-list-main" style="width: 365px;">
        <div class="popover-tags-list-item"
             data-ng-repeat="(key,group) in allSignals | filter: {signal_type: 'technical'} | filter: getSignalsInFilter | groupBy: 'group'">
            <div class="popover-tags-list-filter-title">
                [[ key ]]
            </div>
            <div class="popover-tags-list-filter-text">
                <div class="btn-group">
                    <label data-ng-repeat="signal in group | orderBy :'category'"
                           uib-popover="[[signal.descr]]"
                           popover-append-to-body="true"
                           popover-popup-delay=500
                           popover-trigger="mouseenter"
                           style="width: 70px;"
                           class="btn btn-xs btn-default"
                           ng-class="{positive: signal.category>0, negative: signal.category<0}"
                           ng-model="selected['signals'][key]"
                           uib-btn-radio="signal.value"
                           ng-disabled="loading"
                           ng-click="radioFilter(signal, 'signals')"
                           uncheckable>[[ signal.filterText ]]</label>
                    <label class="btn btn-xs btn-default"
                           ng-model="selected['signals'][key]"
                           uib-btn-radio="null"
                           ng-show="!loading && selected['signals'][key]"
                           ng-click="radioFilter({group: key, value: null}, 'signals')"
                    ><i class="fa fa-times"></i></label>
                </div>
            </div>
        </div>
    </div>
