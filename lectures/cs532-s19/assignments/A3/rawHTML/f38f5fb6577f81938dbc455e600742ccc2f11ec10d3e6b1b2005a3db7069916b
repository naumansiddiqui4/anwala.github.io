<script type="text/javascript">
if (document.body.classList.contains('nightmode')){
	var elementfb = document.querySelector('.social-comments .fb-comments');
	elementfb.setAttribute('data-colorscheme','dark');
}

window.fbAsyncInit = function(){
    FB.Event.subscribe('xfbml.render', function(){
        var elem = document.getElementById('fb-social-comments');

        if(elem){
            elem.className = elem.className.replace('loading-app','');
        }
    });
    
    zaz.use(function(pkg){
    	var commentsCount = pkg.context.page.get("comments-count")
    	var elem = document.getElementsByClassName('comments-count');
    	
    	if(elem.length > 0){
	    	if(commentsCount){
	    		fbComments(elem, commentsCount);
	    	}else{
	            pkg.require(['mod.xRequest'], function(Xrequest) {
	            	var req = new Xrequest();
	                req.get({
	                    url: "https://graph.facebook.com/?id=https://www.terra.com.br/noticias/brasil/politica/bebianno-busca-saida-honrosa-para-crise-mas-nao-e-recebido-por-bolsonaro,6b71f7095517f0efcf1e8b0c7bb4115ev2xebi0s.html",
	                    dataType: 'json',
	                    success: function(data, xhr) {
	    					pkg.context.page.set("comments-count",data.share.comment_count);
	    					fbComments(elem, data.share.comment_count);
	                    },
	                    error: function(xhr, err) {
	                    	console.log("Error");
	                    }
	                });
	
	            });
		   	}
    	}
    	
    	var fbComments = function(elem, commentsCount){
   			for (i = 0; i < elem.length; i++) { 
   			    elem[i].innerHTML = commentsCount
   			}
   		
    	}
    	
    });
}
</script>

 
                                
