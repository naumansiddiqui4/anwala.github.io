    <!-- NIGHTMODE -->
    <script type="text/javascript">

	zaz.use(function(pkg){
		pkg.context.page.set("allowTheme",true);
	});
    var reTheme = new RegExp(/user_theme=(\w+)/);
    var userTheme = reTheme.exec(document.cookie);

    if(userTheme && userTheme.length > 1) {
        if(userTheme[1] == "nightmode"){
            document.body.className += " nightmode";
        	zaz.use(function(pkg){
        		pkg.context.page.set("nightmode",true);
        	});
        }
    }

</script>
