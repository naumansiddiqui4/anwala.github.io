-25412708bc49'></script><!-- End User Code --><!-- User Code --><script>
window.onscroll = throttle(hideStickyAd,200);

function hideStickyAd(){
  var footer_div = document.getElementsByClassName("posts-before-footer")[0];
  var ad_div = document.getElementsByClassName("sticky-target")[0];
  var footer_pos = footer_div.getBoundingClientRect().top;
  var ad_div_pos = ad_div.getBoundingClientRect().top;
  var ad_size = 600;

  if(ad_div_pos+ad_size>=footer_pos){
     ad_div.style.display = "none"; 
  } else {
     ad_div.style.display = "block"; 
  }
}

function throttle(callback, limit) {
    var wait = false;                  
    return function () {               
        if (!wait) {                  
            callback.call();           
            wait = true;              
            setTimeout(function () {   
                wait = false;          
            }, limit);
        }
    }
}

</script><!-- End User Code -->
