  function winCenter(url){
    var subw = 650;
    var subh = 470;

    var subx = ( screen.availWidth  - subw ) / 2;
    var suby = ( screen.availHeight - subh ) / 2;

    var subWinOpt = "width=" + subw + ",height=" + subh + ",top=" + suby + ",left=" + subx + ", personalbar=0, toolbar=0, scrollbars=1, sizable=1";

    window.open(encodeURI(decodeURI(url)), 'tweetwindow', subWinOpt);
  }
</script>
