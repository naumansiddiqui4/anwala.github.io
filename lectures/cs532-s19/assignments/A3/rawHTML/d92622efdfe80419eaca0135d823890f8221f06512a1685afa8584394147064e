            	  var hList=new Array();
            	  $('.bcs_series dl dt').each(function(idx){
            	    hList[idx]=$(this).height();
            	  });
            	  
            	  var maxH=Math.max.apply(null,hList);
            	  $('.bcs_series dl dt').css({height: maxH});
      
            	  var num = $('.bcs_seriesContent dl dd').length;
            	  for (var i = 1; i <= num; i++) {
            	    var maxHeight = 0;
            	    $('.bcs_series dl dd:nth-of-type('+i+')').each(function(){
            	      if ($(this).height() > maxHeight) {
            	        maxHeight = $(this).height();
            	      }
            	    });
            	    $('.bcs_series dl dd:nth-of-type('+i+')').css({height: maxHeight});
            	  }
          }
      });
      
