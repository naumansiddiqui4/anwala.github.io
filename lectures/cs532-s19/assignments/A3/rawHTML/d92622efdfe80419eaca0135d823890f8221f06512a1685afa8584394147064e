function connectedSelect(selIdList){
       for(var i=0;selIdList[i];i++) {
           var CS = new Object();
           var obj = document.getElementById(selIdList[i]);
           if(i) {
               CS.node=document.createElement('select');
               var GR = obj.getElementsByTagName('optgroup');
               while(GR[0]) {
                   CS.node.appendChild(GR[0].cloneNode(true));
                   obj.removeChild(GR[0]);
               }
               obj.disabled = true;
           }
           if(selIdList[i+1]) {
               CS.nextSelect = document.getElementById(selIdList[i+1]);
               obj.onchange = function(){connectedSelectEnabledSelect(this)};
           } else {
               CS.nextSelect = false;
           }
           obj.connectedSelect = CS;
       }
}

function connectedSelectEnabledSelect(oSel){
       var oVal = oSel.options[oSel.selectedIndex].value;
       if(oVal) {
           while(oSel.connectedSelect.nextSelect.options[1])oSel.connectedSelect.nextSelect.remove(1);
               var eF = false;
               for(var OG=oSel.connectedSelect.nextSelect.connectedSelect.node.firstChild;OG;OG=OG.nextSibling) {
                   if(OG.label == oVal) {
                       eF = true;
                       for(var OP=OG.firstChild;OP;OP=OP.nextSibling)
                           oSel.connectedSelect.nextSelect.appendChild(OP.cloneNode(true));
                           break;
               }
           }
           oSel.connectedSelect.nextSelect.disabled = !eF;
       } else {
           oSel.connectedSelect.nextSelect.selectedIndex = 0;
           oSel.connectedSelect.nextSelect.disabled = true;
       }
       if(oSel.connectedSelect.nextSelect.onchange)oSel.connectedSelect.nextSelect.onchange();
}

function changeUrl() {
  var value = $("#bcs_btn").val();
  var url = "/bc/item/" + value + "/";
    location.href = url;
}

var set = 0;
function submitCheck() {
  sameBuyLogic();
  if(set == 0){
      set = 1;
      var retVal = true;
      var checkCondYn1 = document.forms['Goods']['CHECK_COND_YN1'];
      if (checkCondYn1) {
          if (checkCondYn1.checked == false) {
              document.getElementById("cond1Err").style.display="block";
              $('#cond1Check').insertAfter($('#cond1ErrLabel'));
              document.getElementById("cond1").style.display="none";

              alert("ï¿½uï¿½ï¿½ï¿½wï¿½ï¿½ï¿½Oï¿½É‚ï¿½ï¿½Ç‚İ‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½vï¿½Ì“ï¿½ï¿½eï¿½É‚Â‚ï¿½ï¿½ÄAï¿½uï¿½ï¿½ï¿½Ó‚ï¿½ï¿½ï¿½vï¿½ï¿½ï¿½Iï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ä‚ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½B");
              set = 0;
              if ($('#campPairmatchTocart')[0]) {
                  $('#campPairmatchTocart').remove()
              }
              retVal = false;
          } else {
        	  // ï¿½`ï¿½Fï¿½bï¿½Nï¿½ï¿½ï¿½ê‚½ï¿½ê‡ï¿½Aï¿½fï¿½Uï¿½Cï¿½ï¿½ï¿½ï¿½ß‚ï¿½ï¿½B
        	  document.getElementById("cond1Err").style.display="none";
        	  $('#cond1Check').insertAfter($('#cond1Label'));
        	  document.getElementById("cond1").style.display="block";
          }
      }
      var checkCondYn2 = document.forms['Goods']['CHECK_COND_YN2'];
      if (checkCondYn2) {
          if (checkCondYn2.checked == false) {
              document.getElementById("cond2Err").style.display="block";
              $('#cond2Check').insertAfter($('#cond2ErrLabel'));
              document.getElementById("cond2").style.display="none";
              
			ï¿½@ï¿½@//ï¿½Gï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½bï¿½Zï¿½[ï¿½Wï¿½Í‚Qï¿½ï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½È‚ï¿½
              if (retVal == true) {
                  alert("ï¿½uï¿½ï¿½ï¿½wï¿½ï¿½ï¿½Oï¿½É‚ï¿½ï¿½Ç‚İ‚ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½vï¿½Ì“ï¿½ï¿½eï¿½É‚Â‚ï¿½ï¿½ÄAï¿½uï¿½ï¿½ï¿½Ó‚ï¿½ï¿½ï¿½vï¿½ï¿½ï¿½Iï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ä‚ï¿½ï¿½Ü‚ï¿½ï¿½ï¿½B");
              }
              set = 0;
              if ($('#campPairmatchTocart')[0]) {
                  $('#campPairmatchTocart').remove()
              }
              retVal = false;
          } else {
        	  // ï¿½`ï¿½Fï¿½bï¿½Nï¿½ï¿½ï¿½ê‚½ï¿½ê‡ï¿½Aï¿½fï¿½Uï¿½Cï¿½ï¿½ï¿½ï¿½ß‚ï¿½ï¿½B
        	  document.getElementById("cond2Err").style.display="none";
        	  $('#cond2Check').insertAfter($('#cond2Label'));
        	  document.getElementById("cond2").style.display="block";
          }
      }
      var checkLiqYn = document.forms['Goods']['CHECK_LIQ_YN'];
      if (checkLiqYn) {
          if (checkLiqYn.checked == false) {
              document.getElementById("liqErr").style.display="block";
              $('#liqErrLabel').append($('#liqLabel'));
              document.getElementById("liq").style.display="none";

              alert("ï¿½Nï¿½ï¿½Fï¿½Ø‚Ìƒ`ï¿½Fï¿½bï¿½Nï¿½{ï¿½bï¿½Nï¿½Xï¿½ï¿½Iï¿½ï¿½ï¿½ï¿½ï¿½Ä‰ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½B");
              set = 0;
              if ($('#campPairmatchTocart')[0]) {
                  $('#campPairmatchTocart').remove()
              }
              retVal = false;
          }
      }
      return retVal;
  } else {
      if ($('#campPairmatchTocart')[0]) {
          $('#campPairmatchTocart').remove()
      }
      return false;
  }
}

// ï¿½uï¿½êï¿½É”ï¿½ï¿½ï¿½ï¿½vï¿½ï¿½ï¿½iï¿½Ôï¿½ï¿½æ“¾
function sameBuyLogic() {
	// ï¿½ï¿½xï¿½Nï¿½ï¿½ï¿½Aï¿½ï¿½ï¿½ï¿½
	$('.CAMP_PAIRMATCH_TOCART').remove();
	if ($('.sameBuy').length) {
	    var list = $('.sameBuy').find('li');
	    for (var i=0; i<list.length; i++) {
	      if ($(list[i]).find('input').prop('checked')) {
	        var className = list[i].className;
	        var goodsNo = className.split("_")[1];
	        $("<input type='hidden' class='CAMP_PAIRMATCH_TOCART' name='CAMP_PAIRMATCH_TOCART' id='campPairmatchTocart' value='" + goodsNo + "'>").appendTo('form[name="Goods"]');
	      }
	    }
	}
}

// ï¿½Jï¿½[ï¿½gï¿½É“ï¿½ï¿½ï¿½ï¿½{ï¿½^ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ìï¿½ï¿½ï¿½
function cartBtnClick(goodsNo, shopNo) {

	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½ï¿½ÌƒGï¿½ï¿½ï¿½[ï¿½ï¿½ï¿½bï¿½Zï¿½[ï¿½Wï¿½ï¿½ï¿½Nï¿½ï¿½ï¿½A
	$("#dialogError").html("");
	$("#dialogError").removeClass("error");
	
	// ï¿½ï¿½1ï¿½Eï¿½ï¿½2ï¿½Eï¿½Nï¿½ï¿½Fï¿½Ø‚ÌŠeï¿½`ï¿½Fï¿½bï¿½Nï¿½{ï¿½bï¿½Nï¿½Xï¿½ï¿½ï¿½ï¿½ï¿½İ‚ï¿½ï¿½ï¿½ê‡ï¿½Aï¿½`ï¿½Fï¿½bï¿½Nï¿½ï¿½ï¿½ï¿½
	if ($("#Clause1").size()) {
		$("#Clause1").prop('checked', false);
	}
	if ($("#Clause2").size()) {
		$("#Clause2").prop('checked', false);
	}
	if ($("#Over20").size()) {
		$("#Over20").prop('checked', false);
	}
	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½ÌƒTï¿½Cï¿½Yï¿½Äİ’ï¿½
	$.colorbox.resize();

		// ï¿½uï¿½êï¿½É”ï¿½ï¿½ï¿½ï¿½vï¿½ï¿½ï¿½iï¿½æ“¾(ï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½Ìï¿½ï¿½iï¿½Ü‚ï¿½)
		sameBuyLogic();
		var campPairmatchToCart = goodsNo;

			$("input[name=CAMP_PAIRMATCH_TOCART]").each(function(i, elem) {
			campPairmatchToCart += ",";
			campPairmatchToCart += $(elem).val();
		});

	// ï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ÎÛ‚Ìï¿½ï¿½iï¿½Sï¿½Ä‚ï¿½ï¿½ï¿½ï¿½[ï¿½_ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Â”\ï¿½ï¿½ï¿½`ï¿½Fï¿½bï¿½N
	var isModal = false;
	var postData = {}
	postData["CAMP_PAIRMATCH_TOCART"] = campPairmatchToCart;
	var directCartInCheckAjaxUrl = "/bc/cart/CCtDirectCartInCheckAjax.jsp";
	$.ajax({
	    type: "POST",
	    url: directCartInCheckAjaxUrl,
	    data: postData,
	    dataType: "json",
	    async: false
	}).done( function( data ) {
		// ï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ÎÛ‚Ìï¿½ï¿½iï¿½Sï¿½Ä‚ï¿½ï¿½ï¿½ï¿½[ï¿½_ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Â”\ï¿½Ìê‡ï¿½Aï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Nï¿½ï¿½ï¿½Nï¿½ï¿½ï¿½bï¿½N(Ajax)
	    if (data.RESULT == "true") {
	    
	    	$("#CART_IN_AJAX_LINK").click(); 
		
	    } else {
	    	$("#CART_IN").click(); // ï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ÎÛ‚Ìï¿½ï¿½iï¿½ï¿½ï¿½Éˆï¿½Â‚Å‚ï¿½ï¿½ï¿½ï¿½[ï¿½_ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½sï¿½Â‚Ìï¿½ï¿½iï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ê‡ï¿½Aï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½{ï¿½^ï¿½ï¿½ï¿½ï¿½ï¿½Nï¿½ï¿½ï¿½bï¿½N(submit)
	    }
	});

	return true;
}

// Ajaxï¿½ÊMï¿½ÅƒJï¿½[ï¿½gï¿½Ç‰ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½s
function cartInAjax(goodsNo, shopNo) {
	if (set == 1) {
		return false;
	}

	set = 1;
	var campPairmatchToCart = "";
	$("input[name=CAMP_PAIRMATCH_TOCART]").each(function(i, elem) {
		  if (0 < campPairmatchToCart.length) {
			  campPairmatchToCart += ",";
		  }
		  campPairmatchToCart += $(elem).val();
	});
	
	var checkLgtmWrnt = "";
	if ($('input[name="CHECK_LGTM_WRNT"]').prop('checked') == true) {
		checkLgtmWrnt = $('input[name="CHECK_LGTM_WRNT"]').val();
	}
	var checkCondYn1 = "";
	var checkCondYn2 = "";
	var checkLiqYn = "";
	
	var checkLgtmWrntChildGoodsNo = "";
	$('input[name="CHECK_LGTM_WRNT_CHILD_GOODS_NO"]').each(function(i, elem) {
		if ($(elem).prop('checked') == true) {
		  if (0 < checkLgtmWrntChildGoodsNo.length) {
			  checkLgtmWrntChildGoodsNo += ",";
		  }
		  checkLgtmWrntChildGoodsNo += $(elem).val();
	    }
	});
	  
	var cartInAjaxUrl = "/bc" + "/cart/CCtAddGoodsToCartAjax.jsp";
	var postData = {}
	
	postData["GOODS_NO"] = goodsNo;
	postData["GOODS_CNT"] = $("[name='GOODS_CNT']").val();
	postData["CHECK_LGTM_WRNT"] = checkLgtmWrnt;
	postData["CHECK_LGTM_WRNT_CHILD_GOODS_NO"] = checkLgtmWrntChildGoodsNo;
	postData["CHECK_COND_YN1"] = checkCondYn1;
	postData["CHECK_COND_YN2"] = checkCondYn2;
	postData["CHECK_LIQ_YN"] = checkLiqYn;
	postData["SUMUP"] = true;
	postData["SHOP_NO"] = shopNo;
	postData["CFM_ID"] = "806ddf623389f7851e714afbd68722a0e425bb0f";
	postData["CAMP_PAIRMATCH_TOCART"] = campPairmatchToCart;
	$.ajax({
	    type: "POST",
	    url: cartInAjaxUrl,
	    data: postData,
	    dataType: "json",
	    async: false
	}).done( function( data ) {
	    $(".cart_announce").html(data.MESSAGE);

        var cartUpdateAjaxUrl = "/bc" + "/cart/CCtUpdateHeaderCartAjax.jsp";
        // ï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½Äæ“¾ï¿½ï¿½ï¿½Aï¿½wï¿½bï¿½_ï¿½ÌƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½HTMLï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ö‚ï¿½ï¿½ï¿½
        $.ajax({
            type: "POST",
            url: cartUpdateAjaxUrl,
            dataType: "html",
            async: true
        }).done( function( html ) {
            $("img[alt='ï¿½Jï¿½[ï¿½g']").parent().parent().html(html);
        });
          
	}).fail( function( data ) {
	    $(".cart_announce").html(data.MESSAGE);
	});
	
	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½{ï¿½^ï¿½ï¿½ï¿½ï¿½Ô‚ï¿½ÏX
	dialogBtnChange();
	
	set = 0;
	return true;
}


//Ajaxï¿½ÊMï¿½ÅƒJï¿½[ï¿½gï¿½Ç‰ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½s(ï¿½ñŠ¼Eï¿½Nï¿½ï¿½Fï¿½Ø‚ï¿½ï¿½ï¿½Ìê‡)
function cartInAjaxCond(goodsNo, shopNo, button) {
	
	if (set == 1) {
		return false;
	} 

	set = 1;
	$("#dialogError").html("");
	$("#dialogError").removeClass("error");
	$.colorbox.resize();
	
	var campPairmatchToCart = "";
	$("input[name=CAMP_PAIRMATCH_TOCART]").each(function(i, elem) {
		  if (0 < campPairmatchToCart.length) {
			  campPairmatchToCart += ",";
		  }
		  campPairmatchToCart += $(elem).val();
	});
	
	var checkLgtmWrnt = "";
	if ($('input[name="CHECK_LGTM_WRNT"]').prop('checked') == true) {
		checkLgtmWrnt = $('input[name="CHECK_LGTM_WRNT"]').val();
	}
	var checkCondYn1 = "";
	if ($('#Clause1').prop('checked') == true) {
		checkCondYn1 = $('#Clause1').val();
	}
	var checkCondYn2 = "";
	if ($('#Clause2').prop('checked') == true) {
		checkCondYn2 = $('#Clause2').val();
	}
	var checkLiqYn = "";
	if ($('#Over20').prop('checked') == true) {
		checkLiqYn = $('#Over20').val();
	}
	
	var checkLgtmWrntChildGoodsNo = "";
	$('input[name="CHECK_LGTM_WRNT_CHILD_GOODS_NO"]').each(function(i, elem) {
		if ($(elem).prop('checked') == true) {
		  if (0 < checkLgtmWrntChildGoodsNo.length) {
			  checkLgtmWrntChildGoodsNo += ",";
		  }
		  checkLgtmWrntChildGoodsNo += $(elem).val();
	    }
	});
	
	var cartInAjaxUrl = "/bc" + "/cart/CCtAddGoodsToCartAjax.jsp";
	var postData = {}
	postData["GOODS_NO"] = goodsNo;
	postData["GOODS_CNT"] = $("[name='GOODS_CNT']").val();
	postData["CHECK_LGTM_WRNT"] = checkLgtmWrnt;
	postData["CHECK_LGTM_WRNT_CHILD_GOODS_NO"] = checkLgtmWrntChildGoodsNo;
	postData["CHECK_COND_YN1"] = checkCondYn1;
	postData["CHECK_COND_YN2"] = checkCondYn2;
	postData["CHECK_LIQ_YN"] = checkLiqYn;
	postData["SUMUP"] = true;
	postData["SHOP_NO"] = shopNo;
	postData["CFM_ID"] = "806ddf623389f7851e714afbd68722a0e425bb0f";
	postData["CAMP_PAIRMATCH_TOCART"] = campPairmatchToCart;
	$.ajax({
	    type: "POST",
	    url: cartInAjaxUrl,
	    data: postData,
	    dataType: "json",
	    async: false
	}).done( function( data ) {
	    var result = data.RESULT;
	    if (result == "false") {
	    	$("#dialogError").html(data.MESSAGE);
	    	$("#dialogError").addClass("error");
	    	$.colorbox.resize();
	    	set = 0;
	    	return false;
	    }

	     var cartUpdateAjaxUrl = "/bc" + "/cart/CCtUpdateHeaderCartAjax.jsp";
	     // ï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½Äæ“¾ï¿½ï¿½ï¿½Aï¿½wï¿½bï¿½_ï¿½ÌƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½HTMLï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ö‚ï¿½ï¿½ï¿½
	     $.ajax({
	         type: "POST",
	         url: cartUpdateAjaxUrl,
	         dataType: "html",
	         async: true
	     }).done( function( html ) {
	         $("img[alt='ï¿½Jï¿½[ï¿½g']").parent().parent().html(html);
	         if (button == "next") {
	             location.href="/bc/cart/CCtViewCart_001.jsp";
	         } else {
	        	 $.colorbox.close();
	         }
	     });
       
	}).fail( function( data ) {
    	$("#dialogError").html(data.MESSAGE);
    	$("#dialogError").addClass("error");
	});
	
	set = 0;
	return true;
}


function winOpen(url) {
       var subWinObj;
       subWinObj = window.open(url,"window_name","width=1300,height=800,scrollbars=yes");
       subWinObj.blur();
       subWinObj.focus();
}

$(function(){
       var wrnt = $('input[name="CHECK_LGTM_WRNT"]');
       if (!wrnt.size()) {
           wrnt = $('input[name="CHECK_LGTM_WRNT_CHILD_GOODS_NO"]');
       }
       if(wrnt.size() == 1 && wrnt.prop('checked')) {
           wrnt.closest("div").addClass("longgurantee_checkon");
           wrnt.next().next().removeClass("cl_point");
       } else if (wrnt.size() >= 2) {
           wrnt.each(function(){
               if($(this).prop('checked')) {
                   $(this).closest("div").addClass("longgurantee_checkon");
                   $(this).next().next().removeClass("cl_point");
               }
           });
       }
})

function wrntCheck(obj) {
       if($(obj).prop('checked')) {
           $(obj).closest("div").addClass("longgurantee_checkon");
           $(obj).next().next().removeClass("cl_point");
       } else {
           $(obj).closest("div").removeClass("longgurantee_checkon");
           $(obj).next().next().addClass("cl_point");
       }
}

// ï¿½Jï¿½[ï¿½gï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½Ìƒ`ï¿½Fï¿½bï¿½Nï¿½{ï¿½bï¿½Nï¿½Xï¿½ÏXï¿½ï¿½ï¿½Ìï¿½ï¿½ï¿½
function dialogBtnChange() {
	var cond1 = true;
	var cond2 = true;
	var over20 = true;
	
	// ï¿½ï¿½1ï¿½Eï¿½ï¿½2ï¿½Eï¿½Nï¿½ï¿½Fï¿½Ø‚ÌŠeï¿½`ï¿½Fï¿½bï¿½Nï¿½{ï¿½bï¿½Nï¿½Xï¿½ï¿½ï¿½ï¿½ï¿½İ‚ï¿½ï¿½Aï¿½ï¿½ï¿½Âƒ`ï¿½Fï¿½bï¿½Nï¿½ï¿½ï¿½ï¿½Ä‚ï¿½ï¿½È‚ï¿½ï¿½ê‡false
	if ($("#Clause1").size() && !($("#Clause1").prop('checked'))) {
		cond1 = false;
	}
	if ($("#Clause2").size() && !($("#Clause2").prop('checked'))) {
		cond2 = false;
	}
	if ($("#Over20").size() && !($("#Over20").prop('checked'))) {
		over20 = false;
	}
	
	// ï¿½Sï¿½ï¿½ï¿½ï¿½trueï¿½Ìê‡ï¿½Ì‚İƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½{ï¿½^ï¿½ï¿½ï¿½ï¿½Lï¿½ï¿½ï¿½ï¿½
	if (cond1 && cond2 && over20) {
		$('.CM_BtnArea li.NextCheckout .Btn_CartActive').css('display', 'block');
		$('.CM_BtnArea li.BackContinue .Btn_CheckON').css('display', 'block');
		$('.CM_BtnArea li.NextCheckout .Btn_Cart').css('display', 'none');
		$('.CM_BtnArea li.BackContinue .Btn_CheckOFF').css('display', 'none');
	} else {
		$('.CM_BtnArea li.NextCheckout .Btn_CartActive').css('display', 'none');
		$('.CM_BtnArea li.BackContinue .Btn_CheckON').css('display', 'none');
		$('.CM_BtnArea li.NextCheckout .Btn_Cart').css('display', 'block');
		$('.CM_BtnArea li.BackContinue .Btn_CheckOFF').css('display', 'block');
	}
}

    $(document).ready(function(){
        // ï¿½Jï¿½[ï¿½gï¿½Ç‰ï¿½ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½İ’ï¿½
        $(".AddCart").colorbox({
            inline: true
        });

        $(".Btn_CartActive").click(function(){
        	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½ÉƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ä‚ï¿½ï¿½È‚ï¿½ï¿½ê‡ï¿½Aï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½s	
        
        	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½ÉƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½Ï‚İ‚Ìê‡ï¿½ÍƒJï¿½[ï¿½gï¿½ï¿½Ê‚Ö‘Jï¿½ï¿½
        	location.href="/bc/cart/CCtViewCart_001.jsp";
        
        });

        $(".Btn_CheckON").click(function(){
        	// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½ÉƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½Ä‚ï¿½ï¿½È‚ï¿½ï¿½ê‡ï¿½Aï¿½Jï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½s	
        
			// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½\ï¿½ï¿½ï¿½ï¿½ï¿½ÉƒJï¿½[ï¿½gï¿½ï¿½ï¿½ï¿½ï¿½Ï‚İ‚Ìê‡ï¿½Íƒ_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½ï¿½Â‚ï¿½ï¿½ï¿½
			$.colorbox.close();
        
        });
        
        // ï¿½Jï¿½[ï¿½gï¿½É“ï¿½ï¿½ï¿½ï¿½{ï¿½^ï¿½ï¿½ï¿½Nï¿½ï¿½ï¿½bï¿½Nï¿½ï¿½ï¿½ÌƒAï¿½Nï¿½Vï¿½ï¿½ï¿½ï¿½ï¿½İ’ï¿½
        $(".BTN_CART_IN").click(function(){
        	cartBtnClick('6195266','0000100094');
        });
        
		// ï¿½_ï¿½Cï¿½Aï¿½ï¿½ï¿½Oï¿½Ìƒ`ï¿½Fï¿½bï¿½Nï¿½{ï¿½bï¿½Nï¿½Xï¿½ÏXï¿½ï¿½ï¿½Ìï¿½ï¿½ï¿½
		$(".cartDialogChk").change(function(){
			dialogBtnChange();
		});
    });
</script>

