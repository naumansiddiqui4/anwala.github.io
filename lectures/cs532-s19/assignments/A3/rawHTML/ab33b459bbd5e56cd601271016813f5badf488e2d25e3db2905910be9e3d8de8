    <script src="//js.maxmind.com/js/apis/geoip2/v2.1/geoip2.js" type="text/javascript"></script>
    <script type='text/javascript'>
      function getCountryCode(cb) {
        if( typeof Cookies.get('GeoCountryCode') !== 'undefined' ) {
          console.log('Returning country code from cookie');
          return cb(Cookies.get('GeoCountryCode'));
        }
        geoip2.country(function(geoipResponse) {
          if (!geoipResponse.country.iso_code) {
            return;
          }
          var code = geoipResponse.country.iso_code.toLowerCase();
          Cookies.set('GeoCountryCode', code, { expires: 7 });
          return cb(code);
        }, function(e) {
          console.error('Error accessing the maxmind API.', e);
          return cb(false);
        });
      }
    </script>
  
  

  
