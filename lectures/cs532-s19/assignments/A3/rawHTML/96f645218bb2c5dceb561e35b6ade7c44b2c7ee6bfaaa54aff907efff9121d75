<script>
//<![CDATA[
var tag = document.createElement('script');

tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

function initYoutubeVideo() {
  var $t = $(this);

  if(!window.YT || !window.YT.Player) {
    $t.addClass('youtube-auto-init');
    return;
  }

  if (!window.devicePixelRatio && window.screen.deviceXDPI && window.screen.logicalXDPI) {
    window.devicePixelRatio = window.devicePixelRatio ||
      window.screen.deviceXDPI / window.screen.logicalXDPI;
  }

  var heightOffset = 0;
  if (window.devicePixelRatio == 1) {
    // Force hide 1px black bar on standard-res devices
    heightOffset = 3;
  }

  var width = $('.popup-blocks-container').width(),
      height = Math.floor((width * 9) / 16) - heightOffset,
      player;

  $t.parent().height(height);

  var playerVars = { controls: 1, disablekb: 0, rel: 0, modestbranding: 1, showinfo: 1, autoplay: 0 };

  var scope = angular.element(this).scope();
  if(!scope) return;

  var videoId, callback, postInit;

  if(scope.entry_method) {
    scope.entry_method.onExpand = function () {
      try {
        if (!navigator.userAgent.match(/iPod|iPhone/) && ((scope.entry_method.config4 + '').match(/autoplay=1/i) || player.getCurrentTime() > 0)) {
          player.playVideo();
        }
      } catch (e) {
      }
    };

    scope.entry_method.onUnexpand = function () {
      try {
        player.pauseVideo();
      } catch (e) {
      }
    };

    var pairs = (scope.entry_method.config4+'').split(/&/);
    var pair;

    for(var i = 0; i < pairs.length; i++) {
      pair = pairs[i].split('=');
      if(pair.length == 2 && pair[0].toLowerCase() != 'autoplay') {
        playerVars[pair[0]] = pair[1];
      }
    }
    videoId = scope.entry_method.config3;
    callback  = function (event) {
      scope.youtubeVideoStateChange(scope.entry_method, event);
      scope.$apply();
    };

    postInit = function () {
      var grace = 5;
      var secondsOpen;
      var interval = setInterval(function () {
        secondsOpen = scope.entry_method.secondsOpen || 0;

        if(player && player.getDuration && player.getDuration() >  1 && (player.getDuration() > grace || secondsOpen > player.getDuration())) {
          if(player.getCurrentTime() > (player.getDuration() - grace) || secondsOpen > player.getDuration()) {
            scope.$apply(function () {
              clearInterval(interval);
              setTimeout(function () {
                scope.entry_method.timePassed = true
              }, (grace-1) * 1000);
            });
          }
        }
      }, 1000);
    }
  } else {
    videoId = scope.incentives[0].youtube_video_id;
    callback = postInit = function() {}
  }

  if(window.YT && window.YT.Player) {
    player = new YT.Player(this.id, {
      height: height,
      width: width,
      videoId: videoId,
      playerVars: playerVars,
      events: {onStateChange: callback}
    });

    postInit();
  }
}
angular.element(document).ready(function () {
  setTimeout(function () {
    $('.youtube-lazy-init').each(function () {
      var elem = this,
          scope = $(this).scope();
      if(scope && scope.entry_method) {
        scope.entry_method.onExpand = function () {
          initYoutubeVideo.call(elem);
          $(elem).addClass('.youtube-auto-init');
        }
      }
    });
  }, 1);
});

function onYouTubeIframeAPIReady() {
  $(function () {
    $('.youtube-auto-init').each(initYoutubeVideo)
  })
}


//]]>
</script><script>
//<![CDATA[

    angular.element(document).ready(
  function () { setTimeout(function () {
    window.twttr = (function (d,s,id) {
    var t, js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return; js=d.createElement(s); js.id=id;
    js.src="https://platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs);
    return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f) } });
    }(document, "script", "twitter-wjs"));

    var twitterReady = function (twttr) {
      function parseTwitterElements() {
        function initTweetBox(t, tweetOpts, refreshOnUnexpand) {
          var $t = $(t);
          if(!$t.attr('twitter-parsed')) {
            $t.attr('twitter-parsed', true);

            var scope = angular.element(t).scope();
            var entry_method = scope.entry_method;
            var tweetId = $t.data('tweet-id');

            twttr.widgets.createTweet(
              tweetId, t, function (iframe) { scope.resize() }, tweetOpts
            );

            if (refreshOnUnexpand) {
              var will_remount = false; // remount tweet to stop playing video on unexpand
              scope.entry_method.onExpand = function () {
                will_remount = true;
              }

              scope.entry_method.onUnexpand = function () {
                if (will_remount) {
                  $t.html('');
                  twttr.widgets.createTweet(
                    tweetId, t, function (iframe) { scope.resize() }, tweetOpts
                  );
                  will_remount = false;
                }
              };
            }

          }
        }

        $('.retweet-box').each(function () {
          initTweetBox(this, {
            conversation: 'none',
            cards: 'none'
          });
        });

        $('.tweet-box').each(function () {
          initTweetBox(this, {
            conversation: 'none'
          }, true);
        });
      }

      $('body').bind('widget:thirdpartyparse', function () {
        setTimeout(parseTwitterElements, 500);
      });

      setTimeout(parseTwitterElements, 500);

      function showExtra(event) {
        var scope = angular.element(event.target).scope();
        scope.showExtra(scope.entry_method);
        scope.$apply();
      }

      twttr.events.bind('retweet', function (event) {
        showExtra(event);
      });
    }

    var twitterReadychecker = setInterval(function () {
      if(window.twttr && window.twttr.ready) {
        clearInterval(twitterReadychecker);
        twttr.ready(twitterReady);
      }
    }, 100);
  }, 1);
});

