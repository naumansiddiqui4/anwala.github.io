    <script>
        var base_url              = "https://www.theshonet.com";

        $(document).ready(function(){
            $('._token').val($('meta[name=csrf-token]').attr("content"));
            if(!getCookie('openlogin').length) {
                document.cookie = "openlogin=1";
            }
        });

                (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "https://connect.facebook.net/id_ID/sdk.js#xfbml=1&version=v2.11&appId=2034068290174004";
                fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
        
        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for(var i = 0; i <ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        $('.burger').click(function(){
            $('.sidebar-black').toggleClass("show");
        });

        $('.main-content').click(function(){
            $('.sidebar-black').removeClass("show");
        });

        $('.wide-wrap.homepage > section').click(function(){
            $('.sidebar-black').removeClass("show");
        });

        $('#shop-menu').hide();

        $('#shop').mouseenter(function(){
            $('#shop-menu').slideToggle();
        });

        $(':not(#shop-menu)').click(function(){
            $('#shop-menu').slideUp();
        });
    </script>

