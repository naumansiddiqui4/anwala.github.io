<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function(){
	window.addEventListener('submit',_clickSendButton, true);
}, false);

function _clickSendButton() {
	try {
		var gender = document.getElementsByName('sk-sex');
		var generation = document.getElementsByName('sk-generation');
		var prefecture = document.getElementsByName('sk-prefecture')[0];
		var job = document.getElementsByName('sk-job')[0];
		var obj = {}
		var elems = document.querySelectorAll('[id^="sk-group-answer"]');
		for(var i=0; i<elems.length; i++) {
			var id = elems[i].id.replace(/^sk-group-/, "");
			var checkedValue = elems[i].querySelectorAll('[name^="sk-"]:checked');
			if(checkedValue.length>0) {
				obj[id] = checkedValue[0].value;
			}
		}
		if(gender && typeof(gender) == 'object'){
			for (var i=0; i<gender.length; i++){
				if(gender[i].checked){
					obj.gender = gender[i].value;
				}
			}
		}
		if(generation && typeof(generation) == 'object'){
			for (var i=0; i<generation.length; i++){
				if(generation[i].checked){
					obj.generation = generation[i].value;
				}
			}
		}
		if(prefecture && typeof(prefecture) == 'object'){
			for (var i=0; i<prefecture.length; i++){
				if(prefecture[i].selected){
					obj.prefecture = prefecture[i].value;
				}
			}
		}
		if(job && typeof(job) == 'object'){
			for (var i=0; i<job.length; i++){
				if(job[i].selected){
					obj.job = job[i].value;
				}
			}
		}
		var count = 0;
		Object.keys(obj).forEach(function(data){if(data.indexOf("answer")=== 0)count++;});
		
		if(count == elems.length && obj.gender && obj.generation && obj.prefecture && obj.job){
			var cx_questionaire = {origin: "frm-questionaire", persistedQueryId: "de430d09832c8764a35f1ff61b315635ccbe81b6"};
			cX.sendEvent("questionaire", obj, cx_questionaire);
		}
	} catch (e) {}
	return true;
};
</script>
