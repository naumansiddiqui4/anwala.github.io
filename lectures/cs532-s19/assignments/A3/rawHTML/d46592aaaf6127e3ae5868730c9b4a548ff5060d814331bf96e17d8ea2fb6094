<script>
  var cxOverlayPC = (function () {
    var $ = jQuery;
    var _titleHeight = 0;
    var _widgetElementId;
    function _init(data) {
      var contentHeight = data.contentHeight ? data.contentHeight : 0;
      var contentWidth = data.contentWidth ? data.contentWidth : 0;
      var animationDuration = 600;
      _widgetElementId = data.elementId;
      // console.log(data);
      if(contentHeight == 0) {
        // console.log("no banner to show");
        $(function() { $("#cxOverlayParent").css("display", "none"); });
        return;
      }

      //if (!cX.getCookie('cx_overlay_shown')) {
      //  cX.setCookie('cx_overlay_shown', 'true', 0.002, '/', location.host);
      //}
      $(function () {
        function toggleOpen() {
          $("#cxOverlayParent").stop(false, false).animate({ height: contentHeight + "px" }, animationDuration);
        }
        function toggleClose() {
          $("#cxOverlayParent").stop(false, false).animate({ height: _titleHeight + "px" }, animationDuration);
        }
        $(".toggle-panel").hover(
          function (e) {
            if (e.target && e.target.className === "toggle-close") {
              return;
            }
            cX.sendEvent('overlay', {action: "hover"},{origin: "frm-web", persistedQueryId: "950c4c836be85461dcd74fce3cc0fa0516b34b4b"});
            toggleOpen();
          },
          function (e) {
            if ((e.relatedTarget && e.relatedTarget.className === "toggle-close")) {
              return;
            }
            toggleClose();
          }
        );
        $(".toggle-panel").mouseout(
          function (e) {
            if ((e.toElement && e.toElement.nodeName.toLowerCase() === "img") &&
              (e.toElement.parentNode.className === "toggle-close")) {
              return;
            }
            if ((e.target && e.target.className === "toggle-close") &&
              (e.relatedTarget && e.relatedTarget.id === _widgetElementId)) {
              toggleOpen();
            }
          }
        );

        $(".toggle-close").click(
          function (e) {
            cX.sendEvent('overlay', {action: "close"},{origin: "frm-web", persistedQueryId: "950c4c836be85461dcd74fce3cc0fa0516b34b4b"});
            $('#cxOverlayParent').css("display", "none");
            cX.setCookie('cx_overlay_closed', 'true', 1, '/', location.host);
          }
        );

        $(".toggle-panel").css("width", contentWidth + "px").css("margin-left", "5px");
        $(".toggle-close").css("width", _titleHeight + "px").css("height", (_titleHeight + 4) + "px");
        setTimeout(toggleClose, 3000 + animationDuration);
        $("#cxOverlayParent").css("bottom", 0);
        toggleOpen();
      });
    }
    return {
      init: function (params, $) {
        params = params || {};
        _titleHeight = params.titleHeight || 40;
        var cx_window = cX.getCookie('cx_overlay_closed');
        var cx_recency = cX.getCookie('cx_overlay_shown');
        if (!cx_window && !cx_recency){
          $(function() { $("#cxOverlayParent").css("display", "block"); });
          cX.callQueue.push(['insertWidget', {
            widgetId: params.widgetId,
            insertBeforeElementId: params.insertBeforeElementId || 'cxOverlayTarget',
            resizeToContentSize: true,
            titleHeight: _titleHeight,
            width: 10, height: 10, renderTemplateUrl: 'auto',
            onImpressionResult: _init
          }]);
        }
      }
    };
  })();
  var cX = cX || {}; cX.callQueue = cX.callQueue || [];
  cX.callQueue.push(['invoke', function() {
    cxOverlayPC.init({widgetId:"4fd0def7d06f5db5694458ce017253e233d0f7a3",insertBeforeElementId:"cxOverlayTarget"}, jQuery);
  }]);
