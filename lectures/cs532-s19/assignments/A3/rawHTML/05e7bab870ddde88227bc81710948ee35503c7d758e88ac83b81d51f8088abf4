<script>
jQuery(document).ready(function(){
    //ユーザー名の最大幅を計算
    var adjustNicknameWidth = function(){
        if(is_login) {
            //お知らせ残件数左offset
            var notify_left     = jQuery("#siteHeaderNotificationContainer").offset().left - jQuery("#siteHeaderInner").offset().left;
            //左側nicoファミリーサイト群の幅
            var nico_navi_width = jQuery(".siteHeaderGlovalNavigation").width();
            //ユーザーサムネとmarginの幅
            var userthumb_width = 34;
            //"さん"文字列の幅
            var san_width       = 30;
            //少し余裕を持たせる
            var side_margin     = 15;
            //nicknameが使える最大幅を計算
            var nick_width_px = Math.floor( notify_left - nico_navi_width - userthumb_width - san_width - side_margin*2 )+"px";
            //css設定
            jQuery("#siteHeaderUserNickNameContainer").css("max-width",nick_width_px);
            return nick_width_px;
        }else{
            return false;
        }
    }
    setTimeout(adjustNicknameWidth,0);//実行タイミングをズラす
    window.adjustNicknameWidth = adjustNicknameWidth;
});
</script>


