if (parent != window) {
    var allowed = false;

    document.addEventListener('click', function(e) {
        if (!allowed) {
            e.stopPropagation();
            e.preventDefault();
        }
    }, true);

    window.addEventListener('message', function(e) {
        if (e.origin == 'https://ch.nicovideo.jp/') {
            allowed = true;
        }
    }, true);
}
</script>

