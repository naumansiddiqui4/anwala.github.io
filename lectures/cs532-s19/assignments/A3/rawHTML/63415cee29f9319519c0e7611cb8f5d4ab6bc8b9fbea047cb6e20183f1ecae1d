		<script>
		
			$(document).ready(function() {
			    $('#searchButton').click(function() {
			      	calcWaitTimes.callHaversine($('#formValueId').val());
  					$( "#results" ).show( "slow" );
			    });
			    
				$('#formValueId').keypress(function(e){
			        if(e.which == 13){ //Enter key
			            calcWaitTimes.callHaversine($('#formValueId').val());
			            $( "#results" ).show( "slow" );
			        }
			        return event.keyCode != 13;
			    });
			});			
        </script>
