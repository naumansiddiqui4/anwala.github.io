          appPackageName : 'com.tastemade.ja.web',
          sessionLength: 1800000,
          globalAttributes: {
            userId:  null 
          }
        });
    
        window.awsMAClient.submitEvents();
    
        // submit queued events before unloading window
        window.addEventListener('beforeunload', function() {
          window.awsMAClient.submitEvents();
        });
    
        (function() {
          sendEvent('pageview');
    
    
          function sendEvent(eventType, opts) {
            var config = [
              'appId=3f7147f0e6e74d01820883c119e2661a',
              'clientId=' + getClientID(),
              'locale=ja',
              'sessionId=' + (getSessionID() || ''),
              'sessionStartTimestamp=' + (getSessionStartTimestamp() || ''),
              'userId=' + getUserID()
            ]
    
            if (!eventType) throw 'missing required parameter';
            if (typeof opts !== 'object') opts = {};
    
            (new Image).src = 'https://foi8fgxh2b.execute-api.us-east-1.amazonaws.com/prod/track-pixel?' + [
              'eventType=' + eventType,
              'timestamp=' + new Date().toISOString(),
              'target=' + (opts.target || encodeURIComponent(window.location.href)),
              'transactionId=' + (opts.transactionId || ''),
              'playedFrom=' + (opts.playedFrom || encodeURIComponent(document.referrer))
            ].concat(config).join('&');
          }
    
          function getClientID() {
            return window.awsMAClient.client
              && window.awsMAClient.client.storage
              && window.awsMAClient.client.storage.cache
              && window.awsMAClient.client.storage.cache.AWSMobileAnalyticsClientId;
          }
    
          function getUserID() {
            return window.awsMAClient.options
              && window.awsMAClient.options.globalAttributes
              && window.awsMAClient.options.globalAttributes.userId;
          }
    
          function getSessionID() {
            return window.awsMAClient.outputs
              && window.awsMAClient.outputs.session
              && window.awsMAClient.outputs.session.id;
          }
    
          function getSessionStartTimestamp() {
            return window.awsMAClient.outputs
              && window.awsMAClient.outputs.session
              && window.awsMAClient.outputs.session.startTimestamp;
          }
        })();
      }
    </script>
