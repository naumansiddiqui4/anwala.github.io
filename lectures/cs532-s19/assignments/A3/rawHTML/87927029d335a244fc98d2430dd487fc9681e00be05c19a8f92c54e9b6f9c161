<script language="JavaScript" type="text/javascript" >
            periodo = 5;  // segundos entre recargas
            imgoImg=document.getElementById("cover");
            imgsrc = imgoImg.src;
            buffer = new Image();
            buffer.onload = imageChange;
            buffer.onerror = imageError;
            setTimeout("imageReload()", periodo * 5000);
           
            function imageReload() {
                buffer.src = imgsrc + "?d=" + new Date().getTime();
                setTimeout("imageReload()", periodo * 5000);
            }
           
            function imageChange() {
                imgoImg.src = buffer.src;
            }
         
            function imageError() {
                setTimeout("imageReload()", 5000);
            }
</script>
